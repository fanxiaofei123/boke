                        </TextEditor>
                        <Label Key="Lab_Zhusuo" Caption="住所" X="0" Y="5"/>
                        <ComboBox Key="Xueli" Caption="学历" BuddyKey="Lab_Xueli" X="1" Y="7" XSpan="3">
                            <DataBinding TableKey="qiuzhixinxiHead" ColumnKey="Xiala" DefaultValue="本科"/>
                            <Item Caption="专科" Key="1" Value="-1"/>
                            <Item Caption="本科" Key="2" Value="0"/>
                            <Item Caption="研究生" Key="3" Value="1"/>
                            <Item Caption="博士" Key="4" Value="2"/>
