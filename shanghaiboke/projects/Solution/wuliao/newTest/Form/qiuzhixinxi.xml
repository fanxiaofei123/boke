                        </CheckBox>
                        <CheckBox Caption="C++" Key="C" X="8" Y="4">
                            <DataBinding TableKey="qiuzhixinxiHead" ColumnKey="C"/>
                        </CheckBox>
                        <CheckBox Caption="SQL" Key="Sql" X="8" Y="5">
