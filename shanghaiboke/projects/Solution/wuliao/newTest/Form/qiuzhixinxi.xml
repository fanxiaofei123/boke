                        <TextArea Key="Jiaoyu" Caption="教育背景" BuddyKey="Lab_Jiaoyu" X="4" Y="3" XSpan="4" YSpan="3">
                            <DataBinding TableKey="qiuzhixinxiHead" ColumnKey="Jiaoyu" DefaultValue="某某大学"/>
