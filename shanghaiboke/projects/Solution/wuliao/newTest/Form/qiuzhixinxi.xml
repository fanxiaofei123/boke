                        <TextArea Key="Beizhu" Caption="备注" BuddyKey="Lab_Beizhu" X="1" Y="11" XSpan="7" YSpan="2">
                            <DataBinding TableKey="qiuzhixinxiHead" ColumnKey="Beizhu" DefaultValue="非常好~"/>
