                        <RadioButton Caption="女" Key="Wmales" X="2" Y="3" GroupKey="Gender" Value="2"/>
                        <Dict Key="Province" Caption="省份" BuddyKey="Lab_Province" X="1" Y="4" ItemKey="Province">
