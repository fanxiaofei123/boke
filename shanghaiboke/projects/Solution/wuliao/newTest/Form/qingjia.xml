<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="qingjia" Caption="请假" FormType="Entity" ViewKey="qingjiaView">
    <DataSource>
        <DataObject Key="qingjia" Caption="qingjia" PrimaryTableKey="qingjiaHead" PrimaryType="Entity">
            <TableCollection>
                <Table Key="qingjiaHead" Caption="头表1">
                    <Column Key="OID" Caption="对象标识" DataType="Long"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer"/>
                    <Column Key="Status" Caption="单据状态" DataType="Integer"/>
                    <Column Key="BillDate" Caption="单据日期" DataType="DateTime"/>
                    <Column Key="Creator" Caption="制单人" DataType="Long" Description="保存人员" ItemKey="Operator"/>
                    <Column Key="CreateTime" Caption="制单日期" DataType="DateTime"/>
                    <Column Key="Modifier" Caption="修改人" DataType="Long"/>
                    <Column Key="ModifyTime" Caption="修改时间" DataType="DateTime"/>
                    <Column Key="InstanceID" Caption="流程实例" DataType="Long"/>
                    <Column Key="EmpName" Caption="EmpName" DataType="Varchar"/>
                    <Column Key="StatusType" Caption="StatusType" DataType="Varchar"/>
                    <Column Key="StartDate" Caption="StartDate" DataType="DateTime"/>
                    <Column Key="EndDate" Caption="EndDate" DataType="DateTime"/>
                    <Column Key="Days" Caption="Days" DataType="Varchar"/>
                    <Column Key="Shuzhi1" Caption="Shuzhi1" DataType="Numeric" Precision="16" Scale="2"/>
                    <Column Key="Shuzhi2" Caption="Shuzhi2" DataType="Numeric" Precision="16" Scale="2"/>
                    <Column Key="Shuzhi3" Caption="Shuzhi3" DataType="Numeric" Precision="16" Scale="2"/>
                    <Column Key="Lianshidict" Caption="Lianshidict" DataType="Long"/>
                </Table>
                <Table Key="qingjiaDetail" Caption="明细表" DBTableName="qingjiaDetail" TableMode="Detail">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="Sequence" Caption="序号" DataType="Integer" DBColumnName="SEQUENCE"/>
                </Table>
                <Table Key="test" Caption="test" TableMode="Detail">
                    <Column Key="OID" Caption="对象标识" DataType="Long"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer"/>
                    <Column Key="Material" DataType="Long" IsPrimary="true"/>
                    <Column Key="Amount" DataType="Numeric" Precision="16" Scale="2"/>
                    <Column Key="UserType" DataType="Long"/>
                    <Column Key="SumExpand" DataType="Numeric" Precision="16" Scale="2" IsPrimary="true"/>
                    <Column Key="City" DataType="Varchar"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="WORKITEM" Caption="WORKITEM" Tag="WORKITEM"/>
        <Operation Key="BPM" Caption="BPM" Tag="BPM"/>
        <Operation Key="Save" Caption="保存" Visible="!ReadOnly()">
            <Action>
                <![CDATA[SaveData(); UpdateView();]]>
            </Action>
        </Operation>
        <Operation Key="New" Caption="新增" Visible="ReadOnly()">
            <Action>
                <![CDATA[New('qingjia', 'self');]]>
            </Action>
        </Operation>
        <Operation Key="Delete" Caption="删除" Visible="ReadOnly()">
            <Action>
                <![CDATA[DeleteData();UpdateView();Close();]]>
            </Action>
        </Operation>
        <Operation Key="Edit" Caption="编辑" Visible="ReadOnly()">
            <Action>
                <![CDATA[Edit();]]>
            </Action>
        </Operation>
        <Operation Key="Cancel" Caption="取消" Visible="!ReadOnly()">
            <Action>
                <![CDATA[Cancel();]]>
            </Action>
        </Operation>
        <Operation Key="Close" Caption="关闭" Visible="ReadOnly()">
            <Action>
                <![CDATA[Close()]]>
            </Action>
        </Operation>
    </OperationCollection>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板">
                    <GridLayoutPanel Key="GridLayoutPanel1" Caption="GridLayoutPanel1">
                        <TextEditor Key="EmpName" Caption="姓名" BuddyKey="Lab_EmpName" X="3" Y="0">
                            <DataBinding ColumnKey="EmpName" TableKey="qingjiaHead"/>
                        </TextEditor>
                        <Label Key="Lab_EmpName" Caption="姓名" X="2" Y="0"/>
                        <TextEditor Key="StatusType" Caption="状态" BuddyKey="Lab_StatusType" X="5" Y="0">
                            <DataBinding ColumnKey="StatusType" TableKey="qingjiaHead"/>
                        </TextEditor>
                        <Label Key="Lab_StatusType" Caption="状态" X="4" Y="0"/>
                        <DatePicker Key="StartDate" Caption="开始时间" BuddyKey="Lab_StartDate" X="1" Y="1">
                            <DataBinding ColumnKey="StartDate" TableKey="qingjiaHead"/>
                        </DatePicker>
                        <Label Key="Lab_StartDate" Caption="开始时间" X="0" Y="1"/>
                        <DatePicker Key="EndDate" Caption="结束时间" BuddyKey="Lab_EndDate" X="3" Y="1">
                            <DataBinding ColumnKey="EndDate" TableKey="qingjiaHead"/>
                        </DatePicker>
                        <Label Key="Lab_EndDate" Caption="结束时间" X="2" Y="1"/>
                        <TextEditor Key="Days" Caption="请假天数" BuddyKey="Lab_Days" X="5" Y="1">
                            <DataBinding ColumnKey="Days" TableKey="qingjiaHead"/>
                        </TextEditor>
                        <Label Key="Lab_Days" Caption="请假天数" X="4" Y="1"/>
                        <NumberEditor Key="Shuzhi1" Caption="数值1" BuddyKey="Lab_Shuzhi1" X="1" Y="2">
                            <DataBinding ColumnKey="Shuzhi1" TableKey="qingjiaHead"/>
                        </NumberEditor>
                        <Label Key="Lab_Shuzhi1" Caption="数值1" X="0" Y="2"/>
                        <NumberEditor Key="Shuzhi2" Caption="数值2" BuddyKey="Lab_Shuzhi2" X="3" Y="2">
                            <DataBinding ColumnKey="Shuzhi2" TableKey="qingjiaHead"/>
                        </NumberEditor>
                        <Label Key="Lab_Shuzhi2" Caption="数值2" X="2" Y="2"/>
                        <NumberEditor Key="Shuzhi3" Caption="数值3" BuddyKey="Lab_Shuzhi3" X="5" Y="2" Enable="Shuzhi1&gt;Shuzhi2">
                            <DataBinding ColumnKey="Shuzhi3" TableKey="qingjiaHead"/>
                        </NumberEditor>
                        <Label Key="Lab_Shuzhi3" Caption="数值3" X="4" Y="2"/>
                        <RowDefCollection RowGap="5">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="5">
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="106px"/>
                            <ColumnDef Width="67px"/>
                            <ColumnDef Width="105px"/>
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="80px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Key="GridLayoutPanel2" Caption="GridLayoutPanel2">
                        <RadioButton Caption="单选2" Key="Danxuan2" X="2" Y="0" GroupKey="Radio" Value="2"/>
                        <RadioButton Caption="单选1" Key="Danxuan1" X="0" Y="0" GroupKey="Radio" IsGroupHead="true" Value="1"/>
                        <Dict Key="Lianshidict" Caption="链式字典" BuddyKey="Lab_Lianshidict" X="4" Y="0" ItemKey="lianshidict">
                            <DataBinding ColumnKey="Lianshidict" TableKey="qingjiaHead"/>
                        </Dict>
                        <Label Key="Lab_Lianshidict" Caption="链式字典" X="3" Y="0"/>
                        <RowDefCollection RowGap="5">
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="5">
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="42px"/>
                            <ColumnDef Width="69px"/>
                            <ColumnDef Width="39px"/>
                            <ColumnDef Width="78px"/>
                            <ColumnDef Width="80px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <Grid Key="Grid1" Caption="Grid1" PageRowCount="5">
                        <GridColumnCollection>
                            <GridColumn Key="column1" Caption="选择" Width="80px"/>
                            <GridColumn Key="Material" Caption="物料" Width="80px" Sortable="true"/>
                            <GridColumn Key="Amount" Caption="数量" Width="80px" ColumnType="Detail">
                                <ColumnExpand ExpandType="Title">
                                    <![CDATA[]]>
                                </ColumnExpand>
                                <GridColumnCollection>
                                    <GridColumn Key="UserType" Caption="用途" Width="80px">
                                        <ColumnExpand ColumnKey="UserType">
                                            <![CDATA["1,自用;2,转租;3,储备;"]]>
                                        </ColumnExpand>
                                    </GridColumn>
                                </GridColumnCollection>
                            </GridColumn>
                            <GridColumn Key="column4" Caption="汇总" Width="80px"/>
                            <GridColumn Key="column5" Caption="城市" Width="80px"/>
                        </GridColumnCollection>
                        <GridRowCollection>
                            <GridRow DetailKey="" Key="row1" TableKey="test">
                                <GridCell Key="select" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                <GridCell Key="Material" Caption="物料" CellType="Dict" CellGroupType="RowGroup" ItemKey="Province">
                                    <DataBinding ColumnKey="Material"/>
                                </GridCell>
                                <GridCell Key="Amount" Caption="数量" CellType="NumberEditor">
                                    <DataBinding ColumnKey="Amount"/>
                                </GridCell>
                                <GridCell Key="cell11" Caption="汇总">
                                    <DataBinding DefaultFormulaValue="SumExpand('Amount')" ColumnKey="SumExpand" ValueDependency="Amount"/>
                                </GridCell>
                                <GridCell Key="cell14" Caption="城市" CellType="Dict" ItemKey="lianshidict">
                                    <DataBinding ColumnKey="City"/>
                                </GridCell>
                            </GridRow>
                            <GridRow DetailKey="" GroupKey="Material" Key="row2" RowType="Group">
                                <GridCell Key="cell5"/>
                                <GridCell Key="cell6" Caption="小计">
                                    <DataBinding/>
                                </GridCell>
                                <GridCell Key="cell7">
                                    <DataBinding DefaultFormulaValue="Sum('Amount')"/>
                                </GridCell>
                                <GridCell Key="cell12"/>
                                <GridCell Key="cell15"/>
                            </GridRow>
                            <GridRow DetailKey="" Key="row3" RowType="Total">
                                <GridCell Key="cell8"/>
                                <GridCell Key="cell9" Caption="总计"/>
                                <GridCell Key="cell10">
                                    <DataBinding DefaultFormulaValue="Sum('Amount')"/>
                                </GridCell>
                                <GridCell Key="cell13"/>
                                <GridCell Key="cell16"/>
                            </GridRow>
                        </GridRowCollection>
                    </Grid>
                    <SplitSize Size="100px"/>
                    <SplitSize Size="100px"/>
                    <SplitSize Size="100px"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
