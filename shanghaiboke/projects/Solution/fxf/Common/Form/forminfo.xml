<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="forminfo" Caption="员工表单">
    <DataSource>
        <DataObject Key="forminfo" Caption="员工表单" PrimaryTableKey="forminfo">
            <TableCollection>
                <Table Key="forminfo" Caption="员工表单">
                    <Column Key="OID" Caption="对象标识" DataType="Long"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer"/>
                    <Column Key="RichEditor1" Caption="RichEditor1" DataType="Text"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main" Caption="根面板">
                <ToolBar Key="main_toolbar" Height="pref" Caption="main_toolbar">
                    <ToolBarItemCollection/>
                </ToolBar>
                <GridLayoutPanel Key="first_head" Padding="5px" Height="pref" Caption="基本信息">
                    <RadioButton Caption="男" Key="RadioButton1" X="0" Y="6"/>
                    <RadioButton Caption="女" Key="RadioButton2" X="1" Y="6"/>
                    <RadioButton Caption="未知" Key="RadioButton3" X="2" Y="6"/>
                    <RowDefCollection RowGap="5">
                        <RowDef/>
                        <RowDef/>
                        <RowDef/>
                        <RowDef/>
                        <RowDef Height="30px"/>
                        <RowDef Height="30px"/>
                        <RowDef Height="30px"/>
                        <RowDef Height="30px"/>
                    </RowDefCollection>
                    <ColumnDefCollection ColumnGap="5">
                        <ColumnDef Width="34%"/>
                        <ColumnDef/>
                        <ColumnDef/>
                    </ColumnDefCollection>
                </GridLayoutPanel>
                <RichEditor Caption="RichEditor1" Height="pref" Key="RichEditor1">
                    <DataBinding TableKey="forminfo" ColumnKey="RichEditor1"/>
                </RichEditor>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
