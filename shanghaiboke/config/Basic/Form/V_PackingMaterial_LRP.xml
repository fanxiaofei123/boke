<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="V_PackingMaterial_LRP" Caption="包装材料" FormType="Dict">
    <DataSource RefObjectKey="LRP_PackingMaterial"/>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main">
                <TabPanel Caption="TabPanel0" Key="TabPanel0" Height="pref">
                    <GridLayoutPanel Key="BasicTab" Padding="10px" Caption="基本信息">
                        <TextEditor Key="Code" Caption="代码" BuddyKey="L_Code" X="1" Y="0" Enable="!ReadOnly()" Tip="「代码」不可为空" XSpan="3">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="Code" Required="true"/>
                        </TextEditor>
                        <TextEditor Key="Name" Caption="名称" BuddyKey="L_Name" X="6" Y="0" Enable="!ReadOnly()" Tip="「名称」不可为空" XSpan="3">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="Name" Required="true"/>
                        </TextEditor>
                        <Label Key="L_Code" Caption="代码" X="0" Y="0"/>
                        <Label Key="L_Name" Caption="名称" X="5" Y="0"/>
                        <TextEditor Key="Spec" Caption="规格" BuddyKey="Lab_Spec" X="1" Y="1" XSpan="3">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="Spec" Required="true"/>
                        </TextEditor>
                        <Label Key="Lab_Spec" Caption="规格" X="0" Y="1"/>
                        <TextEditor Key="Model" Caption="型号" BuddyKey="Lab_Model" X="6" Y="1" XSpan="3">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="Model" Required="true"/>
                        </TextEditor>
                        <Label Key="Lab_Model" Caption="型号" X="5" Y="1"/>
                        <TextEditor Key="Unit" Caption="单位" BuddyKey="Lab_Unit" X="1" Y="2" XSpan="3">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="Unit" Required="true"/>
                        </TextEditor>
                        <Label Key="Lab_Unit" Caption="单位" X="0" Y="2"/>
                        <NumberEditor Key="Weight" Caption="重量" BuddyKey="Lab_Weight" X="6" Y="2" XSpan="3" Precision="18" Scale="3">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="Weight"/>
                        </NumberEditor>
                        <Label Key="Lab_Weight" Caption="重量" X="5" Y="2"/>
                        <NumberEditor Key="PreCool" Caption="预冷时间" BuddyKey="Lab_PreCool" X="1" Y="3" XSpan="2" ShowZero="true">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="PreCool"/>
                        </NumberEditor>
                        <Label Key="Lab_PreCool" Caption="预冷时间" X="0" Y="3"/>
                        <Label Key="Time" Caption="小时" X="3" Y="3">
                            <Format HAlign="Right"/>
                        </Label>
                        <NumberEditor Key="Discharging" Caption="释冷时间" BuddyKey="Lab_Discharging" X="6" Y="3" XSpan="2" ShowZero="true">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="Discharging"/>
                        </NumberEditor>
                        <Label Key="Lab_Discharging" Caption="释冷时间" X="5" Y="3"/>
                        <Label Key="Time2" Caption="分钟" X="8" Y="3">
                            <Format HAlign="Right"/>
                        </Label>
                        <NumberEditor Key="DichargingOverTime" Caption="释冷超时" BuddyKey="Lab_DichargingOverTime" X="1" Y="4" XSpan="2" ShowZero="true">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="DischargingOverTime"/>
                        </NumberEditor>
                        <Label Key="Lab_DichargingOverTime" Caption="释冷超时" X="0" Y="4"/>
                        <Label Key="Time3" Caption="分钟" X="3" Y="4">
						    <Format HAlign="Right"/>
                        </Label>
						<NumberEditor Key="PackingOverTime" Caption="包装超时" BuddyKey="Lab_PackingOverTime" X="6" Y="4" XSpan="2" ShowZero="true">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="PackingOverTime"/>
                        </NumberEditor>
                        <Label Key="Lab_PackingOverTime" Caption="包装超时" X="5" Y="4"/>
                        <Label Key="Time4" Caption="分钟" X="8" Y="4">
                            <Format HAlign="Right"/>
                        </Label>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="77px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="20px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="10px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Key="SystemTab" Padding="10px" Caption="系统信息">
                        <Dict Key="CREATOR" Caption="创建人" X="1" Y="0" ItemKey="Operator">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="CREATOR" DefaultFormulaValue="GetOperator()"/>
                        </Dict>
                        <DatePicker Key="CREATETIME" Caption="创建时间" X="1" Y="1">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="CREATETIME" DefaultFormulaValue="ServerDate()"/>
                        </DatePicker>
                        <Dict Key="MODIFIER" Caption="修改人" X="1" Y="2" ItemKey="Operator">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="MODIFIER" DefaultFormulaValue="GetOperator()"/>
                        </Dict>
                        <DatePicker Key="MODIFYTIME" Caption="修改时间" X="1" Y="3">
                            <DataBinding TableKey="LRP_PackingMaterial" ColumnKey="MODIFYTIME" DefaultFormulaValue="ServerDate()"/>
                        </DatePicker>
                        <Label Key="L_CREATOR" Caption="创建人" X="0" Y="0"/>
                        <Label Key="L_CREATETIME" Caption="创建时间" X="0" Y="1"/>
                        <Label Key="L_MODIFIER" Caption="修改人" X="0" Y="2"/>
                        <Label Key="L_MODIFYTIME" Caption="修改时间" X="0" Y="3"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="100%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
