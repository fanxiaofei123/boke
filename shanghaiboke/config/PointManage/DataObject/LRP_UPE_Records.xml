<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<DataObject Key="LRP_UPE_Records" Caption="用户积分兑换记录" NoPrefix="UPER" PrimaryTableKey="LRP_UPE_Records_H" PrimaryType="Entity">
