            <Column Key="DVERID" Caption="对象明细版本" DataType="Integer"/>
            <Column Key="Status" Caption="状态" DataType="Integer"/>
            <Column Key="No" Caption="兑换记录号" DataType="Varchar" Length="80"/>
            <Column Key="OwnerUserId" Caption="货主用户id" DataType="Long"/>
            <Column Key="UPEItemId" Caption="兑换项目id" DataType="Long"/>
            <Column Key="ExchangeTime" Caption="兑换日期时间" DataType="DateTime"/>
            <Column Key="Qty" Caption="兑换数量" DataType="Numeric" Precision="18" Scale="0"/>
