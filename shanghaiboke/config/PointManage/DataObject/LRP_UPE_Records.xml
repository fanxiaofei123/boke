            <Column Caption="修改时间" DataType="DateTime" Key="MODIFYTIME"/>
            <Column Caption="序号" Key="SEQUENCE" DataType="Integer"/>
            <Column Key="UPARId" Caption="用户积分增加记录id" DataType="Long"/>
            <Column Key="Points" Caption="核销积分" DataType="Numeric" Precision="18" Scale="0"/>
