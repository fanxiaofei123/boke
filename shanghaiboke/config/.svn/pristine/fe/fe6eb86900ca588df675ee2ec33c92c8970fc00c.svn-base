<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="V_SatisfactionSurv" Caption="满意度调查" FormType="Entity" ViewKey="V_SatisfactionSurvView">
    <DataSource RefObjectKey="LRP_SatisfactionSurv"/>
    <OperationCollection>
        <Operation Key="WORKITEM" Caption="WORKITEM" Tag="WORKITEM"/>
        <Operation Key="BPM" Caption="BPM" Tag="BPM"/>
        <Operation Key="Save" Caption="保存" Visible="!ReadOnly()">
            <Action>
                <![CDATA[SaveData(); UpdateView();]]>
            </Action>
        </Operation>
        <Operation Key="New" Caption="新增" Visible="ReadOnly()">
            <Action>
                <![CDATA[New('V_SatisfactionSurv', 'self');]]>
            </Action>
        </Operation>
        <Operation Key="Delete" Caption="删除" Visible="ReadOnly()">
            <Action>
                <![CDATA[DeleteData();UpdateView();Close();]]>
            </Action>
        </Operation>
        <Operation Key="Edit" Caption="编辑" Visible="ReadOnly()">
            <Action>
                <![CDATA[Edit();]]>
            </Action>
        </Operation>
        <Operation Key="Close" Caption="关闭" Visible="ReadOnly()">
            <Action>
                <![CDATA[Close()]]>
            </Action>
        </Operation>
        <Operation Key="Cancel" Caption="取消" Visible="!ReadOnly()">
            <Action>
                <![CDATA[Cancel();]]>
            </Action>
        </Operation>
    </OperationCollection>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板">
                    <TabPanel Caption="TabPanel1" Key="TabPanel1">
                        <GridLayoutPanel Key="first_head" Height="pref" Padding="5px" Caption="基本信息">
                            <TextEditor Key="TextEditor1" Caption="调查单号" BuddyKey="Lab_TextEditor1" X="1" Y="0" Enable="false" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="NO" TableKey="LRP_SatisfactionSurv"/>
                            </TextEditor>
                            <Label Key="Lab_TextEditor1" Caption="调查单号" X="0" Y="0"/>
                            <Dict Key="Dict1" Caption="货主" BuddyKey="Lab_Dict1" X="4" Y="0" ItemKey="LRP_Owner" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="OwnerId" TableKey="LRP_SatisfactionSurv" Required="true">
                                    <ValueChanged>
                                        <![CDATA[SetValue('Dict2', 0);]]>
                                    </ValueChanged>
                                </DataBinding>
                            </Dict>
                            <Label Key="Lab_Dict1" Caption="货主" X="3" Y="0"/>
                            <TextEditor Key="TextEditor2" Caption="调查类型" BuddyKey="Lab_TextEditor2" X="7" Y="0" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="SurveyType" TableKey="LRP_SatisfactionSurv"/>
                            </TextEditor>
                            <Label Key="Lab_TextEditor2" Caption="调查类型" X="6" Y="0"/>
                            <Dict Key="Dict2" Caption="货主用户" BuddyKey="Lab_Dict2" X="1" Y="1" ItemKey="LRP_OwnerUser" XSpan="2">
                                <DataBinding ColumnKey="OwnerUserId" TableKey="LRP_SatisfactionSurv">
                                    <ValueChanged>
                                        <![CDATA[SetValue("TextEditor3", GetDictValue("LRP_OwnerUser",GetValue("Dict2"),"LRP_OwnerUser.Name"))]]>
                                    </ValueChanged>
                                </DataBinding>
                                <ItemFilter ItemKey="LRP_OwnerUser">
                                    <Filter Type="FieldValue">
                                        <FilterValue FieldKey="OwnerId" RefValue="Dict1" Type="Field" DataType="Long"/>
                                    </Filter>
                                </ItemFilter>
                            </Dict>
                            <Label Key="Lab_Dict2" Caption="货主用户" X="0" Y="1"/>
                            <TextEditor Key="TextEditor3" Caption="用户名称" BuddyKey="Lab_TextEditor3" X="4" Y="1" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="OwnerUserName" TableKey="LRP_SatisfactionSurv"/>
                            </TextEditor>
                            <Label Key="Lab_TextEditor3" Caption="用户名称" X="3" Y="1"/>
                            <TextEditor Key="TextEditor4" Caption="手机号" BuddyKey="Lab_TextEditor4" X="7" Y="1" XSpan="2" MaxLength="20">
                                <DataBinding ColumnKey="Mobile" TableKey="LRP_SatisfactionSurv"/>
                            </TextEditor>
                            <Label Key="Lab_TextEditor4" Caption="手机号" X="6" Y="1"/>
                            <TextEditor Key="TextEditor5" Caption="关联单号" BuddyKey="Lab_TextEditor5" X="1" Y="2" AsQuery="true" XSpan="2" MaxLength="20">
                                <DataBinding ColumnKey="RefNo" TableKey="LRP_SatisfactionSurv"/>
                            </TextEditor>
                            <Label Key="Lab_TextEditor5" Caption="关联单号" X="0" Y="2"/>
                            <DatePicker Key="DatePicker1" Caption="调查日期时间" BuddyKey="Lab_DatePicker1" X="4" Y="2" AsQuery="true" XSpan="2">
                                <DataBinding DefaultFormulaValue="ServerDate();" ColumnKey="SurveyDatetime" TableKey="LRP_SatisfactionSurv" Required="true"/>
                                <Format/>
                            </DatePicker>
                            <Label Key="Lab_DatePicker1" Caption="调查日期时间" X="3" Y="2"/>
                            <ComboBox Key="ComboBox1" Caption="满意度等级" BuddyKey="Lab_ComboBox1" X="7" Y="2" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="SatisfactionLevel" TableKey="LRP_SatisfactionSurv" DefaultValue="100" Required="true"/>
                                <Item Caption="很不满意" Key="10" Value="10"/>
                                <Item Caption="不满意" Key="40" Value="40"/>
                                <Item Caption="一般" Key="60" Value="60"/>
                                <Item Caption="满意" Key="80" Value="80"/>
                                <Item Caption="很满意" Key="100" Value="100"/>
                            </ComboBox>
                            <Label Key="Lab_ComboBox1" Caption="满意度等级" X="6" Y="2"/>
                            <TextEditor Key="TextEditor6" Caption="备注" BuddyKey="Lab_TextEditor6" X="1" Y="3" XSpan="8">
                                <DataBinding ColumnKey="Remark" TableKey="LRP_SatisfactionSurv"/>
                            </TextEditor>
                            <Label Key="Lab_TextEditor6" Caption="备注" X="0" Y="3"/>
                            <RowDefCollection RowGap="8" RowHeight="30">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="8">
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="55px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="GridLayoutPanel1" Caption="系统信息">
                            <Dict Key="Dict3" Caption="创建人" BuddyKey="Lab_Dict3" X="1" Y="0" ItemKey="Operator">
                                <DataBinding DefaultFormulaValue="GetOperator();" ColumnKey="CREATOR" TableKey="LRP_SatisfactionSurv"/>
                            </Dict>
                            <Label Key="Lab_Dict3" Caption="创建人" X="0" Y="0"/>
                            <Dict Key="Dict4" BuddyKey="Lab_Dict4" X="1" Y="1" ItemKey="Operator">
                                <DataBinding DefaultFormulaValue="GetOperator();" ColumnKey="MODIFIER" TableKey="LRP_SatisfactionSurv"/>
                            </Dict>
                            <Label Key="Lab_Dict4" Caption="修改人" X="0" Y="1"/>
                            <DatePicker Key="DatePicker2" Caption="创建时间" BuddyKey="Lab_DatePicker2" X="4" Y="0" TabOrder="0">
                                <DataBinding DefaultFormulaValue="ServerDate();" ColumnKey="CREATETIME" TableKey="LRP_SatisfactionSurv"/>
                            </DatePicker>
                            <Label Key="Lab_DatePicker2" Caption="创建时间" X="3" Y="0"/>
                            <DatePicker Key="DatePicker3" Caption="修改时间" BuddyKey="Lab_DatePicker3" X="4" Y="1">
                                <DataBinding DefaultFormulaValue="ServerDate();" ColumnKey="MODIFYTIME" TableKey="LRP_SatisfactionSurv"/>
                            </DatePicker>
                            <Label Key="Lab_DatePicker3" Caption="修改时间" X="3" Y="1"/>
                            <RowDefCollection RowGap="8">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="8">
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="20px"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <SplitSize Size="100%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
