<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Warn_Time_Set" Caption="预警时间设置" FormType="Dict">
    <DataSource RefObjectKey="CHCS_Warn_Time_Set"/>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Description="载入事件" Range="Action" Verb="Load">
            <![CDATA[LoadData()]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <SplitPanel Key="SplitPanel1" Orientation="Vertical" Height="100%" Caption="SplitPanel1">
                <TabPanel Caption="TabPanel1" Key="TabPanel1">
                    <GridLayoutPanel Key="BasicInfo" Padding="10px" Caption="基础信息">
                        <TextEditor Key="Name" Caption="名称" BuddyKey="L_Name" X="4" Y="0">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Name" Required="true"/>
                        </TextEditor>
                        <Label Key="L_Name" Caption="名称" X="3" Y="0"/>
                        <TextEditor Key="Code" Caption="代码" BuddyKey="Lab_Code" X="1" Y="0">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Code" Required="true"/>
                        </TextEditor>
                        <Label Key="Lab_Code" Caption="代码" X="0" Y="0"/>
                        <TextEditor Key="Abbreviation" Caption="简称" BuddyKey="Lab_Abbreviation" X="1" Y="1">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Abbreviation"/>
                        </TextEditor>
                        <Label Key="Lab_Abbreviation" Caption="简称" X="0" Y="1"/>
                        <ComboBox Key="Status" Caption="状态" BuddyKey="Lab_Status" X="1" Y="2" SourceType="Status">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Status"/>
                        </ComboBox>
                        <Label Key="Lab_Status" Caption="状态" X="0" Y="2"/>
                        <TextEditor Key="Spec" Caption="规格" BuddyKey="Lab_Spec" X="4" Y="2">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Spec"/>
                        </TextEditor>
                        <Label Key="Lab_Spec" Caption="规格" X="3" Y="2"/>
                        <TextEditor Key="Storage_Condition" Caption="存储条件" BuddyKey="Lab_Storage_Condition" X="1" Y="3">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Storage_Condition"/>
                        </TextEditor>
                        <Label Key="Lab_Storage_Condition" Caption="存储条件" X="0" Y="3"/>
                        <TextEditor Key="Sort" Caption="分类" BuddyKey="Lab_Sort" X="4" Y="3">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Sort"/>
                        </TextEditor>
                        <Label Key="Lab_Sort" Caption="分类" X="3" Y="3"/>
                        <TextEditor Key="Type" Caption="类型" BuddyKey="Lab_Type" X="1" Y="4">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Type"/>
                        </TextEditor>
                        <Label Key="Lab_Type" Caption="类型" X="0" Y="4"/>
                        <TextEditor Key="Bar_Code" Caption="条码" BuddyKey="Lab_Bar_Code" X="4" Y="4">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Bar_Code"/>
                        </TextEditor>
                        <Label Key="Lab_Bar_Code" Caption="条码" X="3" Y="4"/>
                        <TextEditor Key="Remark" Caption="备注" BuddyKey="Lab_Remark" X="1" Y="5">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="Remark"/>
                        </TextEditor>
                        <Label Key="Lab_Remark" Caption="备注" X="0" Y="5"/>
                        <RowDefCollection RowGap="6" RowHeight="30">
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="20">
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="30px"/>
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Key="SysInfo" Padding="10px" Caption="系统信息">
                        <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="CREATOR"/>
                        </Dict>
                        <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                        <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="CREATETIME"/>
                        </DatePicker>
                        <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                        <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="MODIFIER"/>
                        </Dict>
                        <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                        <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                            <DataBinding TableKey="CHCS_Warn_Time_Set" ColumnKey="MODIFYTIME"/>
                        </DatePicker>
                        <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                        <RowDefCollection RowGap="5">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="5">
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
                <SplitSize Size="100%"/>
            </SplitPanel>
        </Block>
    </Body>
</Form>
