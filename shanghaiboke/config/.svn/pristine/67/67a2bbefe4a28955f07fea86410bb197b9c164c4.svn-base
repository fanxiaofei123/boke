<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="V_LRP_PlatformReservationQuery" Caption="月台预约状况查询">
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板" Height="100%" Width="100%">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <WebBrowser Caption="WebBrowser1" Height="100%" Key="WebBrowser1" Width="100%"/>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
    <OnPostShow>
        <![CDATA[//设置默认仓储中心
var param = "warehouseId=" & getCurOrgID();
var pdate = Format(ServerDate(),"yyyy-MM-dd");
param = param & "&date=" & pdate;
//默认中文
var url = "LRP_PlatformReservation/query.html?" & param;
if(GetSessionPara("Language")=="en"){
    url = "LRP_PlatformReservation/query_en.html?" & param;
}
SetValue('WebBrowser1', url);]]>
    </OnPostShow>
</Form>
