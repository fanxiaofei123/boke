                                    </GridCell>
                                    <GridCell Caption="目标仓间" CellType="Dict" Enable="!ReadOnly()" ItemKey="LRP_Storeroom" Key="DestStoreroomId">
                                        <DataBinding ColumnKey="DestStoreroomId" DefaultFormulaValue="GetDictValue('LRP_Storearea', DestStoreareaId, 'StoreroomId');"/>
                                    </GridCell>
                                    <GridCell Caption="目标库区" CellType="Dict" Enable="!ReadOnly()" ItemKey="LRP_Storearea" Key="DestStoreareaId">
                                        <DataBinding ColumnKey="DestStoreareaId" Required="true"/>
                                        <ItemFilter ItemKey="LRP_Storearea">
                                            <Filter Type="FieldValue">
                                                <FilterValue FieldKey="IsSampleTestArea" RefValue="1" Type="Const" DataType="Integer"/>
                                                <FilterValue FieldKey="IsRecvArea" RefValue="0" Type="Const" DataType="Integer"/>
                                                <FilterValue FieldKey="IsShipArea" RefValue="0" Type="Const" DataType="Integer"/>
                                                <FilterValue FieldKey="IsKittingArea" RefValue="0" Type="Const" DataType="Integer"/>
                                                <FilterValue FieldKey="WarehouseCenterId" RefValue="WarehouseCenterId" Type="Field" DataType="Long"/>
                                            </Filter>
                                        </ItemFilter>
                                    </GridCell>
                                    <GridCell Caption="目标储位" CellType="Dict" Enable="!ReadOnly()&amp;&amp;DestStoreareaId&gt;0" ItemKey="LRP_Location" Key="DestLocationId">
                                        <DataBinding ColumnKey="DestLocationId" Required="true"/>
                                        <ItemFilter ItemKey="LRP_Location">
                                            <Filter FilterDependency="DestStoreareaId" Type="FieldValue">
                                                <FilterValue FieldKey="StoreareaId" RefValue="DestStoreareaId" Type="Field" DataType="Long"/>
                                            </Filter>
                                        </ItemFilter>
                                    </GridCell>
