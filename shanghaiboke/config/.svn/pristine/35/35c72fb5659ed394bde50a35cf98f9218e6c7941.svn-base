<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Count_HView" Caption="盘点单视图" FormType="View" AbbrCaption="盘点单视图">
    <DataSource>
        <DataObject Key="CHCS_Count_H" Caption="盘点单视图" PrimaryTableKey="CHCS_Count_H">
            <TableCollection>
                <Table Key="CHCS_Count_H" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="NO" Caption="单据编号" DataType="Varchar" SortType="Desc"/>
                    <Column Key="BillDate" Caption="单据日期" DataType="DateTime"/>
                    <Column Key="Status" Caption="单据状态" DataType="Integer"/>
                    <Column Key="Vendor" Caption="航食公司" DataType="Long"/>
                    <Column Key="Creator" Caption="制单人" DataType="Long"/>
                    <Column Key="CreateTime" Caption="制单日期" DataType="DateTime"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="New" Caption="新增" Icon="new.png">
            <Action>
                <![CDATA[New('CHCS_Count_H')]]>
            </Action>
        </Operation>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel" Height="100%">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="Lab_NO_Q" Caption="盘点单单号" X="5" Y="0"/>
                            <Label Key="Lab_BillDate_Comp1" Caption="盘点日期 从" X="0" Y="0"/>
                            <TextEditor Key="NO_Q" Caption="盘点单单号" BuddyKey="Lab_NO_Q" X="6" Y="0">
                                <Condition ColumnKey="NO" TableKey="CHCS_Count_H" CondSign="like"/>
                            </TextEditor>
                            <DatePicker Key="BillDate_Comp1" Caption="盘点日期 从" BuddyKey="Lab_BillDate_Comp1" X="1" Y="0" OnlyDate="true">
                                <Format HAlign="Center"/>
                                <Condition ColumnKey="BillDate" TableKey="CHCS_Count_H" CondSign="between" Group="BillDate" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="L_BillDate_Comp2" Caption="到" X="2" Y="0"/>
                            <DatePicker Key="BillDate_Comp2" Caption="到" BuddyKey="L_BillDate_Comp2" X="3" Y="0" OnlyDate="true">
                                <Format HAlign="Center"/>
                                <Condition ColumnKey="BillDate" TableKey="CHCS_Count_H" CondSign="between" Group="BillDate" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="Lab_Status_Q" Caption="单据状态" X="0" Y="1"/>
                            <ComboBox Key="Status_Q" Caption="单据状态" BuddyKey="Lab_Status_Q" X="1" Y="1" IntegerValue="true" XSpan="3">
                                <Condition ColumnKey="Status" TableKey="CHCS_Count_H" CondSign="="/>
                                <Item Caption="已输入" Key="prepared" Value="100"/>
                                <Item Caption="已确认" Key="confirmed" Value="200"/>
                                <Item Caption="已提交" Key="committed" Value="110"/>
                            </ComboBox>
                            <Button Key="Query" Caption="查询" X="11" Y="0" Enable="true" XSpan="2">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="11" Y="1" Enable="true" XSpan="2">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <Dict Key="Vendor_Q" Caption="航食公司" BuddyKey="Lab_Vendor_Q" X="9" Y="0" ItemKey="Vendor">
                                <Condition ColumnKey="Vendor" TableKey="CHCS_Count_H" CondSign="="/>
                            </Dict>
                            <Label Key="Lab_Vendor_Q" Caption="航食公司" X="8" Y="0"/>
                            <RowDefCollection RowHeight="25" RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="90px"/>
                                <ColumnDef Width="16%"/>
                                <ColumnDef Width="15px"/>
                                <ColumnDef Width="16%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="50px"/>
                                <ColumnDef Width="4%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="list" PageLoadType="DB" TableKey="CHCS_Count_H">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="CHCS_Count_H" Visible="false" Width="30px"/>
                            <ListViewColumn Key="NO" Caption="单据编号" ColumnType="TextEditor" DataColumnKey="NO" Width="106px"/>
                            <ListViewColumn Key="BillDate" Caption="盘点日期" ColumnType="DatePicker" DataColumnKey="BillDate" Width="125px" Format="yyyy-MM-dd" OnlyDate="true"/>
                            <ListViewColumn Key="Status_LV" Caption="单据状态" ColumnType="ComboBox" DataColumnKey="Status" Width="98px" SourceType="Status"/>
                            <ListViewColumn Key="Vendor" Caption="航食公司" ColumnType="Dict" DataColumnKey="Vendor" Width="137px" ItemKey="Vendor"/>
                            <ListViewColumn Key="Creator" Caption="操作人" ColumnType="Dict" DataColumnKey="Creator" Width="159px" ItemKey="Operator"/>
                            <ListViewColumn Key="CreateTime" Caption="操作日期" ColumnType="DatePicker" DataColumnKey="CreateTime" Width="103px" OnlyDate="true"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('CHCS_Count_H', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="75px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
