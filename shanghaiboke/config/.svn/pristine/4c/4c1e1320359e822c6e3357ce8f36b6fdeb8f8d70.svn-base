<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Caption="越库收货单" FormType="Entity" Key="V_Tx104" ViewKey="V_Tx104View">
    <DataSource RefObjectKey="LRP_WMTx"/>
    <OperationCollection>
        <Operation Caption="保存" Key="Save" Visible="!ReadOnly()">
            <Action>
                <![CDATA[               
				var flag=false;			    
				if(GetRowCount('Grid0', false)<=0){    
     				Confirm('LocaleString('OwnerNull', 'key103')', 'OK','OK:{}');
					return true;
				}                     
				var rs=DBNamedQuery("SelectMaSta", TxTypeCode); 
                var index = 0; 				
				loop 'Grid0' (true){ 
                index = index + 1;	
				 var mtlStatusId = GetCellValue('Grid0', -1, 'DestMaterialStatusId');
				rs.beforefirst(); 
                 //遍历本单据设置的物料状态	
                 while(rs.next()){
						  //一条明细，如果物料状态等于此单据本单据设置的物料状态中的一个则跳出循环，比较下一条明细的物料状态
					if( mtlStatusId >0 &&mtlStatusId == rs.MaterialStatusId){
					flag=true;
					break;
					}
                 	flag=false;				
				}
				 //如果没有这一条明细中的物料状态不等于任何一个本单据设置的物料状态，则弹出警告
						  if(flag==false){
								Confirm(LocaleParaFormat("CommonDef","key52",index) &GetDictValue('LRP_MaterialStatus', mtlStatusId, 'Name'), 'OK','OK:{}');
								break;
						  }
						}	  
				if(flag==true){
				     var isShow = ToBool(GetSessionPara('RecvDate_IsShow'));
					if(isShow){
				    var recvDate = GetValue("TxDateTime");
					loop 'Grid0' (true){
				    SetCellValue('Grid0', -1, 'DestRecvDate',recvDate);
					}
					}
					 SaveData(); UpdateView();
				}
             ]]>
            </Action>
        </Operation>
        <Operation Caption="行复制新增" Enable="!ReadOnly()" Key="CopyGridRow">
            <Action>
                <![CDATA[            
             CopyGridRow("Grid0",-1,"Qty","0");
             ]]>
            </Action>
        </Operation>
        <Operation Caption="编辑" Key="Edit" RefKey="Edit" ShortCuts="Ctrl+E"/>
        <Operation Caption="删除" Key="Delete" Visible="ReadOnly()&amp;&amp;Status==StatusValue('prepared')">
            <Action>
                <![CDATA[
			Confirm(LocaleString('OwnerNull', 'key104'),'YES_NO',{YES:{
				 DeleteData();
			     UpdateView();Close();},NO:{}});
			]]>
            </Action>
        </Operation>
        <Operation Caption="取消" Key="Cancel" RefKey="Cancel"/>
        <Operation Caption="完成" Key="Finish" Visible="ReadOnly()&amp;&amp;(Status==StatusValue('prepared')||Status==StatusValue('handled'))">
            <Action>
                <![CDATA[				
				 Confirm(LocaleString('OwnerNull', 'key127'),'YES_NO',{YES:{
					Status=StatusValue('finished');
					FinishDateTime = ServerDate();
				  loop"Grid0"(true)
				{			
					WMTx_Status = 90;
				};
				     SaveData();UpdateView();},NO:{}});
				]]>
            </Action>
            <ExceptionHandler>
                <![CDATA[	
				loop"Grid0"(true)
				{			
					WMTx_Status = 10;
				}			
				Status=StatusValue('confirmed');
			]]>
            </ExceptionHandler>
        </Operation>
        <Operation Caption="撤销完成" Key="Finish-R" Visible="ReadOnly()&amp;&amp;Status==StatusValue('finished')">
            <Action>
                <![CDATA[				
				Confirm(LocaleString('OwnerNull', 'key128'),'YES_NO',{YES:{
				Status=StatusValue('prepared');
				 loop"Grid0"(true)
				{			
					WMTx_Status = 10;
				};
				SaveData();UpdateView();},NO:{}});
				]]>
            </Action>
            <ExceptionHandler>
                <![CDATA[	
				loop"Grid0"(true)
				{			
					WMTx_Status = 90;
				}			
				Status=StatusValue('finished');
			]]>
            </ExceptionHandler>
        </Operation>
        <Operation Caption="发货" Enable="Status==900" Key="Ship">
            <Action>
                <![CDATA[
				MidMap('Tx104-Tx214','V_Tx214');
			]]>
            </Action>
        </Operation>
        <Operation Caption="关闭界面" Key="Close" RefKey="Close"/>
    </OperationCollection>
    <QueryCollection>
        <Query Description="查询所选物料的name" Key="SelectMaterialName">
            <Statement>
                <![CDATA[select m.Name from LRP_Material m join LRP_WMTx_L w on m.OID=?]]>
            </Statement>
            <ParameterCollection>
                <Parameter DataType="Long"/>
            </ParameterCollection>
        </Query>
        <Query Description="查询事务类型中的物料状态" Key="SelectMaSta">
            <Statement>
                <![CDATA[select l.MaterialStatusId from LRP_WhTxType_MS l join LRP_WarehouseTxType h on h.oid=l.soid where h.Code=?]]>
            </Statement>
            <ParameterCollection>
                <Parameter DataType="Long"/>
            </ParameterCollection>
        </Query>
    </QueryCollection>
