<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="V_OwnerPayRecordsView" Caption="货主支付记录叙事簿" FormType="View" AbbrCaption="货主支付记录叙事簿" Platform="">
    <DataSource>
        <DataObject Key="V_OwnerPayRecordsView" Caption="货主支付记录叙事簿" PrimaryTableKey="LRP_OwnerPayRecords">
            <TableCollection>
                <Table Key="LRP_OwnerPayRecords" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Caption="单号" DataType="Varchar" Key="No" Length="100"/>
                    <Column Caption="货主用户" DataType="Long" Key="OwnerUserId"/>
                    <Column Caption="货主" DataType="Long" Key="OwnerId"/>
                    <Column Caption="支付类型" DataType="Varchar" Key="PayType" Length="20"/>
                    <Column Caption="支付结果" DataType="Integer" Key="Result"/>
                    <Column Caption="支付日期时间" DataType="DateTime" Key="PayDatetime"/>
                    <Column Caption="金额" DataType="Numeric" Precision="10" Scale="4" Key="Amount"/>
                    <Column Caption="交易单号" DataType="Varchar" Key="PayNo" Length="40"/>
                    <Column Caption="业务单号" DataType="Varchar" Key="BillNo" Length="40"/>
                    <Column Caption="错误代码" DataType="Varchar" Key="ErrorCode" Length="10"/>
                    <Column Caption="错误信息" DataType="Varchar" Key="ErrorMessage" Length="100"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Caption="关闭界面" Key="Close" RefKey="Close"/>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="Lab_PayDatetime" Caption="支付日期时间" X="0" Y="1"/>
                            <DatePicker Key="PayDatetime" Caption="支付日期时间" BuddyKey="L_PayDatetime" X="1" Y="1" XSpan="2">
                                <Format/>
                                <Condition ColumnKey="DistributeDatetime" TableKey="LRP_OwnerPayRecords" CondSign="between" Group="DistributeDatetime" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="Lab_PayDatetime_Comp2" Caption="到" X="4" Y="1"/>
                            <DatePicker Key="PayDatetime_Comp2" Caption="到" BuddyKey="L_PayDatetime_Comp2" X="5" Y="1" XSpan="2">
                                <Format/>
                                <Condition ColumnKey="DistributeDatetime" TableKey="LRP_OwnerPayRecords" CondSign="between" Group="DistributeDatetime" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="Lab_BillNo" Caption="业务单号" X="4" Y="2"/>
                            <TextEditor Key="BillNo" Caption="业务单号" BuddyKey="L_BillNo" X="5" Y="2" XSpan="2">
                                <Condition ColumnKey="CouponName" TableKey="LRP_OwnerPayRecords" CondSign="like"/>
                            </TextEditor>
                            <Label Key="Lab_OwnerId" Caption="货主" X="0" Y="0"/>
                            <Dict Key="OwnerId" Caption="货主" BuddyKey="L_OwnerId" X="1" Y="0" AllowMultiSelection="true" ItemKey="LRP_Owner" XSpan="2">
                                <Condition ColumnKey="OwnerUserId" TableKey="LRP_OwnerPayRecords" CondSign="in"/>
                            </Dict>
                            <Button Key="Query" Caption="查询" X="8" Y="0" Enable="true">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="8" Y="1" Enable="true">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <TextEditor Key="DataObjectKey" Caption="DataObjectKey" X="8" Y="2" Visible="false" Enable="false">
                                <DataBinding DefaultValue="LRP_OwnerPayRecords"/>
                            </TextEditor>
                            <ComboBox Key="PayType" Caption="支付类型" BuddyKey="Lab_PayType" X="5" Y="0" XSpan="2">
                                <DataBinding ColumnKey="PayType" TableKey="LRP_OwnerPayRecords"/>
                                <Item Caption="微信" Key="wechat" Value="wechat"/>
                                <Item Caption="支付宝" Key="alipay" Value="alipay"/>
                                <Item Caption="余额" Key="amount" Value="amount"/>
                            </ComboBox>
                            <Label Key="Lab_PayType" Caption="支付类型" X="4" Y="0"/>
                            <ComboBox Key="Result" Caption="支付结果" BuddyKey="L_Result" X="1" Y="2" XSpan="2">
                                <DataBinding ColumnKey="Result" TableKey="LRP_OwnerPayRecords"/>
                                <Item Caption="失败" Key="0" Value="0"/>
                                <Item Caption="成功" Key="1" Value="1"/>
                            </ComboBox>
                            <Label Key="Lab_Result" Caption="支付结果" X="0" Y="2"/>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="8">
                                <ColumnDef Width="77px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="80px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="ListView0" PageLoadType="DB" TableKey="LRP_OwnerPayRecords">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="LRP_OwnerPayRecords" Visible="false" Width="30px"/>
                            <ListViewColumn Key="OwnerId_LV" Caption="货主" ColumnType="Dict" Width="80px" DataColumnKey="OwnerId" ItemKey="LRP_Owner"/>
                            <ListViewColumn Key="OwnerUserId_LV" Caption="货主用户" ColumnType="Dict" DataColumnKey="OwnerUserId" ItemKey="LRP_OwnerUser"/>
                            <ListViewColumn Key="PayType_LV" Caption="支付类型" ColumnType="ComboBox" DataColumnKey="PayType">
                                <Item Caption="微信" Key="wechat" Value="wechat"/>
                                <Item Caption="支付宝" Key="alipay" Value="alipay"/>
                                <Item Caption="余额" Key="amount" Value="amount"/>
                            </ListViewColumn>
                            <ListViewColumn Key="Result_LV" Caption="支付结果" ColumnType="ComboBox" DataColumnKey="Result">
                                <Item Caption="成功" Key="1" Value="1"/>
                                <Item Caption="失败" Key="0" Value="0"/>
                            </ListViewColumn>
                            <ListViewColumn Key="PayDatetime_LV" Caption="支付时间" ColumnType="DatePicker" DataColumnKey="PayDatetime"/>
                            <ListViewColumn Key="Amount_LV" Caption="金额" ColumnType="NumberEditor" DataColumnKey="Amount"/>
                            <ListViewColumn Key="PayNo_LV" Caption="交易单号" ColumnType="TextEditor" DataColumnKey="PayNo"/>
                            <ListViewColumn Key="BillNo_LV" Caption="业务单号" ColumnType="TextEditor" DataColumnKey="BillNo"/>
                            <ListViewColumn Key="ErrorCode_LV" Caption="错误代码" ColumnType="TextEditor" DataColumnKey="ErrorCode"/>
                            <ListViewColumn Key="ErrorMessage_LV" Caption="错误信息" ColumnType="TextEditor" DataColumnKey="ErrorMessage"/>
                        </ListViewColumnCollection>
                    </ListView>
                    <SplitSize Size="120px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
