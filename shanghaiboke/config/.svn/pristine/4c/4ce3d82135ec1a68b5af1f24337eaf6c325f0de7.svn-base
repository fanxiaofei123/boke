<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="Budget_Monitoring" Caption="费用预算监控" FormType="View">
    <DataSource RefObjectKey="Annual_Budget"/>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main" Caption="根面板">
                <ToolBar Key="main_toolbar" Height="pref" Caption="main_toolbar">
                    <ToolBarItemCollection/>
                </ToolBar>
                <GridLayoutPanel Key="first_head" Padding="5px" Height="pref" Caption="基本信息">
                    <TextEditor Key="NO" Caption="单据编号" BuddyKey="Lab_NO" X="1" Y="0">
                        <DataBinding TableKey="Annual_Budget" ColumnKey="NO"/>
                    </TextEditor>
                    <Label Key="Lab_NO" Caption="单据编号" X="0" Y="0"/>
                    <ComboBox Key="BudgetaryClassiy" Caption="预算分类" BuddyKey="Lab_BudgetaryClassiy" X="4" Y="0">
                        <DataBinding TableKey="Annual_Budget" ColumnKey="Budgetary_Classiy"/>
                    </ComboBox>
                    <Label Key="Lab_BudgetaryClassiy" Caption="预算分类" X="3" Y="0"/>
                    <ComboBox Key="Status" Caption="预算状态" BuddyKey="Lab_Status" X="7" Y="0">
                        <DataBinding TableKey="Annual_Budget" ColumnKey="Status"/>
                    </ComboBox>
                    <Label Key="Lab_Status" Caption="预算状态" X="6" Y="0"/>
                    <DatePicker Key="TimeFrameStart" Caption="业务日期 从" BuddyKey="Lab_TimeFrameStart" X="1" Y="1">
                        <DataBinding TableKey="Annual_Budget" ColumnKey="Time_Frame_Start"/>
                    </DatePicker>
                    <Label Key="Lab_TimeFrameStart" Caption="业务日期 从" X="0" Y="1"/>
                    <DatePicker Key="TimeFrameStop" Caption="业务日期 至" BuddyKey="Lab_TimeFrameStop" X="4" Y="1">
                        <DataBinding TableKey="Annual_Budget" ColumnKey="Time_Frame_Stop"/>
                    </DatePicker>
                    <Label Key="Lab_TimeFrameStop" Caption="业务日期 至" X="3" Y="1"/>
                    <NumberEditor Key="Staffing" Caption="编制人员" BuddyKey="Lab_Staffing" X="7" Y="1">
                        <DataBinding TableKey="Annual_Budget" ColumnKey="Staffing"/>
                    </NumberEditor>
                    <Label Key="Lab_Staffing" Caption="编制人员" X="6" Y="1"/>
                    <Dict Key="BudgetDepartment" Caption="预算部门" BuddyKey="Lab_BudgetDepartment" X="1" Y="2" ItemKey="CHCS_Department">
                        <DataBinding TableKey="Annual_Budget" ColumnKey="Budget_Department"/>
                    </Dict>
                    <Label Key="Lab_BudgetDepartment" Caption="预算部门" X="0" Y="2"/>
                    <Button Key="Query" Caption="查询" X="6" Y="3"/>
                    <Button Key="cancel" Caption="重置" X="7" Y="3"/>
                    <RowDefCollection RowHeight="30" RowGap="6">
                        <RowDef/>
                        <RowDef/>
                        <RowDef/>
                        <RowDef/>
                    </RowDefCollection>
                    <ColumnDefCollection ColumnGap="20">
                        <ColumnDef Width="60px"/>
                        <ColumnDef Width="33%"/>
                        <ColumnDef Width="30px"/>
                        <ColumnDef Width="60px"/>
                        <ColumnDef Width="33%"/>
                        <ColumnDef Width="30px"/>
                        <ColumnDef Width="60px"/>
                        <ColumnDef Width="34%"/>
                    </ColumnDefCollection>
                </GridLayoutPanel>
                <ListView Caption="ListView1" Height="pref" Key="ListView1" PageLoadType="DB" TableKey="Annual_Budget_L">
                    <ListViewColumnCollection>
                        <ListViewColumn Key="NO_b" Caption="单据编号" ColumnType="TextEditor"/>
                        <ListViewColumn Key="column2" Caption="预算日期" ColumnType="DatePicker"/>
                        <ListViewColumn Key="column3" Caption="预算名称" ColumnType="TextEditor" Sortable="true"/>
                        <ListViewColumn Key="column4" Caption="预算部门" ColumnType="TextEditor"/>
                        <ListViewColumn Key="column5" Caption="预算项目" ColumnType="TextEditor" DataColumnKey="Budget_Items"/>
                        <ListViewColumn Key="column6" Caption="航线分类" ColumnType="TextEditor" DataColumnKey="Route_Type_Code"/>
                        <ListViewColumn Key="column7" Caption="航线" ColumnType="TextEditor" DataColumnKey="Route"/>
                        <ListViewColumn Key="column8" Caption="费用预算（含税）" ColumnType="NumberEditor" DataColumnKey="Cost_Budget_Taxes"/>
                        <ListViewColumn Key="column9" Caption="费用预算（无税）" ColumnType="NumberEditor" DataColumnKey="Cost_Budget" Width="100px"/>
                        <ListViewColumn Key="column10" Caption="预提费用（含税）" ColumnType="NumberEditor"/>
                        <ListViewColumn Key="column15" Caption="预提费用（无税）" ColumnType="NumberEditor"/>
                        <ListViewColumn Key="column14" Caption="结算费用（含税）" ColumnType="NumberEditor"/>
                        <ListViewColumn Key="column13" Caption="结算费用（不含税）" ColumnType="NumberEditor"/>
                        <ListViewColumn Key="column12" Caption="剩余预算（含税）" ColumnType="NumberEditor"/>
                        <ListViewColumn Key="column11" Caption="剩余预算（无税）" ColumnType="NumberEditor"/>
                        <ListViewColumn Key="column18" Caption="人均费用预算（含税）" ColumnType="NumberEditor" DataColumnKey="Per_Capita_Cost_Budget_Taxes"/>
                        <ListViewColumn Key="column17" Caption="人均费用预提（含税）" ColumnType="NumberEditor"/>
                        <ListViewColumn Key="column16" Caption="人均费用结算（含税）" ColumnType="NumberEditor"/>
                    </ListViewColumnCollection>
                </ListView>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
