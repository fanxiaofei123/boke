<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<DataObject Caption="应付引入发票" Key="LRP_QuoteInvoicePay" PrimaryTableKey="LRP_QuoteInvoicePay">
    <TableCollection>
        <Table Caption="应付引入发票" Key="LRP_QuoteInvoicePay" SourceType="Query" TableMode="Detail">
            <Column Key="OID" Caption="发票OID" DataType="Long"/>
            <Column Key="SOID" Caption="发票SOID" DataType="Long"/>
            <Column Key="No" Caption="单号" DataType="Varchar" Length="200"/>
            <Column Key="CompanyCode" Caption="公司" DataType="Long"/>
            <Column Key="SettlementId" Caption="结算方" DataType="Long"/>
            <Column Caption="实体票号" DataType="Varchar" Key="OriginNo" Length="100"/>
            <Column Caption="发票类型" DataType="Integer" Key="InvoiceType"/>
            <Column Caption="开票日期" DataType="DateTime" Key="BillDate"/>
            <Column Caption="费用名称" DataType="Long" Key="ExpenseId"/>
            <Column Caption="合计金额" DataType="Numeric" Key="SumMoney" Precision="18" Scale="6"/>
            <Column Key="CurrencyID" Caption="交易货币" DataType="Long"/>
            <Column Key="HookMoney" Caption="核销金额" DataType="Numeric" Precision="18" Scale="6"/>
            <Column Key="UnSettleMoney" Caption="未核金额" DataType="Numeric" Precision="18" Scale="6"/>
            <Column Key="MapKey" Caption="数据映射key" DataType="Varchar" Length="100"/>
            <Column Key="MapCount" Caption="映射数量标记" DataType="Integer"/>
            <Statement>
                <![CDATA[SELECT *
FROM
  (SELECT 
  OID,
  SOID,
  No,
  CompanyCode,
  SettlementId,
  BillDate,
  OriginNo,
  InvoiceType,
  ExpenseId,
  CurrencyID,
  SumMoney,
  HookMoney,
  UnSettleMoney
FROM LRP_InvoicePayable h
WHERE STATUS = 200 and (HookStatus = 0 or HookStatus = 1)  )a ]]>
            </Statement>
        </Table>
    </TableCollection>
</DataObject>
