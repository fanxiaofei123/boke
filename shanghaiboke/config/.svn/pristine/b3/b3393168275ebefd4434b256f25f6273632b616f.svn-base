<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="V_LTLQuoteView" Caption="零担和干线报价序事簿" FormType="View" AbbrCaption="零担和干线报价序事簿">
    <DataSource>
        <DataObject Key="V_LTLQuoteView" Caption="零担和干线报价序事簿" PrimaryTableKey="LRP_LTLQuote_H">
            <TableCollection>
                <Table Key="LRP_LTLQuote_H" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="No" Caption="单号" DataType="Varchar" Length="100"/>
                    <Column Key="Remark" Caption="备注" DataType="Varchar" Length="200"/>
                    <Column Key="Status" Caption="状态" DataType="Integer"/>
                    <Column Key="DispatchOrgId" Caption="调度组织" DataType="Long"/>
                    <Column Key="DeliveryFeeBasis" Caption="送货费计算依据" DataType="Integer"/>
                    <Column Key="StartProvinceId" Caption="起运省" DataType="Long"/>
                    <Column Key="StartCityId" Caption="起运城市" DataType="Long"/>
                    <Column Key="TransferFeeBasis" Caption="转运费计算依据" DataType="Integer"/>
                    <Column Key="EndProvinceId" Caption="目的省" DataType="Long"/>
                    <Column Key="EndCityId" Caption="目的城市" DataType="Long"/>
                    <Column Key="EndDistrictId" Caption="目的区县" DataType="Varchar" Length="1000"/>
                    <Column Key="EndDistrictName" Caption="目的区县" DataType="Varchar" Length="1000"/>
                    <Column Key="EffectiveDate" Caption="生效日期" DataType="DateTime"/>
                    <Column Key="ExpireDate" Caption="截止日期" DataType="DateTime"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Caption="新增" Icon="new.png" Key="New">
            <Action>
                <![CDATA[
				New('V_LTLQuote')	
			]]>
            </Action>
        </Operation>
        <Operation Key="BatchConfirm" Caption="确认" RefKey="BatchConfirm"/>
        <Operation Key="BatchConfirm-R" Caption="撤销确认" RefKey="BatchConfirm-R"/>
        <Operation Key="BatchCancel" Caption="作废" RefKey="BatchCancel"/>
        <Operation Key="BatchCancel-R" Caption="撤销作废" RefKey="BatchCancel-R"/>
        <Operation Caption="关闭界面" Key="Close" RefKey="Close"/>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="10px" Caption="查询字段">
                            <TextEditor Caption="DataObjectKey" Enable="false" Key="DataObjectKey" Visible="false" X="0" Y="0">
                                <DataBinding DefaultValue="LRP_LTLQuote"/>
                            </TextEditor>
                            <Label Key="L_No" Caption="报价单号" X="0" Y="0"/>
                            <TextEditor Key="No" Caption="报价单号" BuddyKey="L_No" X="1" Y="0" XSpan="2">
                                <Condition ColumnKey="No" TableKey="LRP_LTLQuote_H" CondSign="like"/>
                            </TextEditor>
                            <Label Key="Lab_Status_C" Caption="状态" X="0" Y="3"/>
                            <ComboBox Key="Status_C" Caption="状态" BuddyKey="Lab_Status_C" X="1" Y="3" SourceType="Status" XSpan="2">
                                <Condition ColumnKey="Status" TableKey="LRP_LTLQuote_H" CondSign="="/>
                            </ComboBox>
                            <Label Key="L_DispatchOrgId" Caption="调度组织" X="4" Y="0"/>
                            <Dict Key="DispatchOrgId" Caption="调度组织" BuddyKey="L_DispatchOrgId" X="5" Y="0" AllowMultiSelection="true" ItemKey="LRP_DispatchOrg" XSpan="2">
                                <Condition ColumnKey="DispatchOrgId" TableKey="LRP_LTLQuote_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_StartProvinceId" Caption="起运省" X="0" Y="1"/>
                            <Dict Key="StartProvinceId" Caption="起运省" BuddyKey="L_StartProvinceId" X="1" Y="1" AllowMultiSelection="true" ItemKey="LRP_Province" XSpan="2">
                                <Condition ColumnKey="StartProvinceId" TableKey="LRP_LTLQuote_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_StartCityId" Caption="起运城市" X="4" Y="1"/>
                            <Dict Key="StartCityId" Caption="起运城市" BuddyKey="L_StartCityId" X="5" Y="1" AllowMultiSelection="true" ItemKey="LRP_City" XSpan="2">
                                <Condition ColumnKey="StartCityId" TableKey="LRP_LTLQuote_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_EndProvinceId" Caption="目的省" X="0" Y="2"/>
                            <Dict Key="EndProvinceId" Caption="目的省" BuddyKey="L_EndProvinceId" X="1" Y="2" AllowMultiSelection="true" ItemKey="LRP_Province" XSpan="2">
                                <Condition ColumnKey="EndProvinceId" TableKey="LRP_LTLQuote_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_EndCityId" Caption="目的城市" X="4" Y="2"/>
                            <Dict Key="EndCityId" Caption="目的城市" BuddyKey="L_EndCityId" X="5" Y="2" AllowMultiSelection="true" ItemKey="LRP_City" XSpan="2">
                                <Condition ColumnKey="EndCityId" TableKey="LRP_LTLQuote_H" CondSign="in"/>
                            </Dict>
                            <Button Key="Query" Caption="查询" X="8" Y="0" Enable="true">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="8" Y="1" Enable="true">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="8">
                                <ColumnDef Width="77px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="80px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="ListView0" PageLoadType="DB" TableKey="LRP_LTLQuote_H">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="LRP_LTLQuote_H" Visible="false" Width="74px"/>
                            <ListViewColumn Key="Select" Caption="选择" ColumnType="CheckBox" Enable="true" IsSelect="true"/>
                            <ListViewColumn Key="Status" Caption="状态" DataColumnKey="Status" ColumnType="ComboBox" SourceType="Status"/>
                            <ListViewColumn Caption="报价单号" ColumnType="HyperLink" DataColumnKey="No" Key="No_LV">
                                <OnClick>
                                    <![CDATA[
									Open('V_LTLQuote', OID);
									]]>
                                </OnClick>
                            </ListViewColumn>
                            <ListViewColumn Key="DispatchOrgId_LV" Caption="调度组织" ColumnType="Dict" DataColumnKey="DispatchOrgId" ItemKey="LRP_DispatchOrg"/>
                            <ListViewColumn Key="StartProvinceId_LV" Caption="起运省" ColumnType="Dict" DataColumnKey="StartProvinceId" ItemKey="LRP_Province"/>
                            <ListViewColumn Key="StartCityId_LV" Caption="起运城市" ColumnType="Dict" DataColumnKey="StartCityId" ItemKey="LRP_City"/>
                            <ListViewColumn Key="EndProvinceId_LV" Caption="目的省" ColumnType="Dict" DataColumnKey="EndProvinceId" ItemKey="LRP_Province"/>
                            <ListViewColumn Key="EndCityId_LV" Caption="目的城市" ColumnType="Dict" DataColumnKey="EndCityId" ItemKey="LRP_City"/>
                            <ListViewColumn Key="EndDistrictId_LV" Caption="目的区县" ColumnType="TextEditor" DataColumnKey="EndDistrictName"/>
                            <ListViewColumn Key="EffectiveDate_LV" Caption="生效日期" ColumnType="DatePicker" DataColumnKey="EffectiveDate" Format="yyyy-MM-dd" OnlyDate="true"/>
                            <ListViewColumn Key="ExpireDate_LV" Caption="截止日期" ColumnType="DatePicker" DataColumnKey="EffectiveDate" Format="yyyy-MM-dd" OnlyDate="true"/>
                            <ListViewColumn Key="DeliveryFeeBasis_LV" Caption="送货费计算依据" ColumnType="ComboBox" DataColumnKey="DeliveryFeeBasis">
                                <Item Caption="体积" Key="1" Value="1"/>
                                <Item Caption="重量" Key="2" Value="2"/>
                            </ListViewColumn>
                            <ListViewColumn Key="TransferFeeBasis_LV" Caption="转运费计算依据" ColumnType="ComboBox" DataColumnKey="TransferFeeBasis">
                                <Item Caption="体积" Key="1" Value="1"/>
                                <Item Caption="重量" Key="2" Value="2"/>
                            </ListViewColumn>
                            <ListViewColumn Key="Remark_LV" Caption="备注" ColumnType="TextEditor" DataColumnKey="Remark"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('V_LTLQuote', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="155px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
