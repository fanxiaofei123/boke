<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="V_TransOrderQuery" Caption="运输订单查询" FormType="Report">
    <DataSource>
        <DataObject Key="TransOrderQuery" Caption="运输订单查询" PrimaryTableKey="LRP_TransportOrder_H">
            <TableCollection>
                <Table Key="LRP_TransportOrder_H" Caption="运输订单" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Caption="对象标识" DataType="Long" Key="OID"/>
                    <Column Caption="主对象标识" DataType="Long" Key="SOID"/>
                    <Column Caption="父对象标识" DataType="Long" Key="POID"/>
                    <Column Caption="对象版本" DataType="Integer" Key="VERID"/>
                    <Column Caption="对象明细版本" DataType="Integer" Key="DVERID"/>
                    <Column Caption="状态" DataType="Integer" Key="Status"/>
                    <Column Caption="创建人" DataType="Long" Key="CREATOR"/>
                    <Column Caption="创建时间" DataType="DateTime" Key="CREATETIME"/>
                    <Column Caption="修改人" DataType="Long" Key="MODIFIER"/>
                    <Column Caption="修改时间" DataType="DateTime" Key="MODIFYTIME"/>
                    <Column Key="No" Caption="订单号" DataType="Varchar" Length="100" SortType="Desc"/>
                    <Column Key="OwnerId" Caption="货主id" DataType="Long"/>
                    <Column Key="OwnerNo" Caption="货主单号" DataType="Varchar" Length="100"/>
                    <Column Key="SendPartyId" Caption="发货方" DataType="Long"/>
                    <Column Key="SendPartyName" Caption="发货方名称" DataType="Varchar" Length="100"/>
                    <Column Key="RecvPartyId" Caption="收货方" DataType="Long"/>
                    <Column Key="RecvPartyName" Caption="收货方名称" DataType="Varchar" Length="100"/>
                    <Column Key="OrderDateTime" Caption="下单日期时间" DataType="DateTime"/>
                    <Column Key="OrderType" Caption="订单类型" DataType="Varchar" Length="10"/>
                    <Column Key="SendOrgId" Caption="发站" DataType="Long"/>
                    <Column Key="DestOrgId" Caption="到站" DataType="Long"/>
                    <Column Key="Requirement" Caption="运输要求" DataType="Varchar" Length="1000"/>
                    <Column Key="DispatchOrgId" Caption="调度组织" DataType="Long"/>
                    <Column Key="Amount" Caption="应收金额" DataType="Numeric" Precision="18" Scale="6"/>
                    <Column Key="PayMethod" Caption="付款方式" DataType="Integer"/>
                    <Column Key="GenMethod" Caption="订单生成方式" DataType="Integer"/>
                    <Column Key="SendProvinceId" Caption="发货省" DataType="Long"/>
                    <Column Key="SendCityId" Caption="市" DataType="Long"/>
                    <Column Key="RecvProvinceId" Caption="收货省" DataType="Long"/>
                    <Column Key="RecvCityId" Caption="市" DataType="Long"/>
                    <Column Key="MaterialTypeId" Caption="物料类型" DataType="Long"/>
                    <Column Key="OperateType" Caption="运作流程" DataType="Varchar"/>
                </Table>
            </TableCollection>
            <OIDFilter/>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="Close" Caption="关闭" RefKey="Close"/>
    </OperationCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main">
                <ToolBar Height="pref" Key="main_toolbar">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%">
                    <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                        <Label Key="L_No" Caption="订单号" X="5" Y="1"/>
                        <TextEditor Key="No" Caption="订单号" BuddyKey="L_No" X="6" Y="1" XSpan="3">
                            <Condition ColumnKey="No" TableKey="LRP_TransportOrder_H" CondSign="like"/>
                        </TextEditor>
                        <Label Key="L_OwnerId" Caption="货主" X="5" Y="0"/>
                        <Dict Key="OwnerId" Caption="货主" BuddyKey="L_OwnerId" X="6" Y="0" AllowMultiSelection="true" ItemKey="LRP_Owner" XSpan="3">
                            <Condition ColumnKey="OwnerId" TableKey="LRP_TransportOrder_H" CondSign="in"/>
                        </Dict>
                        <Label Key="L_OwnerNo" Caption="货主单号" X="0" Y="1"/>
                        <TextEditor Key="OwnerNo" Caption="货主单号" BuddyKey="L_OwnerNo" X="1" Y="1" XSpan="3">
                            <Condition ColumnKey="OwnerNo" TableKey="LRP_TransportOrder_H" CondSign="like"/>
                        </TextEditor>
                        <Label Key="L_Status" Caption="状态" X="5" Y="7"/>
                        <CheckListBox Key="Status" Caption="状态" BuddyKey="L_Status" X="6" Y="7" XSpan="3">
                            <Condition ColumnKey="Status" TableKey="LRP_TransportOrder_H" CondSign="in"/>
                            <Item Caption="已输入" Key="prepared" Value="100"/>
                            <Item Caption="已确认" Key="confirmed" Value="200"/>
                            <Item Caption="在途" Key="ontheway" Value="750"/>
                            <Item Caption="已签收" Key="signed" Value="920"/>
                            <Item Caption="拒收" Key="rejected" Value="930"/>
                            <Item Caption="已回单" Key="returnbill" Value="940"/>
                            <Item Caption="已作废" Key="cancelled" Value="20"/>
                        </CheckListBox>
                        <Label Key="L_SendPartyName" Caption="发货方" X="0" Y="3"/>
                        <TextEditor Key="SendPartyName" Caption="发货方" BuddyKey="L_SendPartyName" X="1" Y="3" XSpan="3">
                            <Condition ColumnKey="SendPartyName" TableKey="LRP_TransportOrder_H" CondSign="like"/>
                        </TextEditor>
                        <Label Key="L_RecvPartyName" Caption="收货方" X="5" Y="3"/>
                        <TextEditor Key="RecvPartyName" Caption="收货方" BuddyKey="L_RecvPartyName" X="6" Y="3" XSpan="3">
                            <Condition ColumnKey="RecvPartyName" TableKey="LRP_TransportOrder_H" CondSign="like"/>
                        </TextEditor>
                        <Label Key="L_OrderDateTime" Caption="下单日期时间" X="0" Y="6"/>
                        <DatePicker Key="OrderDateTime" Caption="下单日期时间" BuddyKey="L_OrderDateTime" X="1" Y="6" XSpan="3">
                            <DataBinding DefaultFormulaValue="Format(ServerDate(),'yyyy-MM-dd')&amp;' 00:00:00'"/>
                            <Condition ColumnKey="OrderDateTime" TableKey="LRP_TransportOrder_H" CondSign="between" Group="OrderDateTime" GroupHead="true"/>
                        </DatePicker>
                        <Label Key="L_OrderDateTime@2" Caption="至" X="5" Y="6"/>
                        <DatePicker Key="OrderDateTime@2" Caption="至" BuddyKey="L_OrderDateTime@2" X="6" Y="6" XSpan="3">
                            <DataBinding DefaultFormulaValue="Format(ServerDate(),'yyyy-MM-dd')&amp;' 23:59:59'"/>
                            <Condition ColumnKey="OrderDateTime" TableKey="LRP_TransportOrder_H" CondSign="in" Group="OrderDateTime" GroupTail="true"/>
                        </DatePicker>
                        <Button Key="Query" Caption="查询" X="10" Y="0" Enable="true">
                            <OnClick>
                                <![CDATA[							
                                       DealCondition();LoadData();ShowData();	
								]]>
                            </OnClick>
                        </Button>
                        <Button Key="cancel" Caption="重置" X="10" Y="1" Enable="true">
                            <OnClick>
                                <![CDATA[
								ResetCondition();						
								]]>
                            </OnClick>
                        </Button>
                        <Dict Key="DispatchOrgId" Caption="调度组织" BuddyKey="Lab_DispatchOrgId" X="1" Y="0" XSpan="3" ItemKey="LRP_DispatchOrg" AllowMultiSelection="true">
                            <Condition ColumnKey="DispatchOrgId" TableKey="LRP_TransportOrder_H" CondSign="in"/>
                        </Dict>
                        <Label Key="Lab_DispatchOrgId" Caption="调度组织" X="0" Y="0"/>
                        <CheckListBox Key="OrderType" Caption="订单类型" BuddyKey="Lab_OrderType" X="1" Y="2" XSpan="3">
                            <Condition ColumnKey="OrderType" TableKey="LRP_TransportOrder_H" CondSign="in"/>
                            <Item Caption="零担" Key="LTL" Value="LTL"/>
                            <Item Caption="整车" Key="FTL" Value="FTL"/>
                            <Item Caption="仓配" Key="WD" Value="WD"/>
                            <Item Caption="多式联运" Key="MT" Value="MT"/>
                        </CheckListBox>
                        <Label Key="Lab_OrderType" Caption="订单类型" X="0" Y="2"/>
                        <ComboBox Key="OperateType" Caption="运作流程" BuddyKey="Lab_OperateType" X="6" Y="2" XSpan="3">
                            <Condition ColumnKey="OperateType" TableKey="LRP_TransportOrder_H" CondSign="="/>
                            <Item Caption="快捷" Key="0" Value="0"/>
                            <Item Caption="直发" Key="1" Value="1"/>
                            <Item Caption="多段运输" Key="2" Value="2"/>
                        </ComboBox>
                        <Label Key="Lab_OperateType" Caption="运作流程" X="5" Y="2"/>
                        <Dict Key="SendOrgId" Caption="发站" BuddyKey="Lab_SendOrgId" X="1" Y="4" ItemKey="LRP_WarehouseCenter" XSpan="3" AllowMultiSelection="true">
                            <Condition ColumnKey="SendOrgId" TableKey="LRP_TransportOrder_H" CondSign="in"/>
                        </Dict>
                        <Label Key="Lab_SendOrgId" Caption="发站" X="0" Y="4"/>
                        <Dict Key="DestOrgId" Caption="到站" BuddyKey="Lab_DestOrgId" X="6" Y="4" ItemKey="LRP_WarehouseCenter" XSpan="3" AllowMultiSelection="true">
                            <Condition ColumnKey="DestOrgId" TableKey="LRP_TransportOrder_H" CondSign="in"/>
                        </Dict>
                        <Label Key="Lab_DestOrgId" Caption="到站" X="5" Y="4"/>
                        <Dict Key="SendProvinceId" Caption="发货省" BuddyKey="Lab_SendProvinceId" X="1" Y="5" ItemKey="LRP_Province">
                            <Condition ColumnKey="SendProvinceId" TableKey="LRP_TransportOrder_H" CondSign="="/>
                        </Dict>
                        <Label Key="Lab_SendProvinceId" Caption="发货省" X="0" Y="5"/>
                        <Dict Key="SendCityId" Caption="市" BuddyKey="Lab_SendCityId" X="3" Y="5" ItemKey="LRP_City">
                            <Condition ColumnKey="SendCityId" TableKey="LRP_TransportOrder_H" CondSign="="/>
                            <ItemFilter ItemKey="LRP_City">
                                <Filter Type="FieldValue">
                                    <FilterValue FieldKey="ProvinceId" RefValue="SendProvinceId" Type="Field"/>
                                </Filter>
                            </ItemFilter>
                        </Dict>
                        <Label Key="Lab_SendCityId" Caption="市" X="2" Y="5"/>
                        <Dict Key="RecvProvinceId" Caption="收货省" BuddyKey="Lab_RecvProvinceId" X="6" Y="5" ItemKey="LRP_Province">
                            <Condition ColumnKey="RecvProvinceId" TableKey="LRP_TransportOrder_H" CondSign="="/>
                        </Dict>
                        <Label Key="Lab_RecvProvinceId" Caption="收货省" X="5" Y="5"/>
                        <Dict Key="RecvCityId" Caption="市" BuddyKey="Lab_RecvCityId" X="8" Y="5" ItemKey="LRP_City">
                            <Condition ColumnKey="RecvCityId" TableKey="LRP_TransportOrder_H" CondSign="="/>
                            <ItemFilter ItemKey="LRP_City">
                                <Filter Type="FieldValue">
                                    <FilterValue FieldKey="ProvinceId" RefValue="RecvProvinceId" Type="Field"/>
                                </Filter>
                            </ItemFilter>
                        </Dict>
                        <Label Key="Lab_RecvCityId" Caption="市" X="7" Y="5"/>
                        <Dict Key="MaterialTypeId" Caption="物料类型" BuddyKey="Lab_MaterialTypeId" X="1" Y="7" ItemKey="MaterialType" XSpan="3">
                            <Condition ColumnKey="MaterialTypeId" TableKey="LRP_TransportOrder_H" CondSign="="/>
                        </Dict>
                        <Label Key="Lab_MaterialTypeId" Caption="物料类型" X="0" Y="7"/>
                        <RowDefCollection RowHeight="25" RowGap="5">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="5">
                            <ColumnDef Width="99px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="30px"/>
                            <ColumnDef Width="80px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <ListView Key="list" TableKey="LRP_TransportOrder_H" PageLoadType="DB" RowAlterable="false">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" Visible="false" Width="91px"/>
                            <ListViewColumn Caption="选择" ColumnType="CheckBox" Enable="true" IsSelect="true" Key="Select"/>
                            <ListViewColumn Key="Status_LV" Caption="状态" ColumnType="ComboBox" DataColumnKey="Status" SourceType="Status"/>
                            <ListViewColumn Key="DispatchOrgId_LV" Caption="调度组织" ColumnType="Dict" DataColumnKey="DispatchOrgId" ItemKey="LRP_DispatchOrg"/>
                            <ListViewColumn Key="No_LV" Caption="订单号" ColumnType="HyperLink" DataColumnKey="No" Width="160px">
                                <OnClick>
                                    <![CDATA[				SetPara("OID", GetOID());
				SetPara("No", No_LV);
                                Open('V_TOH_ReadOnly', OID);]]>
                                </OnClick>
                            </ListViewColumn>
                            <ListViewColumn Key="OwnerId_LV" Caption="货主" ColumnType="Dict" DataColumnKey="OwnerId" ItemKey="LRP_Owner"/>
                            <ListViewColumn Key="OwnerNo_LV" Caption="货主单号" ColumnType="TextEditor" DataColumnKey="OwnerNo"/>
                            <ListViewColumn Key="OperateType_LV" Caption="运作流程" ColumnType="ComboBox" DataColumnKey="OperateType">
                                <Item Caption="快捷" Key="0" Value="0"/>
                                <Item Caption="直发" Key="1" Value="1"/>
                                <Item Caption="多段运输" Key="2" Value="2"/>
                            </ListViewColumn>
                            <ListViewColumn Key="OrderType_LV" Caption="订单类型" ColumnType="ComboBox" DataColumnKey="OrderType">
                                <Item Caption="零担" Key="LTL" Value="LTL"/>
                                <Item Caption="整车" Key="FTL" Value="FTL"/>
                                <Item Caption="仓配" Key="WD" Value="WD"/>
                                <Item Caption="多式联运" Key="MT" Value="MT"/>
                            </ListViewColumn>
                            <ListViewColumn Key="OrderDateTime_LV" Caption="下单日期时间" ColumnType="DatePicker" DataColumnKey="OrderDateTime" Width="99px"/>
                            <ListViewColumn Key="SendOrgID_LV" Caption="发站" ColumnType="Dict" DataColumnKey="SendOrgId" ItemKey="LRP_WarehouseCenter"/>
                            <ListViewColumn Key="DestOrgId_LV" Caption="到站" ColumnType="Dict" DataColumnKey="DestOrgId" ItemKey="LRP_WarehouseCenter"/>
                            <ListViewColumn Key="Requirement_LV" Caption="运输要求" ColumnType="TextEditor" DataColumnKey="Requirement"/>
                            <ListViewColumn Key="SendPartyName_LV" Caption="发货方" ColumnType="TextEditor" DataColumnKey="SendPartyName"/>
                            <ListViewColumn Key="SendProvinceId_LV" Caption="发货省" ColumnType="Dict" DataColumnKey="SendProvinceId" ItemKey="LRP_Province"/>
                            <ListViewColumn Key="SendCityId_LV" Caption="市" ColumnType="Dict" DataColumnKey="SendCityId" ItemKey="LRP_City"/>
                            <ListViewColumn Key="RecvPartyName_LV" Caption="收货方" ColumnType="TextEditor" DataColumnKey="RecvPartyName"/>
                            <ListViewColumn Key="RecvProvinceId_LV" Caption="收货省" ColumnType="Dict" DataColumnKey="RecvProvinceId" ItemKey="LRP_Province"/>
                            <ListViewColumn Key="RecvCityId_LV" Caption="市" ColumnType="Dict" DataColumnKey="RecvCityId" ItemKey="LRP_City"/>
                            <ListViewColumn Key="Amount" Caption="金额" ColumnType="TextEditor" DataColumnKey="Amount"/>
                            <ListViewColumn Key="PayMethod_LV" Caption="付款方式" ColumnType="ComboBox" DataColumnKey="PayMethod"/>
                            <ListViewColumn Key="GenMethod_LV" Caption="生成方式" ColumnType="ComboBox" DataColumnKey="GenMethod"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('V_TOH_ReadOnly', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="290px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
