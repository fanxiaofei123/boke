<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<DataObject Key="CHCS_Contract_Amount" Caption="合同金额监控">
    <TableCollection>
        <Table Key="CHCS_Contract_Amount_Monitor" Caption="合同金额监控" TableMode="Detail" SourceType="Query">
            <Column Key="Contract_Code" Caption="合同编号" DataType="Varchar"/>
            <Column Key="Contract_Name" Caption="合同名称" DataType="Varchar"/>
            <Column Key="Supplier_NO" Caption="供应商编号" DataType="Varchar"/>
            <Column Key="Supplier_Name" Caption="供应商名称" DataType="Varchar"/>
            <Column Key="Supplier_Abbr" Caption="供应商简称" DataType="Varchar"/>
            <Column Key="Valid_Time_Start" Caption="合同生效日期" DataType="Date"/>
            <Column Key="Valid_Time_End" Caption="合同到期日期" DataType="Date"/>
            <Column Key="Contract_Type" Caption="合同类型" DataType="Varchar"/>
            <Column Key="Contract_Classify" Caption="合同分类" DataType="Varchar"/>
            <Column Key="Currency" Caption="币种" DataType="Long"/>
            <Statement>
                <![CDATA[SELECT a.Contract_Code /*合同编号*/	,
	a.Contract_Name /*合同名称*/,
	a.Supplier_NO /*供应商编号*/,
	a.Supplier_Name /*供应商名称*/,
	a.Supplier_Abbr /*供应商简称*/,
	a.Valid_Time_Start /*合同生效日期*/,
	a.Valid_Time_End /*合同到期日期*/,
	a.Contract_Type /*合同类型*/,
	a.Contract_Classify /*合同分类*/,
	a.Currency /*币种*/ 
FROM CHCS_Verdor_Contract a]]>
            </Statement>
        </Table>
    </TableCollection>
</DataObject>
