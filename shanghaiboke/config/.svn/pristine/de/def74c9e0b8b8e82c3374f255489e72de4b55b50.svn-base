                    <Statement>
                        <![CDATA[select * from(select q.WarehouseCenterId,q.OwnerId,q.MaterialId,q.BatchNo,q.ManufDate,q.ExpireDate,q.ProjectId,q.RecvDate,
q.StrUserDef1,q.StrUserDef2,q.StrUserDef3,q.StrUserDef4,q.StrUserDef5,q.DictUserDef1,q.DictUserDef2,q.DictUserDef3,q.DictUserDef4,
q.DictUserDef5,q.DateUserDef1,q.DateUserDef2,q.NumUserDef1,q.NumUserDef2,q.NumUserDef3,q.MaterialStatusId,q.OnhandQty,q.ExpectInQty,
q.ExpectReplenishQty,q.ExpectOutQty,m.BasicUnit,q.StoreroomId,q.StoreareaId,q.LocationId,l.IsLocked,q.RefOrderNo,q.HandingUnitNo
 from LRP_Quant q left join lrp_MATERIAL m on q.MaterialId=m.OID left join LRP_Storearea s on q.StoreareaId=s.oid 
 left join LRP_Location l on q.LocationId = l.OID where (q.OnhandQty+q.ExpectInQty+q.ExpectReplenishQty+q.ExpectOutQty)>0 
 and s.IsKittingArea=1)a]]>
                    </Statement>
