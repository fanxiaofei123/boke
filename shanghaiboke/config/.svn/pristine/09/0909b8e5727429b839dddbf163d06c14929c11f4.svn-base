                                    <ColumnDef Width="50px"/>
                                    <ColumnDef Width="100%"/>
                                    <ColumnDef Width="30px"/>
                                </ColumnDefCollection>
                            </GridLayoutPanel>
                            <RowClick>
                                <![CDATA[
                                       PushPara('Type',0);	
									   PushPara('OrderType',GetValue('OrderType'));
									   if(SendOrgId != '')
									   {
											Show('SendInformation');
									   }else{
									       Show('SendRouteInformation');
									   }
								]]>
                            </RowClick>
                        </TableRow>
