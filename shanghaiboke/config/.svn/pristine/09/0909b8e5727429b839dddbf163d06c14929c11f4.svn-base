                                <Image Key="Image1" Caption="Image1" Height="auto" Width="30px" Stretch="true" SourceType="Resource" Image="38.png" Margin="5px" ImageScaleType="Fit_Center" X="0" Y="0" YSpan="2" HasBorder="true"/>
                                <Label Key="lbl_destl" Caption="请输入途经地" Width="80px" Height="auto" Visible="OrgId == ''" X="1" Y="0" XSpan="2" YSpan="2">
                                    <Format HAlign="Left"/>
                                </Label>
                                <Label Key="OrgId" Caption="地址名称" Visible="False" Width="80px" Height="auto" X="1" Y="0" XSpan="2">
                                    <DataBinding ColumnKey="AddressName"/>
                                </Label>
                                <Label Key="Address" Caption="地址" Visible="False" Width="80px" Height="auto" X="2" Y="0">
                                    <DataBinding ColumnKey="Address"/>
                                </Label>
                                <Label Key="Address2" Height="auto" X="1" Y="0" XSpan="2">
                                    <DataBinding DefaultFormulaValue="IIF(Address=='',OrgId,OrgId&amp;'('&amp;Address&amp;')')"/>
                                    <Format HAlign="Left">
                                        <Font Size="15"/>
                                    </Format>
                                </Label>
                                <Label Key="Contact" Width="80px" Height="auto" X="1" Y="1">
                                    <DataBinding ColumnKey="Contact"/>
                                </Label>
                                <Label Key="Tel" Width="80px" Height="auto" X="2" Y="1">
                                    <DataBinding ColumnKey="Tel"/>
                                </Label>
                                <Label Key="Latitude" Width="80px" Visible="False" Height="auto" X="1" Y="1">
                                    <DataBinding ColumnKey="Latitude"/>
                                </Label>
                                <Label Key="Longitude" Width="80px" Visible="False" Height="auto" X="2" Y="1">
                                    <DataBinding ColumnKey="Longitude"/>
                                </Label>
                                <Label Key="ProvinceId" Width="80px" Visible="False" Height="auto" X="1" Y="1">
                                    <DataBinding ColumnKey="ProvinceId"/>
                                </Label>
                                <Label Key="CityId" Width="80px" Visible="False" Height="auto" X="2" Y="1">
                                    <DataBinding ColumnKey="CityId"/>
                                </Label>
                                <Label Key="DistrictId" Width="80px" Visible="False" Height="auto" X="2" Y="1">
                                    <DataBinding ColumnKey="DistrictId"/>
                                </Label>
                                <Button Key="spec_sel3" Caption="删除" Enable="true" Width="38px" Height="auto" BorderStyle="solid" X="3" Y="0" YSpan="2" BorderColor="#ffffff" BorderRadius="4" HasBorder="true">
                                    <Format BackColor="Transparent" ForeColor="#000000">
                                        <Font Size="15"/>
                                    </Format>
                                    <OnClick>
                                        <![CDATA[
									       ShowConfirm('确定删除该途径地吗?', 'YES_NO', 
												{
													YES: {
														DeleteRow('list', GetRowIndex('list'));
														ShowToast('已删除');
													}, 
													NO: {} 
												}
											)
											
									     ]]>
                                    </OnClick>
                                </Button>
