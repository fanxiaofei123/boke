                                    <DataBinding ColumnKey="RecvLatitude" TableKey="LRP_TransportOrder_H"/>
                                </Label>
                                <Button Key="spec_sel2" Icon="more_arrow.png" OnlyIcon="True" Width="20px" Height="auto" BorderStyle="none" X="3" Y="0" Enable="true" YSpan="2"/>
                                <RowDefCollection RowGap="1">
                                    <RowDef Height="40px"/>
                                    <RowDef Height="33px"/>
                                </RowDefCollection>
                                <ColumnDefCollection ColumnGap="3">
                                    <ColumnDef Width="40px"/>
                                    <ColumnDef Width="50px"/>
                                    <ColumnDef Width="100%"/>
                                    <ColumnDef Width="20px"/>
                                </ColumnDefCollection>
                            </GridLayoutPanel>
                            <RowClick>
                                <![CDATA[
								      PushPara('Type',0);
                                       var count = -1;
									   PushPara('rowcount',count);
									   PushPara('formkey','Order');	
									   PushPara('OrderType',GetValue('OrderType'));									  
									   if(DestOrgId != '')
									   {
										   Show('RecInformation');
									   }else{ 
										   Show('RecvRouteInformation');
									   }
								]]>
                            </RowClick>
