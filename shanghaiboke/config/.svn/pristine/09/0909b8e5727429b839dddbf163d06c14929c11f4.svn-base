                                <![CDATA[
								PushPara('rowcount',GetRowCount('list'));
								PushPara('rowIndex',GetRowIndex('list'));
								PushPara('formkey','Order');								
								PushPara('RecType',1);
								SetPara('Type',0);
								PushPara('OrderType',GetValue('OrderType'));
								Show('RecInformation');
								
								
								]]>
                            </RowClick>
                        </TableRow>
