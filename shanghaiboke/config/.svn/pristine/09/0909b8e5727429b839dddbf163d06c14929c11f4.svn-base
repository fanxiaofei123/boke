                    <Label Key="des" Caption="实际费用可能因实际行驶里程/等候时间等因素而异" X="0" Y="1" Height="auto" BorderStyle="solid" BorderColor="Transparent" HasBorder="true" XSpan="4">
                        <Format BackColor="#ffffff" ForeColor="#bbbbbb" HAlign="Center">
                            <Font Size="12"/>
                        </Format>
                    </Label>
                    <RowDefCollection RowGap="1">
                        <RowDef Height="50px"/>
                        <RowDef Height="30px"/>
                    </RowDefCollection>
                    <ColumnDefCollection ColumnGap="3">
                        <ColumnDef Width="25%"/>
                        <ColumnDef Width="25%"/>
                        <ColumnDef Width="25%"/>
                        <ColumnDef Width="25%"/>
                    </ColumnDefCollection>
                </GridLayoutPanel>
                <LinearLayoutPanel Key="opt_pane2" X="0" Y="2" Orientation="Horizontal" BorderStyle="solid none" Width="100%">
                    <Format BackColor="Transparent"/>
                    <Button Key="order" Caption="下一步" Enable="True" Width="100%" Height="auto" Weight="1.0" BorderStyle="none">
                        <Format BackColor="#f14c52" ForeColor="#ffffff"/>
                        <OnClick>
                            <![CDATA[
							if(GetValue('SendOrgId') == ""){
							   ShowToast("请先输入始发地");
							   return true;
							}
							if(GetValue('DestOrgId') == ""){
							  ShowToast("请先输入目的地");
							  return true;
							}
							var count = GetRowCount('list');
							var index = 0;
							while(index < count){
								if(GetCellValue('list',index,'OrgId') == ''){
									index = index + 1;	
									ShowToast("请先输入第"+index+"条途径地");
									return true;
								}
								index = index + 1;
							}
							
							if(GetValue('spec_sel_fee')<0){
							  ShowToast("费用计算失败");
							  return true;
							}
							
							
							PushPara('rowcount',count);
							PushPara('rowIndex',GetRowIndex('list'));
							PushPara('DestOrgId1',GetValue('DestOrgId'));
							PushPara('No1',GetValue('No'));
                            PushPara('Time1',GetValue('Time'));
                            PushPara('SendOrgId1',GetValue('SendOrgId'));
							PushPara('SendAddress1',GetValue('SendAddress'));
                            PushPara('SendContact1',GetValue('SendContact'));
							PushPara('SendTel1',GetValue('SendTel'));
							PushPara('spec_sel_fee1',GetValue('spec_sel_fee'));
                            
							PushPara('DestAddress1',GetValue('DestAddress'));
                            PushPara('DestContact1',GetValue('DestContact'));
							PushPara('DestTel1',GetValue('DestTel'));
							PushPara('OrderLoadWeight1',GetValue('spec_sel_LoadWeight'));
							PushPara('spec_sel_LoadCubage1',GetValue('spec_sel_LoadCubage'));
							PushPara('spec_sel_Mileage1',GetValue('spec_sel_Mileage'));
							PushPara('spec_sel_Payment1',GetValue('spec_sel_Payment'));
							PushPara('spec_sel_Remark1',GetValue('spec_sel_Remark'));
							PushPara('Serv1',GetValue('Serv'));						
							Show('DriverConfirmedOrder');
							]]>
                        </OnClick>
                    </Button>
                </LinearLayoutPanel>
                <RowDefCollection>
                    <RowDef Height="100%"/>
                    <RowDef Height="90px"/>
                    <RowDef Height="45px"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
