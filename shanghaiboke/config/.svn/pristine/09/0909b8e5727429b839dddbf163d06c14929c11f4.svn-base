                                    <RowDef Height="40px"/>
                                    <RowDef Height="30px"/>
                                </RowDefCollection>
                                <ColumnDefCollection ColumnGap="3">
                                    <ColumnDef Width="40px"/>
                                    <ColumnDef Width="50px"/>
                                    <ColumnDef Width="100%"/>
                                    <ColumnDef Width="38px"/>
                                </ColumnDefCollection>
                            </GridLayoutPanel>
