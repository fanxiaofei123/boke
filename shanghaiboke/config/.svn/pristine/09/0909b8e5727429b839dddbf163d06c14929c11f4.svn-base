                            </LinearLayoutPanel>
                        </TableRow>
                        <TableRow Key="r2" TopMargin="5px" SeparatorStyle="solid none" Enable="true" SeparatorColor="#b8becc">
                            <GridLayoutPanel Key="GridLayoutPanel1" Padding="5px" Caption="GridLayoutPanel1">
