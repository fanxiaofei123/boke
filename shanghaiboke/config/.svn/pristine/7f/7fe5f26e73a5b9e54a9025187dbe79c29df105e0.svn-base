<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Caption="退货通知单" FormType="Entity" Key="V_ReturnNotice" ViewKey="V_ReturnNoticeView">
    <DataSource RefObjectKey="LRP_ReturnNotice"/>
    <OperationCollection>
        <Operation Caption="新增" Key="New" Visible="ReadOnly()">
            <Action>
                <![CDATA[
				New('V_ReturnNotice');
			]]>
            </Action>
        </Operation>
        <Operation Caption="复制新增" Key="CopyNew" RefKey="CopyNew" ShortCuts="Ctrl+E"/>
        <Operation Caption="保存" Key="Save" Visible="!ReadOnly()">
            <Action>
                <![CDATA[
				            if(GetRowCount('Grid0', false)<=0){ 
						    Confirm(LocaleString('OwnerNull', 'key103'), 'OK','OK:{}');
						    return true;
				           }
						  var i=0;
						  if(RefOutboundNo!=''){
				           var rs=DBNamedQuery("SelectRefOutboundNo",OwnerId);
							rs.beforefirst();						
						   while(rs.next()){  
							if( GetValue('RefOutboundNo') != rs.No){	
                                    	i=i+1;							  
								 }
								else
								{
								  i=0;
								  break;
								}
							   }
							   if(i>0){
								  Confirm(LocaleString('OwnerNull', 'key121'), 'OK','OK:{}');
								  return true;
								}
						   }
						   SaveData(); UpdateView();						      						   						   						  		   
				]]>
            </Action>
        </Operation>
        <Operation Caption="编辑" Key="Edit" RefKey="Edit" ShortCuts="Ctrl+E"/>
        <Operation Caption="删除" Key="Delete" Visible="ReadOnly()&amp;&amp;Status==StatusValue('prepared')&amp;&amp;LogisticsOrderNo==''">
            <Action>
                <![CDATA[
			Confirm(LocaleString('CommonDef', 'key2'),'YES_NO',{YES:{DeleteData();UpdateView();Close();},NO:{}});
			
			]]>
            </Action>
        </Operation>
        <Operation Caption="取消" Key="Cancel" RefKey="Cancel"/>
        <Operation Caption="确认" Key="Confirm" RefKey="Confirm"/>
        <Operation Caption="撤销确认" Key="Confirm-R" Visible="ReadOnly()&amp;&amp;Status==StatusValue('confirmed')">
            <Action>
                <![CDATA[InvokeService("LRP_PlatformReservationDelete", true, false,GetOID());
Status=StatusValue('prepared');SaveData();UpdateView();
]]>
            </Action>
            <ExceptionHandler>
                <![CDATA[Status=StatusValue('confirmed');]]>
            </ExceptionHandler>
        </Operation>
        <Operation Caption="作废" Key="ToCancel" RefKey="ToCancel"/>
        <Operation Caption="撤销作废" Key="ToCancel-R" RefKey="ToCancel-R"/>
        <Operation Caption="收货" Enable="Status==200||Status==500" Key="Receipt">
            <Action>
                <![CDATA[
				Map('ReturnNotice-Tx103','V_Tx103')
			]]>
            </Action>
        </Operation>
        <Operation Caption="快速上架" Enable="Status==200||Status==500" Key="Putaway">
            <Action>
                <![CDATA[
				MidMap('ReturnNotice-Tx112','V_Tx112')
			]]>
            </Action>
        </Operation>
        <Operation Caption="关闭界面" Key="Close" RefKey="Close"/>
        <Operation Key="Order" Caption="月台预约" Enable="Status==200">
            <Action>
                <![CDATA[//预约类型、仓储中心、单据ID、单号、预计到达日期时间、承运方、月台
PlatformOrder(2,WarehouseCenterId,GetOID(),NoticeNo,ExpectDateTimeFrom,ExpectDateTimeTo,CarrierId,PlatformId);
]]>
            </Action>
        </Operation>
    </OperationCollection>
    <QueryCollection>
        <Query Description="查询所选货主下的出库订单号" Key="SelectRefOutboundNo">
            <Statement>
                <![CDATA[select No from LRP_OutboundNotice_H where OwnerId=?]]>
            </Statement>
            <ParameterCollection>
                <Parameter DataType="Long"/>
            </ParameterCollection>
        </Query>
    </QueryCollection>
