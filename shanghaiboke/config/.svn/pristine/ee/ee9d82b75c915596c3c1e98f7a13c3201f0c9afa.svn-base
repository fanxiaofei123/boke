                            </TextEditor>
                            <Label Key="Lab_Contract_Unit" Caption="签约单位" X="0" Y="3"/>
                            <TextEditor Key="Contract_Classify" Caption="合同分类" BuddyKey="Lab_Contract_Classify" X="7" Y="0" AsQuery="true">
                                <DataBinding ColumnKey="Contract_Classify" TableKey="CHCS_Verdor_Contract_Supplement"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Classify" Caption="合同分类" X="6" Y="0"/>
                            <TextEditor Key="Supplier_NO" Caption="供应商编号" BuddyKey="Lab_Supplier_NO" X="1" Y="1" AsQuery="true">
                                <DataBinding ColumnKey="Supplier_NO" TableKey="CHCS_Verdor_Contract_Supplement"/>
                            </TextEditor>
