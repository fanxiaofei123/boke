                                    <DataBinding ColumnKey="Valid_Time_Start"/>
                                </GridCell>
                                <GridCell Key="Valid_Time_End" Caption="有效期止" CellType="DatePicker" OnlyDate="true">
                                    <DataBinding ColumnKey="Valid_Time_End"/>
                                </GridCell>
                                <UICheckRuleCollection>
                                    <UICheckRule ErrorInfo="餐种不能重复">
