<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Segment" Caption="航段" FormType="ChainDict">
    <DataSource RefObjectKey="CHCS_Segment"/>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Description="载入事件" Range="Action" Verb="Load">
            <![CDATA[LoadData()]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main" Height="100%">
                <TabPanel Caption="TabPanel0" Key="TabPanel0" Height="pref">
                    <GridLayoutPanel Caption="基本信息" Key="BasicTab" Padding="10px">
                        <TextEditor BuddyKey="L_Code" Caption="代码" Enable="!ReadOnly()" Key="Code" Tip="「代码」不可为空" X="1" XSpan="3" Y="0">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="Code" Required="true"/>
                        </TextEditor>
                        <TextEditor BuddyKey="L_Name" Caption="名称" Enable="!ReadOnly()" Key="Name" Tip="「名称」不可为空" X="6" XSpan="3" Y="0">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="Name" Required="true"/>
                        </TextEditor>
                        <Label Caption="代码" Key="L_Code" X="0" Y="0"/>
                        <Label Caption="名称" Key="L_Name" X="5" Y="0"/>
                        <Dict Key="Route_Arrival_Airport_Code" Caption="到达站" BuddyKey="Lab_Route_Arrival_Airport_Code" X="6" Y="1" ItemKey="LRP_Lookup_L" XSpan="3">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="Route_Arrival_Airport_Code"/>
                        </Dict>
                        <Label Key="Lab_Route_Arrival_Airport_Code" Caption="到达站" X="5" Y="1"/>
                        <Dict Key="Route_Departure_Airport_Code" Caption="起飞站" BuddyKey="Lab_Route_Departure_Airport_Code" X="1" Y="1" ItemKey="CHCS_Airport" XSpan="3">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="Route_Departure_Airport_Code"/>
                        </Dict>
                        <Label Key="Lab_Route_Departure_Airport_Code" Caption="起飞站" X="0" Y="1"/>
                        <DatePicker Key="Time_Of_Departure" Caption="开始时间" BuddyKey="Lab_Time_Of_Departure" X="1" Y="2" XSpan="3">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="Time_Of_Departure"/>
                        </DatePicker>
                        <Label Key="Lab_Time_Of_Departure" Caption="开始时间" X="0" Y="2"/>
                        <DatePicker Key="Time_Of_Arrival" Caption="结束时间" BuddyKey="Lab_Time_Of_Arrival" X="6" Y="2" XSpan="3">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="Time_Of_Arrival"/>
                        </DatePicker>
                        <Label Key="Lab_Time_Of_Arrival" Caption="结束时间" X="5" Y="2"/>
                        <NumberEditor Key="Fly_Time" Caption="飞行时间" BuddyKey="Lab_Fly_Time" X="1" Y="3" Format="" XSpan="3">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="Fly_Time"/>
                        </NumberEditor>
                        <Label Key="Lab_Fly_Time" Caption="飞行时间" X="0" Y="3"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="78px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="94px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Key="SysInfo" Padding="10px" Caption="系统信息">
                        <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="CREATOR"/>
                        </Dict>
                        <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                        <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="CREATETIME"/>
                        </DatePicker>
                        <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                        <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="MODIFIER"/>
                        </Dict>
                        <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                        <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                            <DataBinding TableKey="CHCS_Segment" ColumnKey="MODIFYTIME"/>
                        </DatePicker>
                        <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                        <RowDefCollection RowGap="5">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="5">
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
