<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Transfer_Handover" Caption="调拨交接单" FormType="Entity" ViewKey="CHCS_Transfer_HandoverView">
    <DataSource RefObjectKey="CHCS_Transfer_Handover"/>
    <OperationCollection>
        <Operation Key="WORKITEM" Caption="WORKITEM" Tag="WORKITEM"/>
        <Operation Key="BPM" Caption="BPM" Tag="BPM"/>
        <Operation Key="Save" Caption="保存" Visible="!ReadOnly()">
            <Action>
                <![CDATA[SaveData(); UpdateView();]]>
            </Action>
        </Operation>
        <Operation Key="New" Caption="新增" Visible="ReadOnly()">
            <Action>
                <![CDATA[New('CHCS_Transfer_Handover', 'self');]]>
            </Action>
        </Operation>
        <Operation Key="Edit" Caption="编辑" Visible="ReadOnly()">
            <Action>
                <![CDATA[Edit();]]>
            </Action>
        </Operation>
        <Operation Key="Cancel" Caption="取消" Visible="!ReadOnly()">
            <Action>
                <![CDATA[Cancel();]]>
            </Action>
        </Operation>
        <Operation Key="Close" Caption="关闭" Visible="ReadOnly()">
            <Action>
                <![CDATA[Close()]]>
            </Action>
        </Operation>
        <Operation Key="export" Caption="导出">
            <Action>
                <![CDATA[ExportExcel(true,{},true);]]>
            </Action>
        </Operation>
        <Operation Key="print" Caption="打印"/>
        <Operation Key="Sign" Caption="签收"/>
        <Operation Key="Notice" Caption="通知供应室"/>
    </OperationCollection>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板">
                    <TabPanel Caption="TabPanel1" Height="pref" Key="TabPanel1">
                        <GridLayoutPanel Key="first_head" Height="pref" Padding="5px" OverflowY="Scroll" Caption="基本信息">
                            <TextEditor Key="NO" Caption="交接单号" BuddyKey="Lab_NO" X="1" Y="0" Enable="false" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="NO" TableKey="CHCS_Transfer_Handover_H"/>
                            </TextEditor>
                            <Label Key="Lab_NO" Caption="交接单号" X="0" Y="0"/>
                            <DatePicker Key="BillDate" Caption="交接日期" BuddyKey="Lab_BillDate" X="5" Y="0" Enable="false" XSpan="2">
                                <DataBinding ColumnKey="BillDate" TableKey="CHCS_Transfer_Handover_H"/>
                            </DatePicker>
                            <Label Key="Lab_BillDate" Caption="交接日期" X="4" Y="0"/>
                            <ComboBox Key="Status" Caption="状态" BuddyKey="Lab_Status" X="9" Y="0" Enable="false" SourceType="Status" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="Status" TableKey="CHCS_Transfer_Handover_H"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="状态" X="8" Y="0"/>
                            <DatePicker Key="FlightDate" Caption="航班日期" BuddyKey="Lab_FlightDate" X="5" Y="1" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="FlightDate" TableKey="CHCS_Transfer_Handover_H"/>
                                <Format/>
                            </DatePicker>
                            <Label Key="Lab_FlightDate" Caption="航班日期" X="4" Y="1"/>
                            <Dict Key="Aircraft_Number" Caption="飞机号" BuddyKey="Lab_Aircraft_Number" X="9" Y="1" ItemKey="CHCS_Aircraft" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="Aircraft_Number" TableKey="CHCS_Transfer_Handover_H"/>
                            </Dict>
                            <Label Key="Lab_Aircraft_Number" Caption="飞机号" X="8" Y="1"/>
                            <Dict Key="Aircraft_Type_Number" Caption="机型" BuddyKey="Lab_Aircraft_Type_Number" X="1" Y="2" ItemKey="CHCS_Aircraft_Type" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="Aircraft_Type_Number" TableKey="CHCS_Transfer_Handover_H"/>
                            </Dict>
                            <Label Key="Lab_Aircraft_Type_Number" Caption="机型" X="0" Y="2"/>
                            <ComboBox Key="Dishex_Plane_Position" Caption="餐具在机上位置" BuddyKey="Lab_Dishex_Plane_Position" X="5" Y="2" XSpan="2">
                                <DataBinding ColumnKey="Dishex_Plane_Position" TableKey="CHCS_Transfer_Handover_H"/>
                            </ComboBox>
                            <Label Key="Lab_Dishex_Plane_Position" Caption="餐具在机上位置" X="4" Y="2"/>
                            <TextEditor Key="Lock_Catch_Number" Caption="锁扣号" BuddyKey="Lab_Lock_Catch_Number" X="9" Y="2" XSpan="2">
                                <DataBinding ColumnKey="Lock_Catch_Number" TableKey="CHCS_Transfer_Handover_H"/>
                            </TextEditor>
                            <Label Key="Lab_Lock_Catch_Number" Caption="锁扣号" X="8" Y="2"/>
                            <ComboBox Key="In_Warehouse" Caption="调入仓库" BuddyKey="Lab_In_Warehouse" X="1" Y="3" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="In_Warehouse" TableKey="CHCS_Transfer_Handover_H"/>
                            </ComboBox>
                            <Label Key="Lab_In_Warehouse" Caption="调入仓库" X="0" Y="3"/>
                            <TextEditor Key="Dispatch_Order_No" Caption="调拨单编号" BuddyKey="Lab_Dispatch_Order_No" X="5" Y="3" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="Dispatch_Order_No" TableKey="CHCS_Transfer_Handover_H"/>
                            </TextEditor>
                            <Label Key="Lab_Dispatch_Order_No" Caption="调拨单编号" X="4" Y="3"/>
                            <ComboBox Key="Out_Warehouse" Caption="调出仓库" BuddyKey="Lab_Out_Warehouse" X="9" Y="3" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="Out_Warehouse" TableKey="CHCS_Transfer_Handover_H"/>
                            </ComboBox>
                            <Label Key="Lab_Out_Warehouse" Caption="调出仓库" X="8" Y="3"/>
                            <Dict Key="Steward" Caption="乘务员" BuddyKey="Lab_Steward" X="1" Y="4" ItemKey="CHCS_Employee" XSpan="2">
                                <DataBinding ColumnKey="Steward" TableKey="CHCS_Transfer_Handover_H"/>
                            </Dict>
                            <Label Key="Lab_Steward" Caption="乘务员" X="0" Y="4"/>
                            <Dict Key="Sending_Staff" Caption="送出单位航机员" BuddyKey="Lab_Sending_Staff" X="5" Y="4" ItemKey="CHCS_Employee" XSpan="2">
                                <DataBinding ColumnKey="Sending_Staff" TableKey="CHCS_Transfer_Handover_H"/>
                            </Dict>
                            <Label Key="Lab_Sending_Staff" Caption="送出单位航机员" X="4" Y="4"/>
                            <Dict Key="Receiving_Staff" Caption="接收单位航机员" BuddyKey="Lab_Receiving_Staff" X="9" Y="4" ItemKey="CHCS_Employee" XSpan="2">
                                <DataBinding ColumnKey="Receiving_Staff" TableKey="CHCS_Transfer_Handover_H"/>
                            </Dict>
                            <Label Key="Lab_Receiving_Staff" Caption="接收单位航机员" X="8" Y="4"/>
                            <Dict Key="Flight_Number" Caption="航班号" BuddyKey="Lab_Flight_Number" X="1" Y="1" ItemKey="CHCS_Flight" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="Flight_Number" TableKey="CHCS_Transfer_Handover_H"/>
                            </Dict>
                            <Label Key="Lab_Flight_Number" Caption="航班号" X="0" Y="1"/>
                            <RowDefCollection RowGap="5" RowHeight="25">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="SystemInfo" Padding="5px" Caption="系统信息">
                            <DatePicker Key="CreateTime" Caption="制单日期" BuddyKey="Lab_CreateTime" X="3" Y="0">
                                <DataBinding ColumnKey="CreateTime" TableKey="CHCS_Transfer_Handover_H"/>
                            </DatePicker>
                            <Label Key="Lab_CreateTime" Caption="制单日期" X="2" Y="0"/>
                            <DatePicker Key="Modifytime" Caption="修改日期" BuddyKey="Lab_Modifytime" X="3" Y="1">
                                <DataBinding ColumnKey="Modifytime" TableKey="CHCS_Transfer_Handover_H"/>
                            </DatePicker>
                            <Label Key="Lab_Modifytime" Caption="修改日期" X="2" Y="1"/>
                            <Dict Key="Creator" Caption="制单人" BuddyKey="Lab_Creator" X="1" Y="0" ItemKey="Operator">
                                <DataBinding ColumnKey="Creator" TableKey="CHCS_Transfer_Handover_H"/>
                            </Dict>
                            <Label Key="Lab_Creator" Caption="制单人" X="0" Y="0"/>
                            <Dict Key="Modifier" Caption="修改人" BuddyKey="Lab_Modifier" X="1" Y="1" ItemKey="Operator">
                                <DataBinding ColumnKey="Modifier" TableKey="CHCS_Transfer_Handover_H"/>
                            </Dict>
                            <Label Key="Lab_Modifier" Caption="修改人" X="0" Y="1"/>
                            <RowDefCollection RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <Grid Key="Grid1" Caption="Grid1">
                        <GridColumnCollection>
                            <GridColumn Key="column1" Caption="选择" Width="80px"/>
                            <GridColumn Key="TableWareID" Caption="餐具种类" Width="80px"/>
                            <GridColumn Key="Quantity" Caption="实配数量" Width="80px"/>
                            <GridColumn Key="Act_Recv_Qty" Caption="实收数量" Width="80px"/>
                            <GridColumn Key="Urgency_Degree" Caption="紧急程度" Width="80px"/>
                            <GridColumn Key="Unit" Caption="单位" Width="80px"/>
                            <GridColumn Key="Remark" Caption="备注" Width="80px"/>
                        </GridColumnCollection>
                        <GridRowCollection>
                            <GridRow Key="row1" RowHeight="36" TableKey="CHCS_Transfer_Handover_L">
                                <GridCell Key="select" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                <GridCell Key="TableWareID" Caption="餐具种类" CellType="Dict" ItemKey="CHCS_Product">
                                    <DataBinding ColumnKey="TableWareID"/>
                                    <ItemFilter>
                                        <Filter Query="select p.OID from CHCS_Product p left join lrp_lookup_l l on p.Type=l.OID where l.Code=upper(?)" Type="DataSet">
                                            <FilterValue DataType="Varchar" ParaValue="CPLX-CJ" Type="Const"/>
                                        </Filter>
                                    </ItemFilter>
                                </GridCell>
                                <GridCell Key="Quantity" Caption="实配数量" CellType="NumberEditor" Format="">
                                    <DataBinding ColumnKey="Quantity"/>
                                </GridCell>
                                <GridCell Key="Act_Recv_Qty" Caption="实收数量" CellType="NumberEditor" Format="">
                                    <DataBinding ColumnKey="Act_Recv_Qty"/>
                                </GridCell>
                                <GridCell Key="Urgency_Degree" Caption="紧急程度" CellType="ComboBox">
                                    <DataBinding ColumnKey="Urgency_Degree"/>
                                </GridCell>
                                <GridCell Key="Unit" Caption="单位" CellType="Dict" Enable="false" ItemKey="LRP_Lookup_L">
                                    <DataBinding DefaultFormulaValue="GetDictValue(&quot;CHCS_Product&quot;, TableWareID, &quot;Unit&quot;)" ColumnKey="Unit"/>
                                </GridCell>
                                <GridCell Key="Remark" Caption="备注" CellType="TextEditor">
                                    <DataBinding ColumnKey="Remark"/>
                                </GridCell>
                            </GridRow>
                            <GridRow Key="row2" RowHeight="36" RowType="Total">
                                <GridCell Key="cell1"/>
                                <GridCell Key="cell2"/>
                                <GridCell Key="cell3" Caption="实配数量汇总">
                                    <DataBinding DefaultFormulaValue="Sum(&quot;Quantity&quot;);"/>
                                </GridCell>
                                <GridCell Key="cell4" Caption="实收数量汇总">
                                    <DataBinding DefaultFormulaValue="Sum(&quot;Act_Recv_Qty&quot;);"/>
                                </GridCell>
                                <GridCell Key="cell5"/>
                                <GridCell Key="cell6"/>
                                <GridCell Key="cell7"/>
                            </GridRow>
                        </GridRowCollection>
                    </Grid>
                    <SplitSize Size="40%"/>
                    <SplitSize Size="60%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
