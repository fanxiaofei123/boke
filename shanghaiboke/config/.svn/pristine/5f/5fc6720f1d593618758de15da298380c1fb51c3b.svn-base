<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="V_Tx406" Caption="初始库存单" FormType="Entity" ViewKey="V_Tx406View">
    <DataSource RefObjectKey="LRP_WMTx"/>
    <OperationCollection>
        <Operation Key="Save" Caption="保存" Visible="!ReadOnly()">
            <Action>
                <![CDATA[var flag=false;
if(GetRowCount('Grid1', false)<=0){
	Confirm(LocaleString("OwnerNull", "keyo"), 'OK','OK:{}');
	return true;
}
var rs = DBNamedQuery("SelectMaSta", TxTypeCode);
var index = 0;
loop 'Grid1' (true){
	index = index + 1;
        var mtlStatusId = GetCellValue('Grid1', -1, 'DestMaterialStatusId');
	rs.beforefirst();
	while(rs.next()){
		if(mtlStatusId > 0 && rs.MaterialStatusId == mtlStatusId){
			flag=true;
			break;
		}
		flag = false;
	}
	if(flag == false){
		  Confirm(LocaleParaFormat("CommonDef","key45",index)+ GetDictValue('LRP_MaterialStatus', mtlStatusId, 'Name'), 'OK', 'OK:{}');

		  break;		  
	}   
}
if(flag == true){
	SaveData(); 
	UpdateView();
}]]>
            </Action>
        </Operation>
        <Operation Key="Edit" Caption="编辑" RefKey="Edit"/>
        <Operation Key="Delete" Caption="删除" RefKey="Delete"/>
        <Operation Key="Cancel" Caption="取消" RefKey="Cancel"/>
        <Operation Key="CopyGridRow" Caption="行复制新增" Visible="!ReadOnly()  &amp;&amp; Status==100">
            <Action>
                <![CDATA[CopyGridRow("Grid1",-1,"Qty","0");]]>
            </Action>
        </Operation>
        <Operation Key="Finish" Caption="完成" Visible="ReadOnly() &amp;&amp; Status==100">
            <Action>
                <![CDATA[Confirm(LocaleString("CommonDef", "key3"),'YES_NO',{YES:{
    Status = 900;
    if(ToString(FinishDateTime) == ''){
        SetValue('FinishDateTime', ServerDate());
    }
    loop "Grid1" (true){			
        WMTx_Status = 90;
    }
    SaveData();
    UpdateView();
    },NO:{}});]]>
            </Action>
            <ExceptionHandler>
                <![CDATA[	
				loop"Grid1"(true)
				{			
					WMTx_Status = 10;
				}			
				Status=StatusValue('prepared');
			]]>
            </ExceptionHandler>
        </Operation>
        <Operation Key="Finish-R" Caption="撤销完成" Visible="ReadOnly() &amp;&amp; Status==900">
            <Action>
                <![CDATA[Confirm(LocaleString("CommonDef", "key4"),'YES_NO',{YES:{
    Status=100;
    if(ToString(FinishDateTime) != ''){
        SetValue('FinishDateTime', '');
    }
    loop "Grid1" (true){			
        WMTx_Status = 10;
    }
    SaveData();
    UpdateView();
    },NO:{}});]]>
            </Action>
            <ExceptionHandler>
                <![CDATA[	
				loop"Grid1"(true)
				{			
					WMTx_Status = 90;
				}			
				Status=StatusValue('finished');
			]]>
            </ExceptionHandler>
        </Operation>
        <Operation Key="Close" Caption="关闭界面" RefKey="Close"/>
    </OperationCollection>
    <QueryCollection>
        <Query Key="SelectMaSta" Description="查询事务类型中的物料状态">
            <Statement>
                <![CDATA[select l.MaterialStatusId from LRP_WhTxType_MS l join LRP_WarehouseTxType h on h.oid=l.soid where  h.code = ?]]>
            </Statement>
        </Query>
