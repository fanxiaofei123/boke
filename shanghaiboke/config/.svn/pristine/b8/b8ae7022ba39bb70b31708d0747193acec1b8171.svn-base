<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Passenger_Meal" Caption="旅客用餐时段定义" FormType="ChainDict">
    <DataSource RefObjectKey="CHCS_Passenger_Meal"/>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Description="载入事件" Range="Action" Verb="Load">
            <![CDATA[LoadData()]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main" Height="100%">
                <SplitPanel Key="SplitPanel1" Orientation="Vertical" Height="100%" Caption="SplitPanel1">
                    <TabPanel Caption="TabPanel0" Key="TabPanel0" Height="pref">
                        <GridLayoutPanel Caption="基本信息" Key="BasicTab" Padding="10px">
                            <TextEditor BuddyKey="L_Code" Caption="代码" Enable="!ReadOnly()" Key="Code" Tip="「代码」不可为空" X="1" XSpan="3" Y="0">
                                <DataBinding ColumnKey="Code" TableKey="CHCS_Passenger_Meal" Required="true"/>
                            </TextEditor>
                            <TextEditor BuddyKey="L_Name" Caption="名称" Enable="!ReadOnly()" Key="Name" Tip="「名称」不可为空" X="6" XSpan="3" Y="0">
                                <DataBinding ColumnKey="Name" TableKey="CHCS_Passenger_Meal" Required="true"/>
                            </TextEditor>
                            <Label Caption="代码" Key="L_Code" X="0" Y="0"/>
                            <Label Caption="名称" Key="L_Name" X="5" Y="0"/>
                            <Dict Key="Belong_Meal" Caption="所属餐别" BuddyKey="Lab_Belong_Meal" X="1" Y="1" ItemKey="LRP_Lookup_L" XSpan="3">
                                <DataBinding ColumnKey="Belong_Meal" TableKey="CHCS_Passenger_Meal"/>
                                <ItemFilter>
                                    <Filter Query="select oid  from LRP_Lookup_L where HeadCode='SSCB'" Type="DataSet"/>
                                </ItemFilter>
                            </Dict>
                            <Label Key="Lab_Belong_Meal" Caption="所属餐别" X="0" Y="1"/>
                            <RowDefCollection RowGap="8">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="8">
                                <ColumnDef Width="94px"/>
                                <ColumnDef Width="25%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="25%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="109px"/>
                                <ColumnDef Width="25%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="25%"/>
                                <ColumnDef Width="55px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="SysInfo" Padding="10px" Caption="系统信息">
                            <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="CREATOR" TableKey="CHCS_Passenger_Meal"/>
                            </Dict>
                            <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                            <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                                <DataBinding ColumnKey="CREATETIME" TableKey="CHCS_Passenger_Meal"/>
                            </DatePicker>
                            <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                            <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="MODIFIER" TableKey="CHCS_Passenger_Meal"/>
                            </Dict>
                            <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                            <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                                <DataBinding ColumnKey="MODIFYTIME" TableKey="CHCS_Passenger_Meal"/>
                            </DatePicker>
                            <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                            <RowDefCollection RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <Grid Key="Grid1" Caption="Grid1" Height="pref">
                        <GridColumnCollection>
                            <GridColumn Key="column1" Caption="选择" Width="80px"/>
                            <GridColumn Key="FoodMatching_Time_Start" Caption="配餐时段(起)" Width="80px"/>
                            <GridColumn Key="FoodMatching_Time_Stop" Caption="配餐时段(至)" Width="80px"/>
                        </GridColumnCollection>
                        <GridRowCollection>
                            <GridRow Key="row1" TableKey="CHCS_Passenger_Meal_L">
                                <GridCell Key="select" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                <GridCell Key="FoodMatching_Time_Start" Caption="配餐时段(起)" CellType="NumberEditor" IntegerValue="true">
                                    <DataBinding ColumnKey="Food_Matching_Time_Start"/>
                                </GridCell>
                                <GridCell Key="FoodMatching_Time_Stop" Caption="配餐时段(至)" CellType="NumberEditor" IntegerValue="true">
                                    <DataBinding ColumnKey="Food_Matching_Time_Stop"/>
                                </GridCell>
                            </GridRow>
                        </GridRowCollection>
                    </Grid>
                    <SplitSize Size="40%"/>
                    <SplitSize Size="60%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
