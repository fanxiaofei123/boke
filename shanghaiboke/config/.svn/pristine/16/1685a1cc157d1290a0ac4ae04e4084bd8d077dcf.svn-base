<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Caption="公司银行账号" FormType="Dict" Key="V_BankAccount_LRP">
    <DataSource RefObjectKey="LRP_BankAccount"/>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Description="载入事件" Range="Action" Verb="Load">
            <![CDATA[LoadData()]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main">
                <TabPanel Caption="TabPanel0" Key="TabPanel0" Height="pref">
                    <GridLayoutPanel Caption="基本信息" Key="BasicTab" Padding="10px">
                        <TextEditor BuddyKey="L_Code" Caption="代码" Enable="!ReadOnly()" Key="Code" Tip="「代码」不可为空" X="1" XSpan="3" Y="0">
                            <DataBinding ColumnKey="Code" TableKey="LRP_BankAccount" Required="true"/>
                        </TextEditor>
                        <TextEditor BuddyKey="L_Name" Caption="名称" Enable="!ReadOnly()" Key="Name" Tip="「名称」不可为空" X="6" XSpan="3" Y="0">
                            <DataBinding ColumnKey="Name" TableKey="LRP_BankAccount" Required="true"/>
                        </TextEditor>
                        <Label Caption="代码" Key="L_Code" X="0" Y="0"/>
                        <Label Caption="名称" Key="L_Name" X="5" Y="0"/>
                        <TextEditor Key="Remark" Caption="说明" BuddyKey="Lab_Remark" X="1" Y="4" XSpan="8">
                            <DataBinding ColumnKey="Remark" TableKey="LRP_BankAccount"/>
                        </TextEditor>
                        <Label Key="Lab_Remark" Caption="说明" X="0" Y="4"/>
                        <Dict Key="CompanyCode" Caption="公司" BuddyKey="Lab_CompanyCode" X="1" Y="1" ItemKey="CompanyCode" XSpan="3">
                            <DataBinding ColumnKey="CompanyCode" TableKey="LRP_BankAccount"/>
                        </Dict>
                        <Label Key="Lab_CompanyCode" Caption="公司" X="0" Y="1"/>
                        <Dict Key="Bank" Caption="开户银行" BuddyKey="Lab_Bank" X="6" Y="1" ItemKey="LRP_Bank" XSpan="3">
                            <DataBinding ColumnKey="Bank" TableKey="LRP_BankAccount" Required="true"/>
                        </Dict>
                        <Label Key="Lab_Bank" Caption="开户银行" X="5" Y="1"/>
                        <ComboBox Key="AccountType" Caption="账户类型" BuddyKey="Lab_AccountType" X="1" Y="2" XSpan="3">
                            <DataBinding ColumnKey="AccountType" TableKey="LRP_BankAccount"/>
                            <Item Caption="一般账户" Key="1" Value="1"/>
                            <Item Caption="专用账户" Key="2" Value="2"/>
                            <Item Caption="基本账户" Key="3" Value="3"/>
                            <Item Caption="临时账户" Key="4" Value="4"/>
                        </ComboBox>
                        <Label Key="Lab_AccountType" Caption="账户类型" X="0" Y="2"/>
                        <TextEditor Key="BankAccountNo" Caption="银行账号" BuddyKey="Lab_BankAccountNo" X="6" Y="2" XSpan="3">
                            <DataBinding ColumnKey="BankAccountNo" TableKey="LRP_BankAccount" Required="true"/>
                        </TextEditor>
                        <Label Key="Lab_BankAccountNo" Caption="银行账号" X="5" Y="2"/>
                        <Dict Key="AccountCurrency" Caption="账户币种" BuddyKey="Lab_AccountCurrency" X="1" Y="3" ItemKey="Currency" XSpan="3">
                            <DataBinding ColumnKey="AccountCurrency" TableKey="LRP_BankAccount"/>
                        </Dict>
                        <Label Key="Lab_AccountCurrency" Caption="账户币种" X="0" Y="3"/>
                        <DatePicker Key="AccountDate" Caption="开户日期" BuddyKey="Lab_AccountDate" X="6" Y="3" XSpan="3">
                            <DataBinding ColumnKey="AccountDate" TableKey="LRP_BankAccount"/>
                        </DatePicker>
                        <Label Key="Lab_AccountDate" Caption="开户日期" X="5" Y="3"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="系统信息" Key="SystemTab" Padding="10px">
                        <Dict Caption="创建人" ItemKey="Operator" Key="CREATOR" X="1" Y="0">
                            <DataBinding ColumnKey="CREATOR" DefaultFormulaValue="GetOperator()" TableKey="LRP_BankAccount"/>
                        </Dict>
                        <DatePicker Caption="创建时间" Key="CREATETIME" X="1" Y="1">
                            <DataBinding ColumnKey="CREATETIME" DefaultFormulaValue="ServerDate()" TableKey="LRP_BankAccount"/>
                        </DatePicker>
                        <Dict Caption="修改人" ItemKey="Operator" Key="MODIFIER" X="1" Y="2">
                            <DataBinding ColumnKey="MODIFIER" DefaultFormulaValue="GetOperator()" TableKey="LRP_BankAccount"/>
                        </Dict>
                        <DatePicker Caption="修改时间" Key="MODIFYTIME" X="1" Y="3">
                            <DataBinding ColumnKey="MODIFYTIME" DefaultFormulaValue="ServerDate()" TableKey="LRP_BankAccount"/>
                        </DatePicker>
                        <Label Caption="创建人" Key="L_CREATOR" X="0" Y="0"/>
                        <Label Caption="创建时间" Key="L_CREATETIME" X="0" Y="1"/>
                        <Label Caption="修改人" Key="L_MODIFIER" X="0" Y="2"/>
                        <Label Caption="修改时间" Key="L_MODIFYTIME" X="0" Y="3"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="100%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
