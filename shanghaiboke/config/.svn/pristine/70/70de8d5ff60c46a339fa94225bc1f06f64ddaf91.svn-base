<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Aircraft" Caption="飞机信息" FormType="ChainDict">
    <DataSource RefObjectKey="CHCS_Aircraft"/>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Description="载入事件" Range="Action" Verb="Load">
            <![CDATA[LoadData()]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main" Height="100%">
                <TabPanel Caption="TabPanel0" Key="TabPanel0" Height="pref">
                    <GridLayoutPanel Caption="基本信息" Key="BasicTab" Padding="10px">
                        <TextEditor BuddyKey="L_Code" Caption="代码" Enable="!ReadOnly()" Key="Code" Tip="「代码」不可为空" X="1" XSpan="3" Y="0">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Code" Required="true"/>
                        </TextEditor>
                        <TextEditor BuddyKey="L_Name" Caption="机号" Enable="!ReadOnly()" Key="Name" Tip="「名称」不可为空" X="6" XSpan="3" Y="0">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Name" Required="true"/>
                        </TextEditor>
                        <Label Caption="代码" Key="L_Code" X="0" Y="0"/>
                        <Label Caption="机号" Key="L_Name" X="5" Y="0"/>
                        <TextEditor Key="Remark" Caption="说明" BuddyKey="Lab_Remark" X="1" Y="6" XSpan="8">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Remark"/>
                        </TextEditor>
                        <Label Key="Lab_Remark" Caption="说明" X="0" Y="6"/>
                        <TextEditor Key="Short_Type" Caption="短机型" BuddyKey="Lab_Short_Type" X="1" Y="1" XSpan="3">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Short_Type"/>
                        </TextEditor>
                        <Label Key="Lab_Short_Type" Caption="短机型" X="0" Y="1"/>
                        <TextEditor Key="Long_Type" Caption="长机型" BuddyKey="Lab_Long_Type" X="6" Y="1" XSpan="3">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Long_Type"/>
                        </TextEditor>
                        <Label Key="Lab_Long_Type" Caption="长机型" X="5" Y="1"/>
                        <TextEditor Key="Fairway_Type" Caption="航路机型" BuddyKey="Lab_Fairway_Type" X="1" Y="2" XSpan="3">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Fairway_Type"/>
                        </TextEditor>
                        <Label Key="Lab_Fairway_Type" Caption="航路机型" X="0" Y="2"/>
                        <NumberEditor Key="Aircraft_Maximum_Number_Of_Seats" Caption="最大座位数" BuddyKey="Lab_Aircraft_Maximum_Number_Of_Seats" X="6" Y="2" Format="" XSpan="3" IntegerValue="true" Scale="0">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Aircraft_Maximum_Number_Of_Seats"/>
                        </NumberEditor>
                        <Label Key="Lab_Aircraft_Maximum_Number_Of_Seats" Caption="最大座位数" X="5" Y="2"/>
                        <NumberEditor Key="Aircraft_First_Class_Seats" Caption="头等仓数" BuddyKey="Lab_Aircraft_First_Class_Seats" X="1" Y="3" Format="" XSpan="3" IntegerValue="true" Scale="0">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Aircraft_First_Class_Seats"/>
                        </NumberEditor>
                        <Label Key="Lab_Aircraft_First_Class_Seats" Caption="头等仓数" X="0" Y="3"/>
                        <NumberEditor Key="Aircraft_Business_Class_Seats" Caption="公务仓数" BuddyKey="Lab_Aircraft_Business_Class_Seats" X="6" Y="3" Format="" XSpan="3" IntegerValue="true" Scale="0">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Aircraft_Business_Class_Seats"/>
                        </NumberEditor>
                        <Label Key="Lab_Aircraft_Business_Class_Seats" Caption="公务仓数" X="5" Y="3"/>
                        <NumberEditor Key="Number_Of_Kitchens" Caption="厨房数量" BuddyKey="Lab_Number_Of_Kitchens" X="1" Y="5" Format="" XSpan="3" IntegerValue="true" Scale="0">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Number_Of_Kitchens"/>
                        </NumberEditor>
                        <Label Key="Lab_Number_Of_Kitchens" Caption="厨房数量" X="0" Y="5"/>
                        <TextEditor Key="Classify" Caption="分类" BuddyKey="Lab_Classify" X="6" Y="4" XSpan="3">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Classify"/>
                        </TextEditor>
                        <Label Key="Lab_Classify" Caption="分类" X="5" Y="4"/>
                        <NumberEditor Key="Aircraft_Economy_Class_Seats" Caption="经济仓数" BuddyKey="Lab_Aircraft_Economy_Class_Seats" X="1" Y="4" Format="" XSpan="3" IntegerValue="true" Scale="0">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Aircraft_Economy_Class_Seats"/>
                        </NumberEditor>
                        <Label Key="Lab_Aircraft_Economy_Class_Seats" Caption="经济仓数" X="0" Y="4"/>
                        <ComboBox Key="Is_Panda" Caption="是否熊猫号" BuddyKey="Lab_Is_Panda" X="6" Y="5" IntegerValue="true" XSpan="3">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="Is_Panda"/>
                            <Item Caption="是" Key="0" Value="0"/>
                            <Item Caption="否" Key="1" Value="1"/>
                        </ComboBox>
                        <Label Key="Lab_Is_Panda" Caption="是否熊猫号" X="5" Y="5"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="78px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="94px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Key="SysInfo" Padding="10px" Caption="系统信息">
                        <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="CREATOR"/>
                        </Dict>
                        <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                        <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="CREATETIME"/>
                        </DatePicker>
                        <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                        <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="MODIFIER"/>
                        </Dict>
                        <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                        <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                            <DataBinding TableKey="CHCS_Aircraft" ColumnKey="MODIFYTIME"/>
                        </DatePicker>
                        <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                        <RowDefCollection RowGap="5">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="5">
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
