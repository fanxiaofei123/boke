<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Transfer_HandoverView" Caption="调拨交接单视图" FormType="View" AbbrCaption="调拨交接单视图">
    <DataSource>
        <DataObject Key="CHCS_Transfer_Handover" Caption="调拨交接单视图" PrimaryTableKey="CHCS_Transfer_Handover_H">
            <TableCollection>
                <Table Key="CHCS_Transfer_Handover_H" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="NO" Caption="交接单号" DataType="Varchar"/>
                    <Column Key="Status" Caption="状态" DataType="Integer"/>
                    <Column Key="FlightDate" Caption="航班日期" DataType="Date"/>
                    <Column Key="Flight_Number" Caption="航班号" DataType="Long"/>
                    <Column Key="Aircraft_Number" Caption="飞机号" DataType="Long"/>
                    <Column Key="Aircraft_Type_Number" Caption="机型" DataType="Long"/>
                    <Column Key="In_Warehouse" Caption="调入仓库" DataType="Long"/>
                    <Column Key="Dispatch_Order_No" Caption="调拨单编号" DataType="Varchar"/>
                    <Column Key="Out_Warehouse" Caption="调出仓库" DataType="Long"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="New" Caption="新增" Icon="new.png">
            <Action>
                <![CDATA[New('CHCS_Transfer_Handover')]]>
            </Action>
        </Operation>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="L_NO" Caption="交接单号" X="0" Y="0"/>
                            <TextEditor Key="NO" Caption="交接单号" BuddyKey="L_NO" X="1" Y="0" XSpan="3">
                                <Condition ColumnKey="NO" TableKey="CHCS_Transfer_Handover_H" CondSign="like"/>
                            </TextEditor>
                            <Label Key="L_Status" Caption="状态" X="8" Y="0"/>
                            <ComboBox Key="Status" Caption="状态" BuddyKey="L_Status" X="9" Y="0" SourceType="Status">
                                <Condition ColumnKey="Status" TableKey="CHCS_Transfer_Handover_H" CondSign="="/>
                            </ComboBox>
                            <Label Key="L_FlightDate" Caption="航班日期" X="0" Y="1"/>
                            <DatePicker Key="FlightDate" Caption="航班日期" BuddyKey="L_FlightDate" X="1" Y="1">
                                <Format/>
                                <Condition ColumnKey="FlightDate" TableKey="CHCS_Transfer_Handover_H" CondSign="between" Group="FlightDate" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="L_FlightDate_Comp2" Caption="到" X="2" Y="1"/>
                            <DatePicker Key="FlightDate_Comp2" Caption="到" BuddyKey="L_FlightDate_Comp2" X="3" Y="1">
                                <Format/>
                                <Condition ColumnKey="FlightDate" TableKey="CHCS_Transfer_Handover_H" CondSign="between" Group="FlightDate" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="L_Aircraft_Number" Caption="飞机号" X="8" Y="1"/>
                            <Dict Key="Aircraft_Number" Caption="飞机号" BuddyKey="L_Aircraft_Number" X="9" Y="1" ItemKey="CHCS_Aircraft">
                                <Condition ColumnKey="Aircraft_Number" TableKey="CHCS_Transfer_Handover_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_Aircraft_Type_Number" Caption="机型" X="5" Y="1"/>
                            <Dict Key="Aircraft_Type_Number" Caption="机型" BuddyKey="L_Aircraft_Type_Number" X="6" Y="1" ItemKey="CHCS_Aircraft_Type">
                                <Condition ColumnKey="Aircraft_Type_Number" TableKey="CHCS_Transfer_Handover_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_In_Warehouse" Caption="调入仓库" X="0" Y="2"/>
                            <ComboBox Key="In_Warehouse" Caption="调入仓库" BuddyKey="L_In_Warehouse" X="1" Y="2" XSpan="3">
                                <Condition ColumnKey="In_Warehouse" TableKey="CHCS_Transfer_Handover_H" CondSign="="/>
                            </ComboBox>
                            <Label Key="L_Dispatch_Order_No" Caption="调拨单编号" X="4" Y="2"/>
                            <TextEditor Key="Dispatch_Order_No" Caption="调拨单编号" BuddyKey="L_Dispatch_Order_No" X="5" Y="2" XSpan="2">
                                <Condition ColumnKey="Dispatch_Order_No" TableKey="CHCS_Transfer_Handover_H" CondSign="like"/>
                            </TextEditor>
                            <Label Key="L_Out_Warehouse" Caption="调出仓库" X="8" Y="2"/>
                            <ComboBox Key="Out_Warehouse" Caption="调出仓库" BuddyKey="L_Out_Warehouse" X="9" Y="2">
                                <Condition ColumnKey="Out_Warehouse" TableKey="CHCS_Transfer_Handover_H" CondSign="="/>
                            </ComboBox>
                            <Button Key="Query" Caption="查询" X="11" Y="0" Enable="true" XSpan="2">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="11" Y="1" Enable="true" XSpan="2">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <Dict Key="Flight_Number" Caption="航班号" BuddyKey="Lab_Flight_Number" X="6" Y="0" ItemKey="CHCS_Flight">
                                <Condition ColumnKey="Flight_Number" TableKey="CHCS_Transfer_Handover_H" CondSign="="/>
                            </Dict>
                            <Label Key="Lab_Flight_Number" Caption="航班号" X="5" Y="0"/>
                            <RowDefCollection RowHeight="25" RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="90px"/>
                                <ColumnDef Width="16%"/>
                                <ColumnDef Width="15px"/>
                                <ColumnDef Width="16%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="50px"/>
                                <ColumnDef Width="4%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="list" PageLoadType="DB" TableKey="CHCS_Transfer_Handover_H">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="CHCS_Transfer_Handover_H" Visible="false" Width="30px"/>
                            <ListViewColumn Key="NO_LV" Caption="交接单号" ColumnType="TextEditor" DataColumnKey="NO"/>
                            <ListViewColumn Key="Status_LV" Caption="状态" ColumnType="ComboBox" DataColumnKey="Status" SourceType="Status"/>
                            <ListViewColumn Key="FlightDate_LV" Caption="航班日期" ColumnType="DatePicker" DataColumnKey="FlightDate" Width="150px"/>
                            <ListViewColumn Key="Flight_Number_LV" Caption="航班号" ColumnType="TextEditor" DataColumnKey="Flight_Number" Width="120px"/>
                            <ListViewColumn Key="Aircraft_Number_LV" Caption="飞机号" ColumnType="Dict" DataColumnKey="Aircraft_Number" Width="120px" ItemKey="CHCS_Aircraft"/>
                            <ListViewColumn Key="Aircraft_Type_Number_LV" Caption="机型" ColumnType="Dict" DataColumnKey="Aircraft_Type_Number" ItemKey="CHCS_Aircraft_Type"/>
                            <ListViewColumn Key="In_Warehouse_LV" Caption="调入仓库" ColumnType="ComboBox" DataColumnKey="In_Warehouse" Width="120px"/>
                            <ListViewColumn Key="Dispatch_Order_No_LV" Caption="调拨单编号" ColumnType="TextEditor" DataColumnKey="Dispatch_Order_No"/>
                            <ListViewColumn Key="Out_Warehouse_LV" Caption="调出仓库" ColumnType="ComboBox" DataColumnKey="Out_Warehouse" Width="120px"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('CHCS_Transfer_Handover', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="35%"/>
                    <SplitSize Size="65%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
