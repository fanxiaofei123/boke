<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<DataObject Caption="应付引入付款单" Key="LRP_QuotePayment" PrimaryTableKey="LRP_QuotePayment">
    <TableCollection>
        <Table Caption="应付引入付款单" Key="LRP_QuotePayment" SourceType="Query" TableMode="Detail">
            <Column Key="OID" Caption="收款单OID" DataType="Long"/>
            <Column Key="SOID" Caption="收款单SOID" DataType="Long"/>
            <Column Key="No" Caption="单号" DataType="Varchar" Length="200"/>
            <Column Key="CompanyCode" Caption="公司" DataType="Long"/>
            <Column Key="SettlementId" Caption="结算方" DataType="Long"/>
            <Column Caption="付款日期" DataType="DateTime" Key="PaymentDate"/>
            <Column Caption="付款金额" DataType="Numeric" Key="PaymentMoney" Precision="18" Scale="6"/>
            <Column Key="CurrencyID" Caption="交易货币" DataType="Long"/>
            <Column Key="MapCount" Caption="映射数量标记" DataType="Integer"/>
            <Statement>
                <![CDATA[SELECT *
FROM
  (SELECT h.OID,
  h.SOID,
  h.No,
  h.CompanyCode,
  h.SettlementId,
  h.PaymentDate,
  h.PaymentType,
  h.CurrencyID,
  h.PaymentMoney
FROM LRP_Payment h
LEFT JOIN LRP_Payment_Show s
on h.OID = s.SrcOID
WHERE h.STATUS = 200 and h.PaymentType='803' and MapKey IS NULL)a ]]>
            </Statement>
        </Table>
    </TableCollection>
</DataObject>
