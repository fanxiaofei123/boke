    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板">
                    <TabPanel Caption="TabPanel1" Height="pref" Key="TabPanel1">
                        <GridLayoutPanel Key="first_head" Height="pref" Padding="5px" Caption="基本信息">
                            <Label Key="Lab_NO" Caption="单据编号" X="0" Y="0"/>
                            <TextEditor Key="NO" Caption="单据编号" BuddyKey="Lab_NO" X="1" Y="0" Enable="false" AsQuery="true">
                                <DataBinding ColumnKey="No" TableKey="CHCS_Meals_Provide_Rule_Set_H"/>
                            </TextEditor>
                            <Label Key="Lab_BillDate" Caption="单据日期" X="2" Y="0"/>
                            <DatePicker Key="BillDate" Caption="单据日期" BuddyKey="Lab_BillDate" X="3" Y="0" Enable="false" AsQuery="true">
                                <DataBinding ColumnKey="BillDate" TableKey="CHCS_Meals_Provide_Rule_Set_H"/>
                                <Format/>
                            </DatePicker>
                            <Label Key="Lab_Status" Caption="单据状态" X="4" Y="0"/>
                            <ComboBox Key="Status" Caption="单据状态" BuddyKey="Lab_Status" X="5" Y="0" SourceType="Status" Enable="false" AsQuery="true">
                                <DataBinding ColumnKey="Status" TableKey="CHCS_Meals_Provide_Rule_Set_H" DefaultValue="100"/>
                            </ComboBox>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="34%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="second_head" Height="pref" Padding="5px" Caption="审批信息">
                            <Dict Key="Creator" Caption="制单人" BuddyKey="Lab_Creator" X="1" Y="0" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="CREATOR" TableKey="CHCS_Meals_Provide_Rule_Set_H"/>
                            </Dict>
                            <Label Key="Lab_Creator" Caption="制单人" X="0" Y="0"/>
                            <DatePicker Key="CreateTime" Caption="制单时间" BuddyKey="Lab_CreateTime" X="3" Y="0" Enable="false">
                                <DataBinding ColumnKey="CREATETIME" TableKey="CHCS_Meals_Provide_Rule_Set_H"/>
                            </DatePicker>
                            <Label Key="Lab_CreateTime" Caption="制单时间" X="2" Y="0"/>
                            <Dict Key="Modifier" Caption="修改人" BuddyKey="Lab_Modifier" X="1" Y="1" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="MODIFIER" TableKey="CHCS_Meals_Provide_Rule_Set_H"/>
                            </Dict>
                            <Label Key="Lab_Modifier" Caption="修改人" X="0" Y="1"/>
                            <DatePicker Key="ModifyTime" Caption="修改时间" BuddyKey="Lab_ModifyTime" X="3" Y="1" Enable="false">
                                <DataBinding ColumnKey="MODIFYTIME" TableKey="CHCS_Meals_Provide_Rule_Set_H"/>
                            </DatePicker>
                            <Label Key="Lab_ModifyTime" Caption="修改时间" X="2" Y="1"/>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <Grid Key="Grid2" Caption="Grid2">
                        <GridColumnCollection>
                            <GridColumn Key="column1" Caption="选择" Width="80px" Sortable="true"/>
                            <GridColumn Key="Time_Interval" Caption="时段要求" Width="80px" Sortable="true"/>
                            <GridColumn Key="column2" Caption="飞行时长" Width="160px">
                                <GridColumnCollection>
                                    <GridColumn Key="Flight_Time_From" Caption="时长（大于）" Width="80px"/>
                                    <GridColumn Key="Flight_Time_To" Caption="时长（小于）" Width="80px"/>
                                </GridColumnCollection>
                            </GridColumn>
                            <GridColumn Key="column5" Caption="起飞时刻要求" Width="160px">
                                <GridColumnCollection>
                                    <GridColumn Key="Takeoff_Time_From" Caption="起" Width="80px"/>
                                    <GridColumn Key="Takeoff_Time_To" Caption="止" Width="80px"/>
                                </GridColumnCollection>
                            </GridColumn>
                            <GridColumn Key="column8" Caption="落地时刻要求" Width="160px">
                                <GridColumnCollection>
                                    <GridColumn Key="Landing_Time_From" Caption="起" Width="80px"/>
                                    <GridColumn Key="Landing_Time_To" Caption="止" Width="80px"/>
                                </GridColumnCollection>
                            </GridColumn>
                            <GridColumn Key="Class_of_Cabin" Caption="舱位等级" Width="80px" Sortable="true"/>
                            <GridColumn Key="Service_Standard" Caption="服务标准" Width="80px" Sortable="true"/>
                            <GridColumn Key="Economy_Match" Caption="经济舱配餐" Width="80px" Sortable="true"/>
                            <GridColumn Key="Extra_Economy" Caption="超经济舱配餐" Width="80px"/>
                            <GridColumn Key="Business_Class_Catering" Caption="公务舱配餐" Width="80px"/>
                            <GridColumn Key="Meal_Kind" Caption="餐别" Width="80px" Sortable="true"/>
                            <GridColumn Key="Service_Object" Caption="服务对象" Width="80px" Sortable="true"/>
                        </GridColumnCollection>
                        <GridRowCollection>
                            <GridRow Key="row1" TableKey="CHCS_Meals_Provide_Rule_Set_L" RowHeight="35">
                                <GridCell Key="select" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                <GridCell Key="Time_Interval" Caption="时段要求" CellType="Dict" ItemKey="LRP_Lookup_L">
                                    <DataBinding ColumnKey="Time_Interval"/>
                                    <ItemFilter>
                                        <Filter Query="select l.oid from lrp_lookup_L l left join lrp_lookup_h h on h.oid = l.soid where upper(h.code) = upper(?)" Type="DataSet">
                                            <FilterValue DataType="Varchar" ParaValue="SDYQ" Type="Const"/>
                                        </Filter>
                                    </ItemFilter>
                                </GridCell>
                                <GridCell Key="Flight_Time_From" Caption="时长（大于）" CellType="NumberEditor">
                                    <DataBinding ColumnKey="Flight_Time_From"/>
                                </GridCell>
                                <GridCell Key="Flight_Time_To" Caption="时长（小于）" CellType="NumberEditor">
                                    <DataBinding ColumnKey="Flight_Time_To"/>
                                </GridCell>
                                <GridCell Key="Takeoff_Time_From" Caption="起" CellType="NumberEditor">
                                    <DataBinding ColumnKey="Takeoff_Time_From"/>
                                </GridCell>
                                <GridCell Key="Takeoff_Time_To" Caption="止" CellType="NumberEditor">
                                    <DataBinding ColumnKey="Takeoff_Time_To"/>
                                </GridCell>
                                <GridCell Key="Landing_Time_From" Caption="起" CellType="NumberEditor">
                                    <DataBinding ColumnKey="Landing_Time_From"/>
                                </GridCell>
                                <GridCell Key="Landing_Time_To" Caption="止" CellType="NumberEditor">
                                    <DataBinding ColumnKey="Landing_Time_To"/>
                                </GridCell>
                                <GridCell Key="Class_of_Cabin" Caption="舱位等级" CellType="Dict" ItemKey="LRP_Lookup_L">
                                    <DataBinding ColumnKey="Class_of_Cabin"/>
                                    <ItemFilter>
                                        <Filter Query="select l.oid from lrp_lookup_L l left join lrp_lookup_h h on h.oid = l.soid where upper(h.code) = upper(?)" Type="DataSet">
                                            <FilterValue DataType="Varchar" ParaValue="CWDJ" Type="Const"/>
                                        </Filter>
                                    </ItemFilter>
                                </GridCell>
                                <GridCell Key="Service_Standard" Caption="服务标准" CellType="Dict" ItemKey="LRP_Lookup_L">
                                    <DataBinding ColumnKey="Service_Standard"/>
                                    <ItemFilter>
                                        <Filter Query="select l.oid from lrp_lookup_L l left join lrp_lookup_h h on h.oid = l.soid where upper(h.code) = upper(?)" Type="DataSet">
                                            <FilterValue DataType="Varchar" ParaValue="HBFWBZ" Type="Const"/>
                                        </Filter>
                                    </ItemFilter>
                                </GridCell>
                                <GridCell Key="Economy_Match" Caption="经济舱配餐" CellType="TextEditor">
                                    <DataBinding ColumnKey="Economy_Match"/>
                                </GridCell>
                                <GridCell Key="Extra_Economy" Caption="超经济舱配餐" CellType="TextEditor">
                                    <DataBinding ColumnKey="Extra_Economy"/>
                                </GridCell>
                                <GridCell Key="Business_Class_Catering" Caption="公务舱配餐" CellType="TextEditor">
                                    <DataBinding ColumnKey="Business_Class_Catering"/>
                                </GridCell>
                                <GridCell Key="Meal_Kind" Caption="餐别" CellType="Dict" ItemKey="LRP_Lookup_L">
                                    <DataBinding ColumnKey="Meal_Kind"/>
                                    <ItemFilter>
                                        <Filter Query="select l.oid from lrp_lookup_L l left join lrp_lookup_h h on h.oid = l.soid where upper(h.code) = upper(?)" Type="DataSet">
                                            <FilterValue DataType="Varchar" ParaValue="SSCB" Type="Const"/>
                                        </Filter>
                                    </ItemFilter>
                                </GridCell>
                                <GridCell Key="Service_Object" Caption="服务对象" CellType="Dict" ItemKey="LRP_Lookup_L">
                                    <DataBinding ColumnKey="Service_Object"/>
                                    <ItemFilter>
                                        <Filter Query="select l.oid from lrp_lookup_L l left join lrp_lookup_h h on h.oid = l.soid where upper(h.code) = upper(?)" Type="DataSet">
                                            <FilterValue DataType="Varchar" ParaValue="FWDX" Type="Const"/>
                                        </Filter>
                                    </ItemFilter>
                                </GridCell>
                            </GridRow>
                        </GridRowCollection>
                    </Grid>
                    <SubDetail BindingGridKey="Grid2" Caption="SubDetail1" Key="SubDetail1">
                        <Grid Key="Grid3" Caption="Grid3">
                            <GridColumnCollection>
                                <GridColumn Key="select_T" Caption="选择" Width="80px"/>
                                <GridColumn Key="Time_Start" Caption="开始时间" Width="80px"/>
                                <GridColumn Key="Time_End" Caption="结束时间" Width="80px"/>
                            </GridColumnCollection>
                            <GridRowCollection>
                                <GridRow Key="row1" TableKey="CHCS_Meals_Provide_Rule_Set_T">
                                    <GridCell Key="select_T" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                    <GridCell Key="Time_Start" Caption="开始时间" CellType="NumberEditor">
                                        <DataBinding ColumnKey="Time_Start"/>
                                    </GridCell>
                                    <GridCell Key="Time_End" Caption="结束时间" CellType="NumberEditor">
                                        <DataBinding ColumnKey="Time_End"/>
                                    </GridCell>
                                </GridRow>
                            </GridRowCollection>
                        </Grid>
                    </SubDetail>
                    <SplitSize Size="30%"/>
                    <SplitSize Size="70%"/>
                    <SplitSize Size="100px"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
