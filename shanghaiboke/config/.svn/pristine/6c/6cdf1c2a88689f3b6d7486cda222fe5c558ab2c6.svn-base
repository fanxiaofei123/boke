<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Replenishment_HView" Caption="补货申请单视图" FormType="View" AbbrCaption="补货申请单视图">
    <DataSource>
        <DataObject Key="CHCS_Replenishment_H" Caption="补货申请单视图" PrimaryTableKey="CHCS_Replenishment_H">
            <TableCollection>
                <Table Key="CHCS_Replenishment_H" DBTableName="CHCS_Replenishment_H" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="NO" Caption="单据编号" DataType="Varchar"/>
                    <Column Key="BillDate" Caption="申请日期" DataType="DateTime"/>
                    <Column Key="Status" Caption="申请单状态" DataType="Varchar"/>
                    <Column Key="Vendor" Caption="航食公司" DataType="Long"/>
                    <Column Key="Approval_Date" Caption="审批日期" DataType="DateTime"/>
                    <Column Key="Applicant" Caption="申请人" DataType="Long"/>
                    <Column Key="Approver" Caption="审批人" DataType="Long"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="New" Caption="新增" Icon="new.png">
            <Action>
                <![CDATA[New('CHCS_Replenishment_H')]]>
            </Action>
        </Operation>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="L_NO" Caption="补货申请单号" X="6" Y="0"/>
                            <TextEditor Key="NO" Caption="补货申请单号" BuddyKey="L_NO" X="7" Y="0">
                                <Condition ColumnKey="NO" TableKey="CHCS_Replenishment_H" CondSign="like"/>
                            </TextEditor>
                            <DatePicker Key="BillDate_From" Caption="申请日期 从" BuddyKey="Lab_BillDate_From" X="1" Y="0" OnlyDate="true">
                                <Condition ColumnKey="BillDate" TableKey="CHCS_Replenishment_H" CondSign="between" Group="BillDate" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="Lab_BillDate_From" Caption="申请日期 从" X="0" Y="0"/>
                            <DatePicker Key="BillDate_To" Caption="申请日期 止" BuddyKey="Lab_BillDate_To" X="4" Y="0" OnlyDate="true">
                                <Condition ColumnKey="BillDate" TableKey="CHCS_Replenishment_H" CondSign="between" Group="BillDate" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="Lab_BillDate_To" Caption="申请日期 止" X="3" Y="0"/>
                            <Button Key="Query" Caption="查询" X="9" Y="0" Enable="true">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="9" Y="1" Enable="true">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <ComboBox Key="Status" Caption="申请单状态" BuddyKey="Lab_Status" X="1" Y="1" SourceType="Status">
                                <Condition ColumnKey="Status" TableKey="CHCS_Replenishment_H" CondSign="like"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="申请单状态" X="0" Y="1"/>
                            <Dict Key="Vendor" Caption="航食公司" BuddyKey="Lab_Vendor" X="4" Y="1" ItemKey="CompanyCode">
                                <Condition ColumnKey="Vendor" TableKey="CHCS_Replenishment_H" CondSign="like"/>
                            </Dict>
                            <Label Key="Lab_Vendor" Caption="航食公司" X="3" Y="1"/>
                            <DatePicker Key="Approval_Date_From" Caption="审批日期 从" BuddyKey="Lab_Approval_Date_From" X="1" Y="2">
                                <Condition ColumnKey="Approval_Date" TableKey="CHCS_Replenishment_H" CondSign="between" Group="Approval_Date" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="Lab_Approval_Date_From" Caption="审批日期 从" X="0" Y="2"/>
                            <DatePicker Key="Approval_Date_To" Caption="审批日期 止" BuddyKey="Lab_Approval_Date_To" X="4" Y="2">
                                <Condition ColumnKey="Approval_Date" TableKey="CHCS_Replenishment_H" CondSign="between" Group="Approval_Date" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="Lab_Approval_Date_To" Caption="审批日期 止" X="3" Y="2"/>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="82px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="80px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="list" PageLoadType="DB" TableKey="CHCS_Replenishment_H">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="CHCS_Replenishment_H" Visible="false" Width="111px"/>
                            <ListViewColumn Key="NO_LV" Caption="补货申请单号" ColumnType="TextEditor" DataColumnKey="NO" Width="137px"/>
                            <ListViewColumn Key="BillDate" Caption="申请日期" ColumnType="DatePicker" DataColumnKey="BillDate"/>
                            <ListViewColumn Key="Status_L" Caption="申请单状态" ColumnType="ComboBox" DataColumnKey="Status" SourceType="Status"/>
                            <ListViewColumn Key="Vendor_L" Caption="航食公司" ColumnType="Dict" DataColumnKey="Vendor" ItemKey="CompanyCode"/>
                            <ListViewColumn Key="Applicant" Caption="申请人" ColumnType="Dict" DataColumnKey="Applicant" ItemKey="CHCS_Employee"/>
                            <ListViewColumn Key="Approver" Caption="审批人" ColumnType="Dict" DataColumnKey="Approver" ItemKey="CHCS_Employee"/>
                            <ListViewColumn Key="Approval_Date" Caption="审批日期" ColumnType="DatePicker" DataColumnKey="Approval_Date"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('CHCS_Replenishment_H', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="30%"/>
                    <SplitSize Size="70%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
