<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form AbbrCaption="用户优惠活动日志" Caption="用户优惠活动日志" FormType="View" Key="V_UserPromotionsLogView">
    <DataSource>
        <DataObject Caption="用户优惠活动日志" Key="V_UserPromotionsLogView" PrimaryTableKey="LRP_UserPromotionsLog">
            <TableCollection>
                <Table Key="LRP_UserPromotionsLog" Persist="false" SourceType="Query" TableMode="Detail">
                    <Column Caption="对象标识" DataType="Long" Key="OID"/>
                    <Column Caption="主对象标识" DataType="Long" Key="SOID"/>
                    <Column Caption="父对象标识" DataType="Long" Key="POID"/>
                    <Column Caption="对象版本" DataType="Integer" Key="VERID"/>
                    <Column Caption="对象明细版本" DataType="Integer" Key="DVERID"/>
                    <Column Caption="状态" DataType="Integer" Key="Status"/>
                    <Column Caption="创建人" DataType="Long" Key="CREATOR"/>
                    <Column Caption="创建时间" DataType="DateTime" Key="CREATETIME"/>
                    <Column Caption="修改人" DataType="Long" Key="MODIFIER"/>
                    <Column Caption="修改时间" DataType="DateTime" Key="MODIFYTIME"/>
                    <Column Caption="货主用户id" DataType="Long" Key="OwnerUserId"/>
                    <Column Caption="优惠活动id" DataType="Long" Key="PromotionId"/>
                    <Column Caption="领取日期时间" DataType="DateTime" Key="ReceiveTime"/>
                    <Column Caption="目标货主用户id" DataType="Long" Key="DestOwnerUserId"/>
                    <TableFilter>
                        <![CDATA[]]>
                    </TableFilter>
                </Table>
            </TableCollection>
            <OIDFilter/>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Caption="关闭界面" Key="Close" RefKey="Close"/>
        <Operation Caption="TEST" Key="TEST" Visible="false">
            <Action>
                <![CDATA[
					InvokeService("LRP_GenPromotionsRecord", false, false,2872596,1812754,1814038);	
				]]>
            </Action>
        </Operation>
    </OperationCollection>
    <QueryCollection>
        <Query Description="读取券" Key="GetPromotionId">
            <Statement>
                <![CDATA[select OID as N_KEY,Name as N_NAME from LRP_Promotions_H where status=200]]>
            </Statement>
        </Query>
    </QueryCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main">
                <ToolBar Height="pref" Key="main_toolbar">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Height="100%" Key="main_split" Orientation="Vertical">
                    <GridLayoutPanel Caption="GridLayoutPanel0" Key="GridLayoutPanel0" Padding="10px">
                        <Dict Caption="货主用户" ItemKey="LRP_OwnerUser" Key="OwnerUserId_C" X="1" XSpan="2" Y="0">
                            <Condition CondSign="=">
                                <ConditionTarget TableKey="LRP_UserPromotionsLog" ColumnKey="OwnerUserId"/>
                            </Condition>
                        </Dict>
                        <Label Caption="货主用户" Key="L_OwnerUserId" X="0" Y="0"/>
                        <CheckListBox Caption="活动名称" Key="PromotionId_C" SourceType="Formula" X="5" XSpan="2" Y="0">
                            <Condition CondSign="in">
                                <ConditionTarget TableKey="LRP_UserPromotionsLog" ColumnKey="PromotionId"/>
                            </Condition>
                            <FormulaItems>
                                <![CDATA[DBNamedQuery('GetPromotionId');]]>
                            </FormulaItems>
                        </CheckListBox>
                        <Label Caption="活动名称" Key="L_PromotionId" X="4" Y="0"/>
                        <Button Caption="查询" Key="Query" X="8" Y="0">
                            <OnClick>
                                <![CDATA[DealCondition();LoadData();ShowData();]]>
                            </OnClick>
                        </Button>
                        <Button Caption="重置" Key="Reset" X="8" Y="1">
                            <OnClick>
                                <![CDATA[ResetCondition();]]>
                            </OnClick>
                        </Button>
                        <DatePicker Caption="日期时间范围" Key="ReceiveTime_C" X="1" XSpan="2" Y="1">
                            <Condition CondSign="between" Group="ReceiveTime" GroupHead="true">
                                <ConditionTarget TableKey="LRP_UserPromotionsLog" ColumnKey="ReceiveTime"/>
                            </Condition>
                        </DatePicker>
                        <Label Caption="日期时间范围" Key="L_ReceiveTime" X="0" Y="1"/>
                        <DatePicker Caption="至" Key="ReceiveTime_C_1" X="5" XSpan="2" Y="1">
                            <Condition CondSign="none" Group="ReceiveTime" GroupTail="true">
                                <ConditionTarget TableKey="LRP_UserPromotionsLog" ColumnKey="ReceiveTime"/>
                            </Condition>
                        </DatePicker>
                        <Label Caption="至" Key="L_ReceiveTime_1" X="4" Y="1"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="77px"/>
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%" MinWidth="50px"/>
                            <ColumnDef Width="30px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%" MinWidth="50px"/>
                            <ColumnDef Width="30px"/>
                            <ColumnDef Width="80px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <ListView Caption="ListView0" Key="ListView0" TableKey="LRP_UserPromotionsLog" PageLoadType="DB">
                        <ListViewColumnCollection>
                            <ListViewColumn Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" Key="OID" Visible="false"/>
                            <ListViewColumn Caption="用户" ColumnType="Dict" DataColumnKey="OwnerUserId" ItemKey="LRP_OwnerUser" Key="OwnerUserId"/>
                            <ListViewColumn Caption="活动" ColumnType="ComboBox" DataColumnKey="PromotionId" Key="PromotionId" SourceType="Formula">
                                <FormulaItems>
                                    <![CDATA[DBNamedQuery('GetPromotionId');]]>
                                </FormulaItems>
                            </ListViewColumn>
                            <ListViewColumn Caption="目标用户" ColumnType="Dict" DataColumnKey="DestOwnerUserId" ItemKey="LRP_OwnerUser" Key="DestOwnerUserId"/>
                        </ListViewColumnCollection>
                    </ListView>
                    <SplitSize Size="90px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[
        
    ]]>
    </OnLoad>
</Form>
