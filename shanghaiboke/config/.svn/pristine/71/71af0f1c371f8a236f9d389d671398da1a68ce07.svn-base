        <Macro Key="RefreshDetailSum">
            <![CDATA[
			//物料明细加载或者修改后根据新的明细刷新物料明细汇总
			ClearAllRows('Grid_Sum',false);			
			loop"Grid0"(true){
				var i = 0;			
				var b = false;
				var count = GetRowCount('Grid_Sum');
				while ( i < count ) {					
					if(GetCellValue('Grid_Sum', i, 'MaterialId_Sum') == MaterialId
					&& GetCellValue('Grid_Sum', i, 'MaterialUnit_Sum') == MaterialUnit
					
					&& GetCellValue('Grid_Sum', i, 'SrcBatchNo_Sum') == SrcBatchNo
					&& GetCellValue('Grid_Sum', i, 'SrcManufDate_Sum') == SrcManufDate
					&& GetCellValue('Grid_Sum', i, 'SrcExpireDate_Sum') == SrcExpireDate
					&& GetCellValue('Grid_Sum', i, 'SrcRecvDate_Sum') == SrcRecvDate
					&& GetCellValue('Grid_Sum', i, 'SrcProjectId_Sum') == SrcProjectId
					&& GetCellValue('Grid_Sum', i, 'SrcStrUserDef1_Sum') == SrcStrUserDef1
					&& GetCellValue('Grid_Sum', i, 'SrcStrUserDef2_Sum') == SrcStrUserDef2
					&& GetCellValue('Grid_Sum', i, 'SrcStrUserDef3_Sum') == SrcStrUserDef3
					&& GetCellValue('Grid_Sum', i, 'SrcStrUserDef4_Sum') == SrcStrUserDef4
					&& GetCellValue('Grid_Sum', i, 'SrcStrUserDef5_Sum') == SrcStrUserDef5
					&& GetCellValue('Grid_Sum', i, 'SrcDictUserDef1_Sum') == SrcDictUserDef1
					&& GetCellValue('Grid_Sum', i, 'SrcDictUserDef2_Sum') == SrcDictUserDef2
					&& GetCellValue('Grid_Sum', i, 'SrcDictUserDef3_Sum') == SrcDictUserDef3
					&& GetCellValue('Grid_Sum', i, 'SrcDictUserDef4_Sum') == SrcDictUserDef4
					&& GetCellValue('Grid_Sum', i, 'SrcDictUserDef5_Sum') == SrcDictUserDef5
					&& GetCellValue('Grid_Sum', i, 'SrcDateUserDef1_Sum') == SrcDateUserDef1
					&& GetCellValue('Grid_Sum', i, 'SrcDateUserDef2_Sum') == SrcDateUserDef2
					&& GetCellValue('Grid_Sum', i, 'SrcNumUserDef1_Sum') == SrcNumUserDef1
					&& GetCellValue('Grid_Sum', i, 'SrcNumUserDef2_Sum') == SrcNumUserDef2
					&& GetCellValue('Grid_Sum', i, 'SrcNumUserDef3_Sum') == SrcNumUserDef3
					&& GetCellValue('Grid_Sum', i, 'SrcStoreroomId_Sum') == SrcStoreroomId
					&& GetCellValue('Grid_Sum', i, 'SrcStoreareaId_Sum') == SrcStoreareaId
					&& GetCellValue('Grid_Sum', i, 'SrcLocationId_Sum') == SrcLocationId
					&& GetCellValue('Grid_Sum', i, 'SrcHandingUnitNo_Sum') == SrcHandingUnitNo
					&& GetCellValue('Grid_Sum', i, 'SrcMaterialStatusId_Sum') == SrcMaterialStatusId
					
					&& GetCellValue('Grid_Sum', i, 'DestBatchNo_Sum') == DestBatchNo
					&& GetCellValue('Grid_Sum', i, 'DestManufDate_Sum') == DestManufDate
					&& GetCellValue('Grid_Sum', i, 'DestExpireDate_Sum') == DestExpireDate
					&& GetCellValue('Grid_Sum', i, 'DestRecvDate_Sum') == DestRecvDate
					&& GetCellValue('Grid_Sum', i, 'DestProjectId_Sum') == DestProjectId
					&& GetCellValue('Grid_Sum', i, 'DestStrUserDef1_Sum') == DestStrUserDef1
					&& GetCellValue('Grid_Sum', i, 'DestStrUserDef2_Sum') == DestStrUserDef2
					&& GetCellValue('Grid_Sum', i, 'DestStrUserDef3_Sum') == DestStrUserDef3
					&& GetCellValue('Grid_Sum', i, 'DestStrUserDef4_Sum') == DestStrUserDef4
					&& GetCellValue('Grid_Sum', i, 'DestStrUserDef5_Sum') == DestStrUserDef5
					&& GetCellValue('Grid_Sum', i, 'DestDictUserDef1_Sum') == DestDictUserDef1
					&& GetCellValue('Grid_Sum', i, 'DestDictUserDef2_Sum') == DestDictUserDef2
					&& GetCellValue('Grid_Sum', i, 'DestDictUserDef3_Sum') == DestDictUserDef3
					&& GetCellValue('Grid_Sum', i, 'DestDictUserDef4_Sum') == DestDictUserDef4
					&& GetCellValue('Grid_Sum', i, 'DestDictUserDef5_Sum') == DestDictUserDef5
					&& GetCellValue('Grid_Sum', i, 'DestDateUserDef1_Sum') == DestDateUserDef1
					&& GetCellValue('Grid_Sum', i, 'DestDateUserDef2_Sum') == DestDateUserDef2
					&& GetCellValue('Grid_Sum', i, 'DestNumUserDef1_Sum') == DestNumUserDef1
					&& GetCellValue('Grid_Sum', i, 'DestNumUserDef2_Sum') == DestNumUserDef2
					&& GetCellValue('Grid_Sum', i, 'DestNumUserDef3_Sum') == DestNumUserDef3
					&& GetCellValue('Grid_Sum', i, 'DestStoreroomId_Sum') == DestStoreroomId
					&& GetCellValue('Grid_Sum', i, 'DestStoreareaId_Sum') == DestStoreareaId
					&& GetCellValue('Grid_Sum', i, 'DestLocationId_Sum') == DestLocationId
					&& GetCellValue('Grid_Sum', i, 'DestHandingUnitNo_Sum') == DestHandingUnitNo
					&& GetCellValue('Grid_Sum', i, 'DestMaterialStatusId_Sum') == DestMaterialStatusId){
						//认为是同种物料，直接添加数量
						SetCellValue('Grid_Sum', i,'BasicQty_Sum',BasicQty + BasicQty_Sum);
						SetCellValue('Grid_Sum', i,'Qty_Sum',Qty + Qty_Sum);
						b = true;
					}
					i = i + 1;
				}
				if(b == false){
					//没有同种物料，直接新增一行物料明细
					var index = InsertRow('Grid_Sum',-1);
					SetCellValue('Grid_Sum', index,'MaterialId_Sum',MaterialId);
					SetCellValue('Grid_Sum', index,'MaterialUnit_Sum',MaterialUnit);
					SetCellValue('Grid_Sum', index,'BasicQty_Sum',BasicQty);
					SetCellValue('Grid_Sum', index,'Qty_Sum',Qty);
					SetCellValue('Grid_Sum', index,'SrcBatchNo_Sum',SrcBatchNo);
					SetCellValue('Grid_Sum', index,'SrcManufDate_Sum',SrcManufDate);
					SetCellValue('Grid_Sum', index,'SrcExpireDate_Sum',SrcExpireDate);
					SetCellValue('Grid_Sum', index,'SrcRecvDate_Sum',SrcRecvDate);
					SetCellValue('Grid_Sum', index,'SrcProjectId_Sum',SrcProjectId);
					SetCellValue('Grid_Sum', index,'SrcStrUserDef1_Sum',SrcStrUserDef1);
					SetCellValue('Grid_Sum', index,'SrcStrUserDef2_Sum',SrcStrUserDef2);
					SetCellValue('Grid_Sum', index,'SrcStrUserDef3_Sum',SrcStrUserDef3);
					SetCellValue('Grid_Sum', index,'SrcStrUserDef4_Sum',SrcStrUserDef4);
					SetCellValue('Grid_Sum', index,'SrcStrUserDef5_Sum',SrcStrUserDef5);
					SetCellValue('Grid_Sum', index,'SrcDictUserDef1_Sum',SrcDictUserDef1);
					SetCellValue('Grid_Sum', index,'SrcDictUserDef2_Sum',SrcDictUserDef2);
					SetCellValue('Grid_Sum', index,'SrcDictUserDef3_Sum',SrcDictUserDef3);
					SetCellValue('Grid_Sum', index,'SrcDictUserDef4_Sum',SrcDictUserDef4);
					SetCellValue('Grid_Sum', index,'SrcDictUserDef5_Sum',SrcDictUserDef5);
					SetCellValue('Grid_Sum', index,'SrcDateUserDef1_Sum',SrcDateUserDef1);
					SetCellValue('Grid_Sum', index,'SrcDateUserDef2_Sum',SrcDateUserDef2);
					SetCellValue('Grid_Sum', index,'SrcNumUserDef1_Sum',SrcNumUserDef1);
					SetCellValue('Grid_Sum', index,'SrcNumUserDef2_Sum',SrcNumUserDef2);
					SetCellValue('Grid_Sum', index,'SrcNumUserDef3_Sum',SrcNumUserDef3);
					SetCellValue('Grid_Sum', index,'SrcStoreroomId_Sum',SrcStoreroomId);
					SetCellValue('Grid_Sum', index,'SrcStoreareaId_Sum',SrcStoreareaId);
					SetCellValue('Grid_Sum', index,'SrcLocationId_Sum',SrcLocationId);
					SetCellValue('Grid_Sum', index,'SrcHandingUnitNo_Sum',SrcHandingUnitNo);
					SetCellValue('Grid_Sum', index,'SrcMaterialStatusId_Sum',SrcMaterialStatusId);
					SetCellValue('Grid_Sum', index,'DestBatchNo_Sum',DestBatchNo);
					SetCellValue('Grid_Sum', index,'DestManufDate_Sum',DestManufDate);
					SetCellValue('Grid_Sum', index,'DestExpireDate_Sum',DestExpireDate);
					SetCellValue('Grid_Sum', index,'DestRecvDate_Sum',DestRecvDate);
					SetCellValue('Grid_Sum', index,'DestProjectId_Sum',DestProjectId);
					SetCellValue('Grid_Sum', index,'DestStrUserDef1_Sum',DestStrUserDef1);
					SetCellValue('Grid_Sum', index,'DestStrUserDef2_Sum',DestStrUserDef2);
					SetCellValue('Grid_Sum', index,'DestStrUserDef3_Sum',DestStrUserDef3);
					SetCellValue('Grid_Sum', index,'DestStrUserDef4_Sum',DestStrUserDef4);
					SetCellValue('Grid_Sum', index,'DestStrUserDef5_Sum',DestStrUserDef5);
					SetCellValue('Grid_Sum', index,'DestDictUserDef1_Sum',DestDictUserDef1);
					SetCellValue('Grid_Sum', index,'DestDictUserDef2_Sum',DestDictUserDef2);
					SetCellValue('Grid_Sum', index,'DestDictUserDef3_Sum',DestDictUserDef3);
					SetCellValue('Grid_Sum', index,'DestDictUserDef4_Sum',DestDictUserDef4);
					SetCellValue('Grid_Sum', index,'DestDictUserDef5_Sum',DestDictUserDef5);
					SetCellValue('Grid_Sum', index,'DestDateUserDef1_Sum',DestDateUserDef1);
					SetCellValue('Grid_Sum', index,'DestDateUserDef2_Sum',DestDateUserDef2);
					SetCellValue('Grid_Sum', index,'DestNumUserDef1_Sum',DestNumUserDef1);
					SetCellValue('Grid_Sum', index,'DestNumUserDef2_Sum',DestNumUserDef2);
					SetCellValue('Grid_Sum', index,'DestNumUserDef3_Sum',DestNumUserDef3);
					SetCellValue('Grid_Sum', index,'DestStoreroomId_Sum',DestStoreroomId);
					SetCellValue('Grid_Sum', index,'DestStoreareaId_Sum',DestStoreareaId);
					SetCellValue('Grid_Sum', index,'DestLocationId_Sum',DestLocationId);
					SetCellValue('Grid_Sum', index,'DestHandingUnitNo_Sum',DestHandingUnitNo);
					SetCellValue('Grid_Sum', index,'DestMaterialStatusId_Sum',DestMaterialStatusId);
				}
			}
			]]>
