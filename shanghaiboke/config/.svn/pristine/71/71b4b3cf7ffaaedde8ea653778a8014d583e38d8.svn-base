<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Verdor_ContractView" Caption="供应商合同视图" FormType="View" AbbrCaption="供应商合同视图">
    <DataSource>
        <DataObject Key="CHCS_Verdor_Contract" Caption="供应商合同视图" PrimaryTableKey="CHCS_Verdor_Contract">
            <TableCollection>
                <Table Key="CHCS_Verdor_Contract" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="Business_Unit" Caption="业务单元" DataType="Varchar" Length="20"/>
                    <Column Key="Contract_Name" Caption="合同名称" DataType="Varchar" Length="200"/>
                    <Column Key="Contract_Classify" Caption="合同分类" DataType="Varchar" Length="20"/>
                    <Column Key="Contract_Unit" Caption="签约单位" DataType="Varchar" Length="20"/>
                    <Column Key="Principal_Contract_Code" Caption="主合同编号" DataType="Varchar" Length="100"/>
                    <Column Key="Related_Contract_Name" Caption="关联合同名称" DataType="Varchar" Length="100"/>
                    <Column Key="Contract_Code" Caption="合同编号" DataType="Varchar" Length="200"/>
                    <Column Key="Contract_Code_Number" Caption="合同编号分号" DataType="Varchar" Length="100"/>
                    <Column Key="Effective_Date" Caption="生效日期" DataType="DateTime"/>
                    <Column Key="Expiration_Date" Caption="到期日期" DataType="DateTime"/>
                    <Column Key="Contract_Type" Caption="合同类型" DataType="Varchar"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="New" Caption="新增" Icon="new.png">
            <Action>
                <![CDATA[New('CHCS_Verdor_Contract')]]>
            </Action>
        </Operation>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="L_Business_Unit" Caption="业务单元" X="0" Y="0"/>
                            <TextEditor Key="Business_Unit" Caption="业务单元" BuddyKey="L_Business_Unit" X="1" Y="0">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="like" ColumnKey="Business_Unit"/>
                            </TextEditor>
                            <Label Key="L_Contract_Name" Caption="合同名称" X="3" Y="0"/>
                            <TextEditor Key="Contract_Name" Caption="合同名称" BuddyKey="L_Contract_Name" X="4" Y="0">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="like" ColumnKey="Contract_Name"/>
                            </TextEditor>
                            <Label Key="L_Contract_Classify" Caption="合同分类" X="0" Y="4"/>
                            <TextEditor Key="Contract_Classify" Caption="合同分类" BuddyKey="L_Contract_Classify" X="1" Y="4">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="like" ColumnKey="Contract_Classify"/>
                            </TextEditor>
                            <Label Key="L_Contract_Unit" Caption="签约单位" X="6" Y="0"/>
                            <TextEditor Key="Contract_Unit" Caption="签约单位" BuddyKey="L_Contract_Unit" X="7" Y="0">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="like" ColumnKey="Contract_Unit"/>
                            </TextEditor>
                            <Label Key="L_Principal_Contract_Code" Caption="主合同编号" X="0" Y="1"/>
                            <TextEditor Key="Principal_Contract_Code" Caption="主合同编号" BuddyKey="L_Principal_Contract_Code" X="1" Y="1">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="like" ColumnKey="Principal_Contract_Code"/>
                            </TextEditor>
                            <Label Key="L_Related_Contract_Name" Caption="关联合同名称" X="0" Y="3"/>
                            <TextEditor Key="Related_Contract_Name" Caption="关联合同名称" BuddyKey="L_Related_Contract_Name" X="1" Y="3">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="like" ColumnKey="Related_Contract_Name"/>
                            </TextEditor>
                            <Label Key="L_Contract_Code" Caption="合同编号" X="6" Y="1"/>
                            <TextEditor Key="Contract_Code" Caption="合同编号" BuddyKey="L_Contract_Code" X="7" Y="1">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="like" ColumnKey="Contract_Code"/>
                            </TextEditor>
                            <Label Key="L_Contract_Code_Number" Caption="合同编号分号" X="0" Y="2"/>
                            <TextEditor Key="Contract_Code_Number" Caption="合同编号分号" BuddyKey="L_Contract_Code_Number" X="1" Y="2">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="like" ColumnKey="Contract_Code_Number"/>
                            </TextEditor>
                            <Label Key="L_Effective_Date" Caption="生效日期" X="3" Y="2"/>
                            <DatePicker Key="Effective_Date" Caption="生效日期" BuddyKey="L_Effective_Date" X="4" Y="2">
                                <Format/>
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="between" ColumnKey="Effective_Date" Group="Effective_Date" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="L_Effective_Date_Comp2" Caption="到" X="6" Y="2"/>
                            <DatePicker Key="Effective_Date_Comp2" Caption="到" BuddyKey="L_Effective_Date_Comp2" X="7" Y="2">
                                <Format/>
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="between" ColumnKey="Effective_Date" Group="Effective_Date" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="L_Expiration_Date" Caption="到期日期" X="3" Y="3"/>
                            <DatePicker Key="Expiration_Date" Caption="到期日期" BuddyKey="L_Expiration_Date" X="4" Y="3">
                                <Format/>
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="between" ColumnKey="Expiration_Date" Group="Expiration_Date" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="L_Expiration_Date_Comp2" Caption="到" X="6" Y="3"/>
                            <DatePicker Key="Expiration_Date_Comp2" Caption="到" BuddyKey="L_Expiration_Date_Comp2" X="7" Y="3">
                                <Format/>
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="between" ColumnKey="Expiration_Date" Group="Expiration_Date" GroupTail="true"/>
                            </DatePicker>
                            <Button Key="Query" Caption="查询" X="7" Y="4" Enable="true">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="7" Y="5" Enable="true">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <ComboBox Key="Contract_Type" Caption="合同类型" BuddyKey="Lab_Contract_Type" X="4" Y="4">
                                <Condition TableKey="CHCS_Verdor_Contract" CondSign="="/>
                                <Item Caption="国内" Key="0" Value="国内"/>
                                <Item Caption="国际" Key="1" Value="国际"/>
                            </ComboBox>
                            <Label Key="Lab_Contract_Type" Caption="合同类型" X="3" Y="4"/>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="33%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="list" PageLoadType="DB" TableKey="CHCS_Verdor_Contract">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="CHCS_Verdor_Contract" Visible="false" Width="30px"/>
                            <ListViewColumn Key="Business_Unit_LV" Caption="业务单元" ColumnType="TextEditor" DataColumnKey="Business_Unit"/>
                            <ListViewColumn Key="Contract_Name_LV" Caption="合同名称" ColumnType="TextEditor" DataColumnKey="Contract_Name"/>
                            <ListViewColumn Key="Contract_Classify_LV" Caption="合同分类" ColumnType="TextEditor" DataColumnKey="Contract_Classify"/>
                            <ListViewColumn Key="Contract_Unit_LV" Caption="签约单位" ColumnType="TextEditor" DataColumnKey="Contract_Unit"/>
                            <ListViewColumn Key="Principal_Contract_Code_LV" Caption="主合同编号" ColumnType="TextEditor" DataColumnKey="Principal_Contract_Code"/>
                            <ListViewColumn Key="Related_Contract_Name_LV" Caption="关联合同名称" ColumnType="TextEditor" DataColumnKey="Related_Contract_Name"/>
                            <ListViewColumn Key="Contract_Code_LV" Caption="合同编号" ColumnType="TextEditor" DataColumnKey="Contract_Code"/>
                            <ListViewColumn Key="Contract_Code_Number_LV" Caption="合同编号分号" ColumnType="TextEditor" DataColumnKey="Contract_Code_Number"/>
                            <ListViewColumn Key="Effective_Date_LV" Caption="生效日期" ColumnType="DatePicker" DataColumnKey="Effective_Date"/>
                            <ListViewColumn Key="Expiration_Date_LV" Caption="到期日期" ColumnType="DatePicker" DataColumnKey="Expiration_Date"/>
                            <ListViewColumn Key="Contract_Type_LV" Caption="合同类型" ColumnType="ComboBox">
                                <Item Caption="国内" Key="0" Value="国内"/>
                                <Item Caption="国际" Key="1" Value="国际"/>
                            </ListViewColumn>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('CHCS_Verdor_Contract', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="45%"/>
                    <SplitSize Size="55%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
