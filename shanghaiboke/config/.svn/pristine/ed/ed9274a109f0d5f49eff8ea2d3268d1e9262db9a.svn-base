                                    <GridCell Key="Confirmed" Caption="确定" Enable="BidingStatus2==0" CellType="Button">
                                        <OnClick>
                                            <![CDATA[
									  var rowIndex = GetRowIndex('Grid6');
									  var i = 0;
									  var rowCount = GetRowCount('Grid6');
									  while(i < rowCount){
									      if(GetCellValue('Grid6',i,'BidingStatus2') == 0 || GetCellValue('Grid6',i,'BidingStatus2') == 1){
									      if(i == rowIndex){
										     SetCellValue('Grid6',i,'Select_6',"1");
											 SetValue('CarrierId',CarrierId2);
						                     SetValue('BidingPrice',BidingPrice2);
											 SetCellValue('Grid6',i,'BidingStatus2',"1");
										  }else{
										    SetCellValue('Grid6',i,'Select_6',"0");
											SetCellValue('Grid6',i,'BidingStatus2',"0");
										  }
										  }
										 i = i + 1;
									}									  
									   SaveData(); UpdateView();
									
									]]>
                                        </OnClick>
                                    </GridCell>
                                </GridRow>
                            </GridRowCollection>
                        </Grid>
                    </TabPanel>
                    <SplitSize Size="265px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
