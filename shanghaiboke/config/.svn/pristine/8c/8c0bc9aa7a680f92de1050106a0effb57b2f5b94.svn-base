<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Passenger_Meal_OrderView" Caption="旅客点餐单视图" FormType="View" AbbrCaption="旅客点餐单视图">
    <DataSource>
        <DataObject Key="CHCS_Passenger_Meal_Order" Caption="旅客点餐单视图" PrimaryTableKey="CHCS_Passenger_Meal_Order">
            <TableCollection>
                <Table Key="CHCS_Passenger_Meal_Order" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="NO" Caption="单据编号" DataType="Varchar"/>
                    <Column Key="FlightDate" Caption="航班日期" DataType="DateTime"/>
                    <Column Key="Status" Caption="状态" DataType="Integer"/>
                    <Column Key="Flight_Number" Caption="航班号" DataType="Varchar"/>
                    <Column Key="Aircraft_Type_Number" Caption="机型" DataType="Long"/>
                    <Column Key="Aircraft_Number" Caption="机号" DataType="Varchar"/>
                    <Column Key="Point_Food" Caption="所点餐种" DataType="Long"/>
                    <Column Key="Point_Dish" Caption="所点菜品" DataType="Long"/>
                    <Column Key="Position_Level" Caption="舱位等级" DataType="Long"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="New" Caption="新增" Icon="new.png">
            <Action>
                <![CDATA[New('CHCS_Passenger_Meal_Order')]]>
            </Action>
        </Operation>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="L_NO" Caption="单据编号" X="0" Y="0"/>
                            <TextEditor Key="NO" Caption="单据编号" BuddyKey="L_NO" X="1" Y="0">
                                <Condition ColumnKey="NO" TableKey="CHCS_Passenger_Meal_Order" CondSign="like"/>
                            </TextEditor>
                            <Label Key="L_FlightDate" Caption="航班日期" X="2" Y="0"/>
                            <DatePicker Key="FlightDate" Caption="航班日期" BuddyKey="L_FlightDate" X="3" Y="0">
                                <Format/>
                                <Condition ColumnKey="FlightDate" TableKey="CHCS_Passenger_Meal_Order" CondSign="between" Group="FlightDate" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="L_FlightDate_Comp2" Caption="到" X="4" Y="0"/>
                            <DatePicker Key="FlightDate_Comp2" Caption="到" BuddyKey="L_FlightDate_Comp2" X="5" Y="0">
                                <Format/>
                                <Condition ColumnKey="FlightDate" TableKey="CHCS_Passenger_Meal_Order" CondSign="between" Group="FlightDate" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="L_Status" Caption="单据状态" X="0" Y="1"/>
                            <ComboBox Key="Status" Caption="单据状态" BuddyKey="L_Status" X="1" Y="1" SourceType="Status">
                                <Condition ColumnKey="Status" TableKey="CHCS_Passenger_Meal_Order" CondSign="="/>
                            </ComboBox>
                            <Label Key="L_Flight_Number" Caption="航班号" X="2" Y="1"/>
                            <TextEditor Key="Flight_Number" Caption="航班号" BuddyKey="L_Flight_Number" X="3" Y="1">
                                <Condition ColumnKey="Flight_Number" TableKey="CHCS_Passenger_Meal_Order" CondSign="like"/>
                            </TextEditor>
                            <Label Key="L_Aircraft_Type_Number" Caption="机型" X="4" Y="1"/>
                            <Dict Key="Aircraft_Type_Number" Caption="机型" BuddyKey="L_Aircraft_Type_Number" X="5" Y="1" AllowMultiSelection="true" ItemKey="CHCS_Aircraft_Type">
                                <Condition ColumnKey="Aircraft_Type_Number" TableKey="CHCS_Passenger_Meal_Order" CondSign="in"/>
                            </Dict>
                            <Label Key="L_Aircraft_Number" Caption="机号" X="0" Y="2"/>
                            <TextEditor Key="Aircraft_Number" Caption="机号" BuddyKey="L_Aircraft_Number" X="1" Y="2">
                                <Condition ColumnKey="Aircraft_Number" TableKey="CHCS_Passenger_Meal_Order" CondSign="like"/>
                            </TextEditor>
                            <Label Key="L_Point_Food" Caption="所点餐种" X="2" Y="2"/>
                            <Dict Key="Point_Food" Caption="所点餐种" BuddyKey="L_Point_Food" X="3" Y="2" AllowMultiSelection="true" ItemKey="LRP_Lookup_L">
                                <Condition ColumnKey="Point_Food" TableKey="CHCS_Passenger_Meal_Order" CondSign="in"/>
                                <ItemFilter>
                                    <Filter Query="select l.OID from lrp_lookup_l l left join lrp_lookup_h h on l.SOID=h.OID where upper(h.code)=upper(?)" Type="DataSet">
                                        <FilterValue DataType="Varchar" ParaValue="CPFL" Type="Const"/>
                                    </Filter>
                                </ItemFilter>
                            </Dict>
                            <Label Key="L_Point_Dish" Caption="所点菜品" X="4" Y="2"/>
                            <Dict Key="Point_Dish" Caption="所点菜品" BuddyKey="L_Point_Dish" X="5" Y="2" AllowMultiSelection="true" ItemKey="LRP_Lookup_L">
                                <Condition ColumnKey="Point_Dish" TableKey="CHCS_Passenger_Meal_Order" CondSign="in"/>
                                <ItemFilter>
                                    <Filter Query="select l.OID from lrp_lookup_l l left join lrp_lookup_h h on l.SOID=h.OID where upper(h.code)=upper(?)" Type="DataSet">
                                        <FilterValue DataType="Varchar" ParaValue="CPDPFL" Type="Const"/>
                                    </Filter>
                                </ItemFilter>
                            </Dict>
                            <Label Key="L_Position_Level" Caption="舱位等级" X="0" Y="3"/>
                            <Dict Key="Position_Level" Caption="舱位等级" BuddyKey="L_Position_Level" X="1" Y="3" AllowMultiSelection="true" ItemKey="LRP_Lookup_L">
                                <Condition ColumnKey="Position_Level" TableKey="CHCS_Passenger_Meal_Order" CondSign="in"/>
                                <ItemFilter>
                                    <Filter Query="select l.OID from lrp_lookup_l l left join lrp_lookup_h h on l.SOID=h.OID where upper(h.code)=upper(?)" Type="DataSet">
                                        <FilterValue DataType="Varchar" ParaValue="CWDJ" Type="Const"/>
                                    </Filter>
                                </ItemFilter>
                            </Dict>
                            <Button Key="Query" Caption="查询" X="4" Y="3" Enable="true">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="5" Y="3" Enable="true">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="34%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="list" PageLoadType="DB" TableKey="CHCS_Passenger_Meal_Order">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="CHCS_Passenger_Meal_Order" Visible="false" Width="30px"/>
                            <ListViewColumn Key="NO_LV" Caption="单据编号" ColumnType="TextEditor" DataColumnKey="NO" Width="200px"/>
                            <ListViewColumn Key="FlightDate_LV" Caption="航班日期" ColumnType="DatePicker" DataColumnKey="FlightDate" Width="150px"/>
                            <ListViewColumn Key="Status_LV" Caption="单据状态" ColumnType="ComboBox" DataColumnKey="Status" SourceType="Status"/>
                            <ListViewColumn Key="Flight_Number_LV" Caption="航班号" ColumnType="TextEditor" DataColumnKey="Flight_Number" Width="150px"/>
                            <ListViewColumn Key="Aircraft_Type_Number_LV" Caption="机型" ColumnType="Dict" DataColumnKey="Aircraft_Type_Number" ItemKey="CHCS_Aircraft_Type"/>
                            <ListViewColumn Key="Aircraft_Number_LV" Caption="机号" ColumnType="TextEditor" DataColumnKey="Aircraft_Number"/>
                            <ListViewColumn Key="Point_Food_LV" Caption="所点餐种" ColumnType="Dict" DataColumnKey="Point_Food" ItemKey="LRP_Lookup_L"/>
                            <ListViewColumn Key="Point_Dish_LV" Caption="所点菜品" ColumnType="Dict" DataColumnKey="Point_Dish" ItemKey="LRP_Lookup_L"/>
                            <ListViewColumn Key="Position_Level_LV" Caption="舱位等级" ColumnType="Dict" DataColumnKey="Position_Level" ItemKey="LRP_Lookup_L"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('CHCS_Passenger_Meal_Order', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="35%"/>
                    <SplitSize Size="65%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
