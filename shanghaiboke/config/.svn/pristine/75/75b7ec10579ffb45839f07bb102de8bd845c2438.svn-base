<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Transfer_HView" Caption="调拨单视图" FormType="View" AbbrCaption="调拨单视图">
    <DataSource>
        <DataObject Key="CHCS_Transfer_H" Caption="调拨单视图" PrimaryTableKey="CHCS_Transfer_H">
            <TableCollection>
                <Table Key="CHCS_Transfer_H" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="NO" Caption="单据编号" DataType="Varchar" SortType="Desc"/>
                    <Column Key="BillDate" Caption="单据日期" DataType="DateTime"/>
                    <Column Key="Status" Caption="单据状态" DataType="Integer"/>
                    <Column Key="Out_Warehouse" Caption="调出仓库" DataType="Long"/>
                    <Column Key="In_Warehouse" Caption="调入仓库" DataType="Long"/>
                    <Column Key="Flight_Date" Caption="航班日期" DataType="DateTime"/>
                    <Column Key="Out_Date" Caption="出库日期" DataType="DateTime"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="New" Caption="新增" Icon="new.png">
            <Action>
                <![CDATA[New('CHCS_Transfer_H')]]>
            </Action>
        </Operation>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="L_NO" Caption="系统单号" X="5" Y="0"/>
                            <TextEditor Key="NO" Caption="系统单号" BuddyKey="L_NO" X="6" Y="0">
                                <Condition ColumnKey="NO" TableKey="CHCS_Transfer_H" CondSign="like"/>
                            </TextEditor>
                            <Label Key="L_BillDate" Caption="单据日期 从" X="0" Y="0"/>
                            <DatePicker Key="BillDate" Caption="单据日期 从" BuddyKey="L_BillDate" X="1" Y="0" OnlyDate="true">
                                <Format HAlign="Center"/>
                                <Condition Group="BillDate" ColumnKey="BillDate" TableKey="CHCS_Transfer_H" CondSign="between" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="L_BillDate_Comp2" Caption="到" X="2" Y="0"/>
                            <DatePicker Key="BillDate_Comp2" Caption="到" BuddyKey="L_BillDate_Comp2" X="3" Y="0" OnlyDate="true">
                                <Format HAlign="Center"/>
                                <Condition Group="BillDate" GroupTail="true" ColumnKey="BillDate" TableKey="CHCS_Transfer_H" CondSign="between"/>
                            </DatePicker>
                            <Label Key="L_Status" Caption="单据状态" X="8" Y="0"/>
                            <ComboBox Key="Status" Caption="单据状态" BuddyKey="L_Status" X="9" Y="0" SourceType="Status">
                                <Condition ColumnKey="Status" TableKey="CHCS_Transfer_H" CondSign="="/>
                            </ComboBox>
                            <Label Key="L_Out_Warehouse" Caption="调出仓库" X="5" Y="1"/>
                            <Dict Key="Out_Warehouse" Caption="调出仓库" BuddyKey="L_Out_Warehouse" X="6" Y="1" AllowMultiSelection="true" ItemKey="Vendor">
                                <Condition ColumnKey="Out_Warehouse" TableKey="CHCS_Transfer_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_In_Warehouse" Caption="调入仓库" X="8" Y="1"/>
                            <Dict Key="In_Warehouse" Caption="调入仓库" BuddyKey="L_In_Warehouse" X="9" Y="1" AllowMultiSelection="true" ItemKey="Vendor">
                                <Condition ColumnKey="In_Warehouse" TableKey="CHCS_Transfer_H" CondSign="in"/>
                            </Dict>
                            <Label Key="Lab_Out_Date" Caption="出库日期 从" X="0" Y="1"/>
                            <DatePicker Key="Out_Date" Caption="出库日期 从" BuddyKey="Lab_Out_Date" X="1" Y="1" OnlyDate="true">
                                <Format/>
                                <Condition Group="Out_Date" ColumnKey="Out_Date" TableKey="CHCS_Transfer_H" CondSign="between" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="Lab_Out_Date_Comp2" Caption="到" X="2" Y="1"/>
                            <DatePicker Key="Out_Date_Comp2" Caption="到" BuddyKey="Lab_Out_Date_Comp2" X="3" Y="1" OnlyDate="true">
                                <Format/>
                                <Condition Group="Out_Date" GroupTail="true" ColumnKey="Out_Date" TableKey="CHCS_Transfer_H" CondSign="between"/>
                            </DatePicker>
                            <Button Key="Query" Caption="查询" X="11" Y="0" Enable="true" XSpan="2">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="11" Y="1" Enable="true" XSpan="2">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <RowDefCollection RowHeight="25" RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="90px"/>
                                <ColumnDef Width="16%"/>
                                <ColumnDef Width="15px"/>
                                <ColumnDef Width="16%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="50px"/>
                                <ColumnDef Width="4%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="list" PageLoadType="DB" TableKey="CHCS_Transfer_H">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="CHCS_Transfer_H" Visible="false" Width="30px"/>
                            <ListViewColumn Key="NO_LV" Caption="系统单号" ColumnType="TextEditor" DataColumnKey="NO" Width="132px"/>
                            <ListViewColumn Key="Out_Warehouse_LV" Caption="调出仓库" ColumnType="Dict" DataColumnKey="Out_Warehouse" Width="149px" ItemKey="Vendor"/>
                            <ListViewColumn Key="In_Warehouse_LV" Caption="调入仓库" ColumnType="Dict" DataColumnKey="In_Warehouse" Width="173px" ItemKey="Vendor"/>
                            <ListViewColumn Key="BillDate_LV" Caption="单据日期" ColumnType="DatePicker" DataColumnKey="BillDate" Width="131px" Format="yyyy-MM-dd" OnlyDate="true"/>
                            <ListViewColumn Key="Out_Date_LV" Caption="出库日期" ColumnType="DatePicker" DataColumnKey="Out_Date" Width="142px" Format="yyyy-MM-dd" OnlyDate="true"/>
                            <ListViewColumn Key="Status_LV" Caption="单据状态" ColumnType="ComboBox" DataColumnKey="Status" Width="141px" SourceType="Status"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('CHCS_Transfer_H', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="75px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
