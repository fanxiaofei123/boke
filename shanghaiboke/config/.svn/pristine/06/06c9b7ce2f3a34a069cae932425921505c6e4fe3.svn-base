<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="Annual_BudgetView" Caption="年度费用预算视图" FormType="View" AbbrCaption="年度费用预算视图">
    <DataSource>
        <DataObject Key="Annual_Budget" Caption="年度费用预算视图" PrimaryTableKey="Annual_Budget">
            <TableCollection>
                <Table Key="Annual_Budget" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="NO" Caption="单据编号" DataType="Varchar"/>
                    <Column Key="Budgetary_Classiy" Caption="预算分类" DataType="Varchar"/>
                    <Column Key="Status" Caption="订单状态" DataType="Integer"/>
                    <Column Key="BillDate" Caption="单据日期" DataType="DateTime"/>
                    <Column Key="Time_Frame_Start" Caption="业务日期 从" DataType="DateTime"/>
                    <Column Key="Time_Frame_Stop" Caption="业务日期 至" DataType="DateTime"/>
                    <Column Key="Budget_Department" Caption="预算部门" DataType="Varchar"/>
                    <Column Key="Budgetary_Name" Caption="预算名称" DataType="Varchar"/>
                    <Column Key="Total_Budget_Amount_Taxes" Caption="费用总金额（含税）" DataType="Numeric" Precision="16" Scale="2"/>
                    <Column Key="Total_Amount" Caption="总税额" DataType="Numeric" Precision="16" Scale="2"/>
                    <Column Key="Total_Budget_Amount" Caption="费用总金额（无税）" DataType="Numeric" Precision="16" Scale="2"/>
                    <Column Key="Staffing" Caption="编制人员" DataType="Long"/>
                    <Column Key="Per_Capita_Cost_Budget_Taxes" Caption="人均费用预算（含税）" DataType="Numeric" Precision="16" Scale="2"/>
                    <Column Key="Per_Capita_Cost_Budget" Caption="人均费用预算（无税）" DataType="Numeric" Precision="16" Scale="2"/>
                    <Statement>
                        <![CDATA[select Annual_Budget_L.OID,Annual_Budget_L.SOID,NO,Budgetary_Classiy,
Annual_Budget.Status,BillDate,Time_Frame_Start,Time_Frame_Stop,Budget_Department,
Budgetary_Name,Total_Budget_Amount_Taxes,Total_Amount,Total_Budget_Amount,Staffing,Per_Capita_Cost_Budget_Taxes,
Per_Capita_Cost_Budget from Annual_Budget 
left join Annual_Budget_L on Annual_Budget.OID=Annual_Budget_L.SOID]]>
                    </Statement>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="New" Caption="新增" Icon="new.png">
            <Action>
                <![CDATA[New('Annual_Budget')]]>
            </Action>
        </Operation>
    </OperationCollection>
    <QueryCollection>
        <Query Key="queryTimeRange" Description="查询时间范围">
            <Statement>
                <![CDATA[select concat(date_format(Time_Frame_Start,'%Y-%c-%d'),"至",date_format(Time_Frame_Stop,'%Y-%c-%d')) from Annual_Budget where oid = ?]]>
            </Statement>
            <ParameterCollection>
                <Parameter DataType="Long"/>
            </ParameterCollection>
        </Query>
    </QueryCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="L_NO" Caption="单据编号" X="0" Y="0"/>
                            <TextEditor Key="NO" Caption="单据编号" BuddyKey="L_NO" X="1" Y="0">
                                <Condition ColumnKey="NO" TableKey="Annual_Budget" CondSign="like"/>
                            </TextEditor>
                            <Button Key="Query" Caption="查询" X="9" Y="0" Enable="true">
                                <OnClick>
                                    <![CDATA[DealCondition();LoadData();ShowData();							]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="9" Y="1" Enable="true">
                                <OnClick>
                                    <![CDATA[ResetCondition();]]>
                                </OnClick>
                            </Button>
                            <ComboBox Key="BudgetType" Caption="预算分类" BuddyKey="Lab_BudgetType" X="4" Y="0">
                                <Condition ColumnKey="Budgetary_Classiy" TableKey="Annual_Budget" CondSign="like"/>
                                <Item Caption="餐食" Key="1" Value="1"/>
                                <Item Caption="机供品" Key="2" Value="2"/>
                            </ComboBox>
                            <Label Key="Lab_BudgetType" Caption="预算分类" X="3" Y="0"/>
                            <ComboBox Key="Status" Caption="单据状态" BuddyKey="Lab_Status" X="7" Y="0" SourceType="Status">
                                <Condition ColumnKey="Status" TableKey="Annual_Budget" CondSign="like"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="单据状态" X="6" Y="0"/>
                            <DatePicker Key="BudgetDateRangeStart" Caption="业务日期 从" BuddyKey="Lab_BudgetDateRangeStart" X="1" Y="1" OnlyDate="true">
                                <Condition ColumnKey="BillDate" TableKey="Annual_Budget" CondSign="between" Group="BillDate" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="Lab_BudgetDateRangeStart" Caption="业务日期 从" X="0" Y="1"/>
                            <DatePicker Key="BudgetDateRangeStop" Caption="业务日期 至" BuddyKey="Lab_BudgetDateRangeStop" X="4" Y="1" OnlyDate="true">
                                <Condition ColumnKey="BillDate" TableKey="Annual_Budget" CondSign="between" Group="BillDate" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="Lab_BudgetDateRangeStop" Caption="业务日期 至" X="3" Y="1"/>
                            <Dict Key="BudgetDepartment" Caption="预算部门" BuddyKey="Lab_BudgetDepartment" X="1" Y="2" ItemKey="CHCS_Department">
                                <Condition ColumnKey="Budget_Department" TableKey="Annual_Budget" CondSign="like"/>
                            </Dict>
                            <Label Key="Lab_BudgetDepartment" Caption="预算部门" X="0" Y="2"/>
                            <Dict Key="Staffing" Caption="编制人员" BuddyKey="Lab_Staffing" X="7" Y="1" ItemKey="CHCS_Employee">
                                <Condition ColumnKey="Staffing" TableKey="Annual_Budget" CondSign="like"/>
                            </Dict>
                            <Label Key="Lab_Staffing" Caption="编制人员" X="6" Y="1"/>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="80px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Height="pref" Key="list" PageLoadType="DB" TableKey="Annual_Budget">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="SOID" TableKey="Annual_Budget_H" Visible="false" Width="109px">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="NO_LV" Caption="单据编号" ColumnType="TextEditor" DataColumnKey="NO" Width="250px">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="Status_b" Caption="单据状态" ColumnType="ComboBox" DataColumnKey="Status" SourceType="Status">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="BillDate" Caption="单据日期" ColumnType="DatePicker" DataColumnKey="BillDate" Width="178px">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="BudgetaryName" Caption="预算名称" ColumnType="Dict" DataColumnKey="Budgetary_Name" Width="113px" ItemKey="CHCS_Budget_Items"/>
                            <ListViewColumn Key="BudgetDepartment_b" Caption="预算部门" ColumnType="Dict" DataColumnKey="Budget_Department" Width="144px" ItemKey="CHCS_Department">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="DateRange" Caption="时间范围" ColumnType="TextEditor" Width="292px" DefaultFormulaValue="DBNamedQueryValue('queryTimeRange',OID)">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="TotalBudgetAmountTaxes" Caption="费用总金额（含税）" ColumnType="NumberEditor" DataColumnKey="Total_Budget_Amount_Taxes" Width="137px">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="TotalBudgetAmount" Caption=" 费用总金额（无税）" ColumnType="NumberEditor" DataColumnKey="Total_Budget_Amount" Width="143px">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="TotalAmount" Caption="总税额" ColumnType="NumberEditor" DataColumnKey="Total_Amount" Width="88px">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="PerCapitaCostBudgetTaxes" Caption="人均费用预算（含税）" ColumnType="NumberEditor" Width="181px" DataColumnKey="Per_Capita_Cost_Budget_Taxes">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="PerCapitaCostBudget" Caption="人均费用预算（无税）" ColumnType="NumberEditor" Width="191px" DataColumnKey="Per_Capita_Cost_Budget">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                            <ListViewColumn Key="Staff_b" Caption="编制人员" ColumnType="Dict" DataColumnKey="Staffing" ItemKey="CHCS_Employee">
                                <Format HAlign="Left"/>
                            </ListViewColumn>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('Annual_Budget', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="30%"/>
                    <SplitSize Size="70%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
