<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Meal_Spectrum" Caption="餐谱" FormType="Dict">
    <DataSource RefObjectKey="CHCS_Meal_Spectrum"/>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Description="载入事件" Range="Action" Verb="Load">
            <![CDATA[LoadData()]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <SplitPanel Key="SplitPanel1" Orientation="Vertical" Caption="SplitPanel1">
                <TabPanel Caption="TabPanel1" Key="TabPanel1">
                    <GridLayoutPanel Key="Basic_Info" Padding="10px" Caption="基础信息">
                        <TextEditor Key="Code" Caption="代码" BuddyKey="L_Code" X="1" Y="0">
                            <DataBinding ColumnKey="Code" TableKey="CHCS_Meal_Spectrum"/>
                        </TextEditor>
                        <Label Key="L_Code" Caption="代码" X="0" Y="0"/>
                        <TextEditor Key="Name" Caption="名称" BuddyKey="L_Name" X="4" Y="0">
                            <DataBinding ColumnKey="Name" TableKey="CHCS_Meal_Spectrum"/>
                        </TextEditor>
                        <Label Key="L_Name" Caption="名称" X="3" Y="0"/>
                        <Dict Key="Product" Caption="对应餐种" BuddyKey="Lab_Product" X="1" Y="1" ItemKey="LRP_FinicialPeriod">
                            <DataBinding ColumnKey="Product" TableKey="CHCS_Meal_Spectrum"/>
                        </Dict>
                        <Label Key="Lab_Product" Caption="对应餐种" X="0" Y="1"/>
                        <ComboBox Key="Domestic_International" Caption="国内/国际" BuddyKey="Lab_Domestic_International" X="4" Y="1">
                            <DataBinding ColumnKey="Domestic_International" TableKey="CHCS_Meal_Spectrum"/>
                        </ComboBox>
                        <Label Key="Lab_Domestic_International" Caption="国内/国际" X="3" Y="1"/>
                        <TextEditor Key="Special_Requirements" Caption="特殊要求" BuddyKey="Lab_Special_Requirements" X="1" Y="2" XSpan="4" MaxLength="200">
                            <DataBinding ColumnKey="Special_Requirements" TableKey="CHCS_Meal_Spectrum"/>
                        </TextEditor>
                        <Label Key="Lab_Special_Requirements" Caption="特殊要求" X="0" Y="2"/>
                        <TextEditor Key="Remark" Caption="备注" BuddyKey="Lab_Remark" X="1" Y="3" MaxLength="200" XSpan="4">
                            <DataBinding ColumnKey="Remark" TableKey="CHCS_Meal_Spectrum"/>
                        </TextEditor>
                        <Label Key="Lab_Remark" Caption="备注" X="0" Y="3"/>
                        <RowDefCollection RowGap="6" RowHeight="30">
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="20">
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="30px"/>
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Key="Sys_Info" Padding="10px" Caption="系统信息">
                        <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                            <DataBinding ColumnKey="CREATOR" TableKey="CHCS_Meal_Spectrum"/>
                        </Dict>
                        <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                        <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                            <DataBinding ColumnKey="CREATETIME" TableKey="CHCS_Meal_Spectrum"/>
                        </DatePicker>
                        <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                        <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                            <DataBinding ColumnKey="MODIFIER" TableKey="CHCS_Meal_Spectrum"/>
                        </Dict>
                        <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                        <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                            <DataBinding ColumnKey="MODIFYTIME" TableKey="CHCS_Meal_Spectrum"/>
                        </DatePicker>
                        <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                        <RowDefCollection RowGap="5">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="5">
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
                <Grid Key="Grid1" Caption="Grid1">
                    <GridColumnCollection>
                        <GridColumn Key="column1" Caption="选择" Width="80px" Sortable="true"/>
                        <GridColumn Key="Single_Name" Caption="单品名称" Width="80px" Sortable="true"/>
                        <GridColumn Key="Tableware_Name" Caption="餐具名称" Width="80px" Sortable="true"/>
                        <GridColumn Key="Tableware_Number" Caption="餐具数量" Width="80px" Sortable="true"/>
                        <GridColumn Key="Is_Heating" Caption="是否加热" Width="80px" Sortable="true"/>
                        <GridColumn Key="Proportions" Caption="配备比例" Width="80px" Sortable="true"/>
                        <GridColumn Key="Price" Caption="单价" Width="80px" Sortable="true"/>
                    </GridColumnCollection>
                    <GridRowCollection>
                        <GridRow Key="row1" RowHeight="35" TableKey="CHCS_Meal_Spectrum_Detail">
                            <GridCell Key="select" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                            <GridCell Key="Single_Name" Caption="单品名称" CellType="TextEditor">
                                <DataBinding ColumnKey="Single_Name"/>
                            </GridCell>
                            <GridCell Key="Tableware_Name" Caption="餐具名称" CellType="TextEditor">
                                <DataBinding ColumnKey="Tableware_Name"/>
                            </GridCell>
                            <GridCell Key="Tableware_Number" Caption="餐具数量" CellType="NumberEditor">
                                <DataBinding ColumnKey="Tableware_Number"/>
                            </GridCell>
                            <GridCell Key="Is_Heating" Caption="是否加热">
                                <DataBinding ColumnKey="Is_Heating"/>
                            </GridCell>
                            <GridCell Key="Proportions" Caption="配备比例" CellType="NumberEditor">
                                <DataBinding ColumnKey="Proportions"/>
                            </GridCell>
                            <GridCell Key="Price" Caption="单价" CellType="NumberEditor">
                                <DataBinding ColumnKey="Price"/>
                            </GridCell>
                        </GridRow>
                    </GridRowCollection>
                </Grid>
                <SplitSize Size="40%"/>
                <SplitSize Size="60%"/>
            </SplitPanel>
        </Block>
    </Body>
</Form>
