<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Caption="仓间" FormType="Dict" Key="V_Storeroom_LRP">
    <DataSource RefObjectKey="LRP_Storeroom"/>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main">
                <TabPanel Caption="TabPanel0" Key="TabPanel0" Height="pref">
                    <GridLayoutPanel Caption="基本信息" Key="BasicTab" Padding="10px">
                        <TextEditor Caption="代码" Enable="!ReadOnly()" Key="Code" Tip="「代码」不可为空" X="1" XSpan="3" Y="0">
                            <DataBinding ColumnKey="Code" TableKey="LRP_Storeroom" Required="true"/>
                        </TextEditor>
                        <TextEditor Caption="名称" Enable="!ReadOnly()" Key="Name" Tip="「名称」不可为空" X="6" XSpan="3" Y="0">
                            <DataBinding ColumnKey="Name" TableKey="LRP_Storeroom" Required="true"/>
                        </TextEditor>
                        <GridLayoutPanel Caption="联系" Key="TabSelect" X="0" XSpan="4" Y="1">
                            <TextEditor Caption="联系人" Enable="!ReadOnly()" Key="Contacts" X="1" Y="0">
                                <DataBinding ColumnKey="Contacts" TableKey="LRP_Storeroom"/>
                            </TextEditor>
                            <TextEditor Caption="联系电话" Enable="!ReadOnly()" Key="Tel" X="4" Y="0">
                                <DataBinding ColumnKey="Tel" TableKey="LRP_Storeroom"/>
                            </TextEditor>
                            <Label Caption="联系人" Key="L_Contacts" X="0" Y="0"/>
                            <Label Caption="联系电话" Key="L_Tel" X="3" Y="0"/>
                            <RowDefCollection>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection>
                                <ColumnDef Width="65px"/>
                                <ColumnDef Width="50%" MinWidth="55px"/>
                                <ColumnDef Width="20px"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="50%" MinWidth="55px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <Label Caption="代码" Key="L_Code" X="0" Y="0"/>
                        <Label Caption="名称" Key="L_Name" X="5" Y="0"/>
                        <Dict Key="WarehouseCenterId" Caption="仓储中心" BuddyKey="Lab_WarehouseCenterId" X="6" Y="1" Visible="false" ItemKey="LRP_WarehouseCenter">
                            <DataBinding ColumnKey="WarehouseCenterId" DefaultFormulaValue="getCurOrgID();" Required="true" TableKey="LRP_Storeroom"/>
                        </Dict>
                        <Label Key="Lab_WarehouseCenterId" Caption="仓储中心" X="5" Y="1"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="系统信息" Key="SystemTab" Padding="10px">
                        <Dict Caption="创建人" ItemKey="Operator" Key="CREATOR" X="1" Y="0">
                            <DataBinding ColumnKey="CREATOR" DefaultFormulaValue="GetOperator()" TableKey="LRP_Storeroom"/>
                        </Dict>
                        <DatePicker Caption="创建时间" Key="CREATETIME" OnlyDate="false" X="1" Y="1">
                            <DataBinding ColumnKey="CREATETIME" DefaultFormulaValue="ServerDate()" TableKey="LRP_Storeroom"/>
                        </DatePicker>
                        <Dict Caption="修改人" ItemKey="Operator" Key="MODIFIER" X="1" Y="2">
                            <DataBinding ColumnKey="MODIFIER" DefaultFormulaValue="GetOperator()" TableKey="LRP_Storeroom"/>
                        </Dict>
                        <DatePicker Caption="修改时间" Key="MODIFYTIME" OnlyDate="false" X="1" Y="3">
                            <DataBinding ColumnKey="MODIFYTIME" DefaultFormulaValue="ServerDate()" TableKey="LRP_Storeroom"/>
                        </DatePicker>
                        <Label Caption="创建人" Key="L_CREATOR" X="0" Y="0"/>
                        <Label Caption="创建时间" Key="L_CREATETIME" X="0" Y="1"/>
                        <Label Caption="修改人" Key="L_MODIFIER" X="0" Y="2"/>
                        <Label Caption="修改时间" Key="L_MODIFYTIME" X="0" Y="3"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="100%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
