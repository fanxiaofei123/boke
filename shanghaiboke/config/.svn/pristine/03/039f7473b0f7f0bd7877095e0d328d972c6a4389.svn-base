<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Swap_HandoverView" Caption="对换交接单视图" FormType="View" AbbrCaption="对换交接单视图">
    <DataSource>
        <DataObject Key="CHCS_Swap_Handover" Caption="对换交接单视图" PrimaryTableKey="CHCS_Swap_Handover_H">
            <TableCollection>
                <Table Key="CHCS_Swap_Handover_H" TableMode="Detail" SourceType="Query" Persist="false">
                    <Column Key="OID" Caption="对象标识" DataType="Long" DBColumnName="OID"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long" DBColumnName="POID"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long" DBColumnName="SOID"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer" DBColumnName="VERID"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer" DBColumnName="DVERID"/>
                    <Column Key="Status" Caption="单据状态" DataType="Integer"/>
                    <Column Key="NO" Caption="配备编号" DataType="Varchar"/>
                    <Column Key="Vendor" Caption="航食公司" DataType="Long"/>
                    <Column Key="Flight_Number" Caption="航班号" DataType="Long"/>
                    <Column Key="ModifyDate" Caption="航班日期" DataType="Date"/>
                    <Column Key="Aircraft_Number" Caption="飞机号" DataType="Long"/>
                    <Column Key="Aircraft_Type_Number" Caption="机型" DataType="Long"/>
                    <Column Key="Segment" Caption="航段" DataType="Long"/>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Key="New" Caption="新增" Icon="new.png">
            <Action>
                <![CDATA[New('CHCS_Swap_Handover')]]>
            </Action>
        </Operation>
    </OperationCollection>
    <Body>
        <Block>
            <GridLayoutPanel Key="main" Height="100%" Padding="5px">
                <ToolBar Key="main_toolbar" X="0" Y="0">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main_split" Orientation="Vertical" Height="100%" X="0" Y="1">
                    <FlexFlowLayoutPanel Key="base_split" Caption="FlexFlowLayoutPanel">
                        <GridLayoutPanel Key="query" Height="100%" Padding="5px" Caption="查询字段">
                            <Label Key="L_NO" Caption="配备单号" X="0" Y="0"/>
                            <TextEditor Key="NO" Caption="配备单号" BuddyKey="L_NO" X="1" Y="0" XSpan="2">
                                <Condition ColumnKey="NO" TableKey="CHCS_Swap_Handover_H" CondSign="like"/>
                            </TextEditor>
                            <Label Key="L_Vendor" Caption="航食公司" X="4" Y="0"/>
                            <Dict Key="Vendor" Caption="航食公司" BuddyKey="L_Vendor" X="5" Y="0" ItemKey="Vendor" XSpan="2">
                                <Condition ColumnKey="Vendor" TableKey="CHCS_Swap_Handover_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_Flight_Number" Caption="航班号" X="8" Y="1"/>
                            <Dict Key="Flight_Number" Caption="航班号" BuddyKey="L_Flight_Number" X="9" Y="1" ItemKey="CHCS_Flight" XSpan="2">
                                <Condition ColumnKey="Flight_Number" TableKey="CHCS_Swap_Handover_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_ModifyDate" Caption="航班日期" X="0" Y="1"/>
                            <DatePicker Key="ModifyDate" Caption="航班日期" BuddyKey="L_ModifyDate" X="1" Y="1" OnlyDate="true" XSpan="2">
                                <Format/>
                                <Condition ColumnKey="ModifyDate" TableKey="CHCS_Swap_Handover_H" CondSign="between" Group="ModifyDate" GroupHead="true"/>
                            </DatePicker>
                            <Label Key="L_ModifyDate_Comp2" Caption="到" X="4" Y="1"/>
                            <DatePicker Key="ModifyDate_Comp2" Caption="到" BuddyKey="L_ModifyDate_Comp2" X="5" Y="1" OnlyDate="true" XSpan="2">
                                <Format/>
                                <Condition ColumnKey="ModifyDate" TableKey="CHCS_Swap_Handover_H" CondSign="between" Group="ModifyDate" GroupTail="true"/>
                            </DatePicker>
                            <Label Key="L_Aircraft_Number" Caption="飞机号" X="4" Y="2"/>
                            <Dict Key="Aircraft_Number" Caption="飞机号" BuddyKey="L_Aircraft_Number" X="5" Y="2" ItemKey="CHCS_Aircraft" XSpan="2">
                                <Condition ColumnKey="Aircraft_Number" TableKey="CHCS_Swap_Handover_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_Aircraft_Type_Number" Caption="机型" X="8" Y="2"/>
                            <Dict Key="Aircraft_Type_Number" Caption="机型" BuddyKey="L_Aircraft_Type_Number" X="9" Y="2" ItemKey="CHCS_Aircraft_Type" XSpan="2">
                                <Condition ColumnKey="Aircraft_Type_Number" TableKey="CHCS_Swap_Handover_H" CondSign="in"/>
                            </Dict>
                            <Label Key="L_Segment" Caption="航段" X="0" Y="2"/>
                            <ComboBox Key="Segment" Caption="航段" BuddyKey="L_Segment" X="1" Y="2" XSpan="2">
                                <Condition ColumnKey="Segment" TableKey="CHCS_Swap_Handover_H" CondSign="="/>
                            </ComboBox>
                            <Button Key="Query" Caption="查询" X="12" Y="1" Enable="true">
                                <OnClick>
                                    <![CDATA[
								DealCondition();LoadData();ShowData();	
								]]>
                                </OnClick>
                            </Button>
                            <Button Key="cancel" Caption="重置" X="12" Y="2" Enable="true">
                                <OnClick>
                                    <![CDATA[
								ResetCondition();	
								]]>
                                </OnClick>
                            </Button>
                            <ComboBox Key="Status" Caption="单据状态" BuddyKey="Lab_Status" X="9" Y="0" SourceType="Status" XSpan="2">
                                <DataBinding TableKey="CHCS_Swap_Handover_H" ColumnKey="Status"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="单据状态" X="8" Y="0"/>
                            <RowDefCollection RowHeight="25" RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="32%"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="50px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </FlexFlowLayoutPanel>
                    <ListView Key="list" PageLoadType="DB" TableKey="CHCS_Swap_Handover_H">
                        <ListViewColumnCollection>
                            <ListViewColumn Key="OID" Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" TableKey="CHCS_Swap_Handover_H" Visible="false" Width="72px"/>
                            <ListViewColumn Key="NO_LV" Caption="配备编号" ColumnType="TextEditor" DataColumnKey="NO" Width="180px"/>
                            <ListViewColumn Key="Vendor_LV" Caption="航食公司" ColumnType="Dict" DataColumnKey="Vendor" Width="150px" ItemKey="Vendor"/>
                            <ListViewColumn Key="Flight_Number_LV" Caption="航班号" ColumnType="Dict" DataColumnKey="Flight_Number" Width="120px" ItemKey="CHCS_Flight"/>
                            <ListViewColumn Key="ModifyDate_LV" Caption="航班日期" ColumnType="DatePicker" DataColumnKey="ModifyDate" Width="120px" Format="yyyy-MM-dd" OnlyDate="true"/>
                            <ListViewColumn Key="Aircraft_Number_LV" Caption="飞机号" ColumnType="Dict" DataColumnKey="Aircraft_Number" ItemKey="CHCS_Aircraft"/>
                            <ListViewColumn Key="Aircraft_Type_Number_LV" Caption="机型" ColumnType="Dict" DataColumnKey="Aircraft_Type_Number" ItemKey="CHCS_Aircraft_Type"/>
                            <ListViewColumn Key="Segment_LV" Caption="航段" ColumnType="ComboBox" DataColumnKey="Segment"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[Open('CHCS_Swap_Handover', OID)]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="35%"/>
                    <SplitSize Size="65%"/>
                </SplitPanel>
                <RowDefCollection RowHeight="25">
                    <RowDef Height="40px"/>
                    <RowDef Height="100%"/>
                </RowDefCollection>
                <ColumnDefCollection>
                    <ColumnDef Width="100%"/>
                </ColumnDefCollection>
            </GridLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData();]]>
    </OnLoad>
</Form>
