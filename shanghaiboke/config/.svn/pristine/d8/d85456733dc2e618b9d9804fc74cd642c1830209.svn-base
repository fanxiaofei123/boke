<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="V_CheckItem" Caption="安检项目" FormType="Dict">
    <DataSource RefObjectKey="LRP_CheckItem"/>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main">
                <TabPanel Caption="TabPanel0" Key="TabPanel0" Height="pref">
                    <GridLayoutPanel Key="BasicTab" Padding="10px" Caption="基本信息">
                        <TextEditor Key="Code" Caption="代码" X="1" Y="0" Tip="「代码」不可为空" XSpan="3">
                            <DataBinding TableKey="LRP_CheckItem" ColumnKey="Code" Required="true"/>
                        </TextEditor>
                        <TextEditor Key="Name" Caption="名称" X="6" Y="0" Tip="「名称」不可为空" XSpan="3">
                            <DataBinding TableKey="LRP_CheckItem" ColumnKey="Name" Required="true"/>
                        </TextEditor>
                        <Label Key="L_Code" Caption="代码" X="0" Y="0"/>
                        <Label Key="L_Name" Caption="名称" X="5" Y="0"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Key="SystemTab" Padding="10px" Caption="系统信息">
                        <Dict Key="CREATOR" Caption="创建人" X="1" Y="0" ItemKey="Operator">
                            <DataBinding TableKey="LRP_CheckItem" ColumnKey="CREATOR" DefaultFormulaValue="GetOperator()"/>
                        </Dict>
                        <DatePicker Key="CREATETIME" Caption="创建时间" X="1" Y="1">
                            <DataBinding TableKey="LRP_CheckItem" ColumnKey="CREATETIME" DefaultFormulaValue="ServerDate()"/>
                        </DatePicker>
                        <Dict Key="MODIFIER" Caption="修改人" X="1" Y="2" ItemKey="Operator">
                            <DataBinding TableKey="LRP_CheckItem" ColumnKey="MODIFIER" DefaultFormulaValue="GetOperator()"/>
                        </Dict>
                        <DatePicker Key="MODIFYTIME" Caption="修改时间" X="1" Y="3">
                            <DataBinding TableKey="LRP_CheckItem" ColumnKey="MODIFYTIME" DefaultFormulaValue="ServerDate()"/>
                        </DatePicker>
                        <Label Key="L_CREATOR" Caption="创建人" X="0" Y="0"/>
                        <Label Key="L_CREATETIME" Caption="创建时间" X="0" Y="1"/>
                        <Label Key="L_MODIFIER" Caption="修改人" X="0" Y="2"/>
                        <Label Key="L_MODIFYTIME" Caption="修改时间" X="0" Y="3"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="100%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
