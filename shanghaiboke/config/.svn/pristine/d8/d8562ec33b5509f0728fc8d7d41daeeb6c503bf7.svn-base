<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Full_Match_Order" Caption="满配交接单" FormType="Entity" ViewKey="CHCS_Full_Match_OrderView">
    <DataSource RefObjectKey="CHCS_Full_Match_Order"/>
    <OperationCollection>
        <Operation Key="WORKITEM" Caption="WORKITEM" Tag="WORKITEM"/>
        <Operation Key="BPM" Caption="BPM" Tag="BPM"/>
        <Operation Key="Save" Caption="保存" Visible="!ReadOnly()">
            <Action>
                <![CDATA[SaveData(); UpdateView();]]>
            </Action>
        </Operation>
        <Operation Key="New" Caption="新增" Visible="ReadOnly()">
            <Action>
                <![CDATA[New('CHCS_Full_Match_Order', 'self');]]>
            </Action>
        </Operation>
        <Operation Key="Edit" Caption="编辑" Visible="ReadOnly()">
            <Action>
                <![CDATA[Edit();]]>
            </Action>
        </Operation>
        <Operation Key="Cancel" Caption="取消" Visible="!ReadOnly()">
            <Action>
                <![CDATA[Cancel();]]>
            </Action>
        </Operation>
        <Operation Key="Delete" Caption="删除" Visible="ReadOnly()">
            <Action>
                <![CDATA[Confirm(LocaleString("CommonDef", "key2"),'YES_NO',{YES:{DeleteData();UpdateView();Close();},NO:{}});]]>
            </Action>
        </Operation>
        <Operation Key="Sign" Caption="签收"/>
        <Operation Key="optKey1" Caption="打印预览">
            <Action>
                <![CDATA[PrintPreview("", false);]]>
            </Action>
        </Operation>
    </OperationCollection>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板">
                    <TabPanel Caption="TabPanel1" Height="pref" Key="TabPanel1">
                        <GridLayoutPanel Key="first_head" Height="pref" Padding="5px" OverflowY="Scroll" Caption="基本信息">
                            <TextEditor Key="NO" Caption="配备编号" BuddyKey="Lab_NO" X="1" Y="0" Enable="false" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="NO" TableKey="CHCS_Full_Match_Order_H"/>
                            </TextEditor>
                            <Label Key="Lab_NO" Caption="配备编号" X="0" Y="0"/>
                            <DatePicker Key="BillDate" Caption="交接日期" BuddyKey="Lab_BillDate" X="5" Y="0" Enable="false" XSpan="2">
                                <DataBinding ColumnKey="BillDate" TableKey="CHCS_Full_Match_Order_H"/>
                            </DatePicker>
                            <Label Key="Lab_BillDate" Caption="交接日期" X="4" Y="0"/>
                            <ComboBox Key="Status" Caption="单据状态" BuddyKey="Lab_Status" X="9" Y="0" Enable="false" XSpan="2">
                                <DataBinding DefaultFormulaValue="100" ColumnKey="Status" TableKey="CHCS_Full_Match_Order_H"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="单据状态" X="8" Y="0"/>
                            <DatePicker Key="FlightDate" Caption="航班日期" BuddyKey="Lab_FlightDate" X="1" Y="1" AsQuery="true" XSpan="2" OnlyDate="true">
                                <DataBinding ColumnKey="FlightDate" TableKey="CHCS_Full_Match_Order_H"/>
                                <Format/>
                            </DatePicker>
                            <Label Key="Lab_FlightDate" Caption="航班日期" X="0" Y="1"/>
                            <Dict Key="Aircraft_Number" Caption="飞机号" BuddyKey="Lab_Aircraft_Number" X="1" Y="2" AsQuery="true" XSpan="2" ItemKey="CHCS_Aircraft">
                                <DataBinding ColumnKey="Aircraft_Number" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Aircraft_Number" Caption="飞机号" X="0" Y="2"/>
                            <Dict Key="Aircraft_Type_Number" Caption="机型" BuddyKey="Lab_Aircraft_Type_Number" X="5" Y="2" ItemKey="CHCS_Aircraft_Type" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="Aircraft_Type_Number" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Aircraft_Type_Number" Caption="机型" X="4" Y="2"/>
                            <Dict Key="Departure_Vendor" Caption="出港航食" BuddyKey="Lab_Departure_Vendor" X="1" Y="3" ItemKey="Vendor" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="Departure_Vendor" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Departure_Vendor" Caption="出港航食" X="0" Y="3"/>
                            <Dict Key="Arrival_Vendor" Caption="进港航食" BuddyKey="Lab_Arrival_Vendor" X="5" Y="3" ItemKey="Vendor" AsQuery="true" XSpan="2">
                                <DataBinding ColumnKey="Arrival_Vendor" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Arrival_Vendor" Caption="进港航食" X="4" Y="3"/>
                            <TextEditor Key="Lock_Catch_Number" Caption="锁扣号" BuddyKey="Lab_Lock_Catch_Number" X="9" Y="3" XSpan="2">
                                <DataBinding ColumnKey="Lock_Catch_Number" TableKey="CHCS_Full_Match_Order_H"/>
                            </TextEditor>
                            <Label Key="Lab_Lock_Catch_Number" Caption="锁扣号" X="8" Y="3"/>
                            <Dict Key="Departure_Flight_Attendant" Caption="出港乘务员" BuddyKey="Lab_Departure_Flight_Attendant" X="1" Y="4" ItemKey="CHCS_Employee" XSpan="2">
                                <DataBinding ColumnKey="Departure_Flight_Attendant" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Departure_Flight_Attendant" Caption="出港乘务员" X="0" Y="4"/>
                            <Dict Key="Arrival_Flight_Attendant" Caption="回港乘务员" BuddyKey="Lab_Arrival_Flight_Attendant" X="5" Y="4" ItemKey="CHCS_Employee" XSpan="2">
                                <DataBinding ColumnKey="Arrival_Flight_Attendant" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Arrival_Flight_Attendant" Caption="回港乘务员" X="4" Y="4"/>
                            <Dict Key="Departure_Aircrew" Caption="出港航机员" BuddyKey="Lab_Departure_Aircrew" X="1" Y="5" ItemKey="CHCS_Employee" XSpan="2">
                                <DataBinding ColumnKey="Departure_Aircrew" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Departure_Aircrew" Caption="出港航机员" X="0" Y="5"/>
                            <Dict Key="Arrival_Aircrew" Caption="回港航机员" BuddyKey="Lab_Arrival_Aircrew" X="5" Y="5" ItemKey="CHCS_Employee" XSpan="2">
                                <DataBinding ColumnKey="Arrival_Aircrew" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Arrival_Aircrew" Caption="回港航机员" X="4" Y="5"/>
                            <Dict Key="Flight_Number" Caption="航班号" BuddyKey="Lab_Flight_Number" X="5" Y="1" ItemKey="CHCS_Flight" XSpan="2">
                                <DataBinding ColumnKey="Flight_Number" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Flight_Number" Caption="航班号" X="4" Y="1"/>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="70px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="70px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="80px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="second_head" Height="pref" Padding="5px" Caption="其他信息">
                            <Dict Key="Creator" Caption="制单人" BuddyKey="Lab_Creator" X="1" Y="0" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="Creator" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Creator" Caption="制单人" X="0" Y="0"/>
                            <DatePicker Key="CreateTime" Caption="制单日期" BuddyKey="Lab_CreateTime" X="3" Y="0" Enable="false">
                                <DataBinding ColumnKey="CreateTime" TableKey="CHCS_Full_Match_Order_H"/>
                            </DatePicker>
                            <Label Key="Lab_CreateTime" Caption="制单日期" X="2" Y="0"/>
                            <Dict Key="Modifier" Caption="修改人" BuddyKey="Lab_Modifier" X="1" Y="1" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="Modifier" TableKey="CHCS_Full_Match_Order_H"/>
                            </Dict>
                            <Label Key="Lab_Modifier" Caption="修改人" X="0" Y="1"/>
                            <DatePicker Key="ModifyTime" Caption="修改时间" BuddyKey="Lab_ModifyTime" X="3" Y="1" Enable="false">
                                <DataBinding ColumnKey="ModifyTime" TableKey="CHCS_Full_Match_Order_H"/>
                            </DatePicker>
                            <Label Key="Lab_ModifyTime" Caption="修改时间" X="2" Y="1"/>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <Grid Key="Grid1" Caption="Grid1">
                        <GridColumnCollection>
                            <GridColumn Key="column1" Caption="选择" Width="80px"/>
                            <GridColumn Key="Tableware_Type" Caption="餐具种类" Width="80px"/>
                            <GridColumn Key="Meal_Tableware_Qty" Caption="有餐餐具" Width="80px"/>
                            <GridColumn Key="column2" Caption="出港" Width="280px">
                                <GridColumnCollection>
                                    <GridColumn Key="Empty_Tableware_Qty" Caption="空餐具数量" Width="80px"/>
                                    <GridColumn Key="Act_Recv_Empty_Qty" Caption="实收空餐具数量" Width="100px"/>
                                    <GridColumn Key="Departure_Full_Match_Diff" Caption="与满配对比差异" Width="100px"/>
                                </GridColumnCollection>
                            </GridColumn>
                            <GridColumn Key="column6" Caption="进港" Width="200px">
                                <GridColumnCollection>
                                    <GridColumn Key="Arrival_Vendor_Recv_Qty" Caption="回港航食回收数量" Width="100px"/>
                                    <GridColumn Key="Arrival_Full_Match_Diff" Caption="与满配对比差异" Width="100px"/>
                                </GridColumnCollection>
                            </GridColumn>
                            <GridColumn Key="Unit" Caption="单位" Width="80px"/>
                            <GridColumn Key="Remark" Caption="备注" Width="80px"/>
                        </GridColumnCollection>
                        <GridRowCollection>
                            <GridRow Key="row1" RowHeight="36" TableKey="CHCS_Full_Match_Order_L">
                                <GridCell Key="select" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                <GridCell Key="Tableware_Type" Caption="餐具种类" CellType="Dict" ItemKey="CHCS_Product">
                                    <DataBinding ColumnKey="Tableware_Type"/>
                                </GridCell>
                                <GridCell Key="Meal_Tableware_Qty" Caption="有餐餐具" CellType="NumberEditor" Format="">
                                    <DataBinding ColumnKey="Meal_Tableware_Qty"/>
                                </GridCell>
                                <GridCell Key="Empty_Tableware_Qty" Caption="空餐具数量" CellType="NumberEditor" Format="">
                                    <DataBinding ColumnKey="Empty_Tableware_Qty"/>
                                </GridCell>
                                <GridCell Key="Act_Recv_Empty_Qty" Caption="实收空餐具数量" CellType="NumberEditor" Format="">
                                    <DataBinding ColumnKey="Act_Recv_Empty_Qty"/>
                                </GridCell>
                                <GridCell Key="Departure_Full_Match_Diff" Caption="与满配对比差异" CellType="NumberEditor" Format="">
                                    <DataBinding ColumnKey="Departure_Full_Match_Diff"/>
                                </GridCell>
                                <GridCell Key="Arrival_Vendor_Recv_Qty" Caption="回港航食回收数量" CellType="NumberEditor" Format="">
                                    <DataBinding ColumnKey="Arrival_Vendor_Recv_Qty"/>
                                </GridCell>
                                <GridCell Key="Arrival_Full_Match_Diff" Caption="与满配对比差异" CellType="NumberEditor" Format="">
                                    <DataBinding ColumnKey="Arrival_Full_Match_Diff"/>
                                </GridCell>
                                <GridCell Key="Unit" Caption="单位" CellType="Dict" ItemKey="LRP_Lookup_L">
                                    <DataBinding ColumnKey="Unit"/>
                                    <ItemFilter>
                                        <Filter Query="select oid  from LRP_Lookup_L where HeadCode='DW'" Type="DataSet"/>
                                    </ItemFilter>
                                </GridCell>
                                <GridCell Key="Remark" Caption="备注" CellType="TextEditor">
                                    <DataBinding ColumnKey="Remark"/>
                                </GridCell>
                            </GridRow>
                            <GridRow Key="row2" RowHeight="36" RowType="Total">
                                <GridCell Key="cell26" Caption="合计"/>
                                <GridCell Key="cell27"/>
                                <GridCell Key="cell28">
                                    <DataBinding DefaultFormulaValue="Sum(&quot;Meal_Tableware_Qty&quot;)"/>
                                </GridCell>
                                <GridCell Key="cell29">
                                    <DataBinding DefaultFormulaValue="Sum(&quot;Empty_Tableware_Qty&quot;)"/>
                                </GridCell>
                                <GridCell Key="cell30">
                                    <DataBinding DefaultFormulaValue="Sum(&quot;Act_Recv_Empty_Qty&quot;)"/>
                                </GridCell>
                                <GridCell Key="cell31">
                                    <DataBinding DefaultFormulaValue="Sum(&quot;Departure_Full_Match_Diff&quot;)"/>
                                </GridCell>
                                <GridCell Key="cell32">
                                    <DataBinding DefaultFormulaValue="Sum(&quot;Arrival_Vendor_Recv_Qty&quot;)"/>
                                </GridCell>
                                <GridCell Key="cell33">
                                    <DataBinding DefaultFormulaValue="Sum(&quot;Arrival_Full_Match_Diff&quot;)"/>
                                </GridCell>
                                <GridCell Key="cell34"/>
                                <GridCell Key="cell35"/>
                            </GridRow>
                        </GridRowCollection>
                    </Grid>
                    <SplitSize Size="60%"/>
                    <SplitSize Size="40%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
