<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Caption="成本控制范围" FormType="Dict" Key="V_ControllingArea">
    <DataSource RefObjectKey="ControllingArea"/>
    <Body>
        <Block>
            <SplitPanel Height="100%" Key="main_split" Orientation="Vertical">
                <GridLayoutPanel Caption="Tab页选择" Key="TabSelect" Padding="10px">
                    <CheckBox Caption="大猫" Key="DaMaoCheck" X="0" Y="0"/>
                    <CheckBox Caption="物流" Key="WuliuCheck" X="1" Y="0"/>
                    <CheckBox Caption="集团财务" Key="CaiWuCheck" X="2" Y="0"/>
                    <CheckBox Caption="电商" Key="DianShangCheck" X="3" Y="0"/>
                    <CheckBox Caption="HRP" Key="HRPCheck" X="4" Y="0"/>
                    <CheckBox Caption="OA" Key="OACheck" X="5" Y="0"/>
                    <RowDefCollection RowGap="8">
                        <RowDef Height="30px"/>
                    </RowDefCollection>
                    <ColumnDefCollection ColumnGap="8">
                        <ColumnDef Width="16%" MinWidth="55px"/>
                        <ColumnDef Width="16%" MinWidth="55px"/>
                        <ColumnDef Width="17%" MinWidth="55px"/>
                        <ColumnDef Width="17%" MinWidth="55px"/>
                        <ColumnDef Width="17%" MinWidth="55px"/>
                        <ColumnDef Width="17%" MinWidth="55px"/>
                    </ColumnDefCollection>
                </GridLayoutPanel>
                <TabPanel Caption="TabPanel0" Key="TabPanel0">
                    <GridLayoutPanel Caption="基本信息" Key="BasicTab" Padding="10px">
                        <Label Caption="代码" Key="L_Code" X="0" Y="0"/>
                        <TextEditor Caption="代码" Key="Code" Tip="「代码」不能为空" X="1" XSpan="3" Y="0">
                            <DataBinding ColumnKey="Code" TableKey="BK_ControllingArea" Required="true"/>
                        </TextEditor>
                        <Label Caption="名称" Key="L_Name" X="5" Y="0"/>
                        <TextEditor Caption="名称" Key="Name" Tip="「名称」不能为空" X="6" XSpan="3" Y="0">
                            <DataBinding ColumnKey="Name" TableKey="BK_ControllingArea" Required="true"/>
                        </TextEditor>
                        <Label Caption="币种" Key="L_CurrencyID" X="0" Y="1"/>
                        <Dict Caption="币种" ItemKey="Currency" Key="CurrencyID" Tip="「币种」不能为空" X="1" XSpan="3" Y="1">
                            <DataBinding ColumnKey="CurrencyID" TableKey="BK_ControllingArea" Required="true"/>
                        </Dict>
                        <Label Caption="货币类型" Key="L_CurrencyType" X="5" Y="1"/>
                        <ComboBox Caption="货币类型" Key="CurrencyType" X="6" XSpan="3" Y="1">
                            <DataBinding ColumnKey="CurrencyType" TableKey="BK_ControllingArea"/>
                            <Item Key="combox1" Caption="公司代码货币" Value="10"/>
                            <Item Key="combox2" Caption="控制范围货币" Value="20"/>
                            <Item Key="combox3" Caption="集团货币" Value="30"/>
                            <Item Key="combox4" Caption="硬通货" Value="40"/>
                            <Item Key="combox5" Caption="公司货币" Value="60"/>
                        </ComboBox>
                        <Label Caption="公司分配控制" Key="L_CocdCOAreaAssignRule" X="0" Y="2"/>
                        <ComboBox Caption="公司分配控制" Key="CocdCOAreaAssignRule" X="1" XSpan="3" Y="2">
                            <DataBinding ColumnKey="CocdCOAreaAssignRule" TableKey="BK_ControllingArea"/>
                            <Item Key="combox1" Caption="单公司" Value="1"/>
                            <Item Key="combox2" Caption="跨公司" Value="2"/>
                        </ComboBox>
                        <Label Caption="科目表" Key="L_AccountChartID" Tip="「科目表」不能为空" X="5" Y="2"/>
                        <Dict Caption="科目表" Itemkey="FI_AccountChart" Key="AccountChartID" X="6" XSpan="3" Y="2" ItemKey="AccountChart">
                            <DataBinding ColumnKey="AccountChartID" TableKey="BK_ControllingArea" Required="true"/>
                        </Dict>
                        <Label Caption="会计期方案" Key="L_PeriodTypeID" Tip="「会计期方案」不能为空" X="0" Y="3"/>
                        <Dict Caption="会计期方案" Itemkey="PeriodType" Key="PeriodTypeID" X="1" XSpan="3" Y="3" ItemKey="PeriodType">
                            <DataBinding ColumnKey="PeriodTypeID" TableKey="BK_ControllingArea" Required="true"/>
                        </Dict>
                        <Label Caption="激活财务调整" Key="L_ActiveRecoLedger" X="5" Y="3"/>
                        <CheckBox Caption="激活财务调整" Key="ActiveRecoLedger" X="6" XSpan="3" Y="3">
                            <DataBinding ColumnKey="ActiveRecoLedger" TableKey="BK_ControllingArea"/>
                        </CheckBox>
                        <Label Caption="凭证类型" Key="L_VoucherTypeID" X="0" Y="4"/>
                        <Dict Caption="凭证类型" Itemkey="VoucherType" Key="VoucherTypeID" X="1" XSpan="3" Y="4" ItemKey="VoucherType">
                            <DataBinding ColumnKey="VoucherTypeID" TableKey="BK_ControllingArea"/>
                        </Dict>
                        <Label Caption="负责人" Key="L_ResponsiblePersonID" X="5" Y="4"/>
                        <Dict Caption="负责人" Itemkey="Operator" Key="ResponsiblePersonID" X="6" XSpan="3" Y="4" ItemKey="Operator">
                            <DataBinding ColumnKey="ResponsiblePersonID" TableKey="BK_ControllingArea"/>
                        </Dict>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="99px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="大猫" Key="DaMaoTab" Visible="DaMaoCheck">
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="物流" Key="WuliuTab" Visible="WuliuCheck">
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="集团财务" Key="CaiWuTab" Visible="CaiWuCheck">
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="电商" Key="DianShangTab" Visible="DianShangCheck">
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="HRP" Key="HRPTab" Visible="HRPCheck">
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="OA" Key="OATab" Visible="OACheck">
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="25%"/>
                            <ColumnDef Width="55px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Caption="系统信息" Key="SystemTab" Padding="10px">
                        <Dict Caption="创建人" ItemKey="Operator" Key="CREATOR" X="1" Y="0">
                            <DataBinding ColumnKey="CREATOR" DefaultFormulaValue="GetOperator()" TableKey="BK_ControllingArea"/>
                        </Dict>
                        <DatePicker Caption="创建时间" Key="CREATETIME" OnlyDate="false" X="1" Y="1">
                            <DataBinding ColumnKey="CREATETIME" DefaultFormulaValue="ServerDate()" TableKey="BK_ControllingArea"/>
                        </DatePicker>
                        <Dict Caption="修改人" ItemKey="Operator" Key="MODIFIER" X="1" Y="2">
                            <DataBinding ColumnKey="MODIFIER" DefaultFormulaValue="GetOperator()" TableKey="BK_ControllingArea"/>
                        </Dict>
                        <DatePicker Caption="修改时间" Key="MODIFYTIME" OnlyDate="false" X="1" Y="3">
                            <DataBinding ColumnKey="MODIFYTIME" DefaultFormulaValue="ServerDate()" TableKey="BK_ControllingArea"/>
                        </DatePicker>
                        <Label Caption="创建人" Key="L_CREATOR" X="0" Y="0"/>
                        <Label Caption="创建时间" Key="L_CREATETIME" X="0" Y="1"/>
                        <Label Caption="修改人" Key="L_MODIFIER" X="0" Y="2"/>
                        <Label Caption="修改时间" Key="L_MODIFYTIME" X="0" Y="3"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="100%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
                <SplitSize Size="55px"/>
                <SplitSize Size="100%"/>
            </SplitPanel>
        </Block>
    </Body>
</Form>
