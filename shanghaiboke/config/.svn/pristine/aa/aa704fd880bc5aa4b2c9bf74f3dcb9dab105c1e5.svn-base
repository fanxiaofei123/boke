<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="Annual_Budget" Caption="年度费用预算" FormType="Entity" ViewKey="Annual_BudgetView">
    <DataSource RefObjectKey="Annual_Budget"/>
    <OperationCollection>
        <Operation Key="WORKITEM" Caption="WORKITEM" Tag="WORKITEM"/>
        <Operation Key="BPM" Caption="BPM" Tag="BPM"/>
        <Operation Key="Save" Caption="保存" Visible="!ReadOnly()">
            <Action>
                <![CDATA[SaveData(); UpdateView();]]>
            </Action>
        </Operation>
        <Operation Key="New" Caption="新增" Visible="ReadOnly()">
            <Action>
                <![CDATA[New('Annual_Budget', 'self');]]>
            </Action>
        </Operation>
        <Operation Key="Edit" Caption="修改" Visible="ReadOnly()">
            <Action>
                <![CDATA[Edit();UpdateView();]]>
            </Action>
        </Operation>
        <Operation Key="Delete" Caption="删除" Visible="ReadOnly()">
            <Action>
                <![CDATA[DeleteData();UpdateView();Close();]]>
            </Action>
        </Operation>
        <Operation Key="Audit" Caption="审核" Visible="ReadOnly()"/>
        <Operation Key="Export" Caption="导出" Visible="ReadOnly()">
            <Action>
                <![CDATA[UIExportExcel()]]>
            </Action>
        </Operation>
        <Operation Key="Close" Caption="关闭界面" Visible="ReadOnly()">
            <Action>
                <![CDATA[Close()]]>
            </Action>
        </Operation>
        <Operation Key="Cancel" Caption="取消" Visible="!ReadOnly()">
            <Action>
                <![CDATA[Cancel();]]>
            </Action>
        </Operation>
    </OperationCollection>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板">
                    <TabPanel Height="100%" Key="DetailTab">
                        <GridLayoutPanel Key="first_head" Caption="基本信息">
                            <TextEditor Key="NO" Caption="单据编号" BuddyKey="Lab_NO" X="1" Y="0" Enable="false" AsQuery="true">
                                <DataBinding ColumnKey="NO" TableKey="Annual_Budget"/>
                            </TextEditor>
                            <Label Key="Lab_NO" Caption="单据编号" X="0" Y="0"/>
                            <DatePicker Key="BillDate" Caption="业务日期" BuddyKey="Lab_BillDate" X="4" Y="0" Enable="false">
                                <DataBinding ColumnKey="BillDate" TableKey="Annual_Budget"/>
                            </DatePicker>
                            <Label Key="Lab_BillDate" Caption="业务日期" X="3" Y="0"/>
                            <ComboBox Key="Status" Caption="单据状态" BuddyKey="Lab_Status" X="7" Y="0" Enable="false" SourceType="Status">
                                <DataBinding ColumnKey="Status" TableKey="Annual_Budget"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="单据状态" X="6" Y="0"/>
                            <TextEditor Key="BudgetaryName" Caption="预算名称" BuddyKey="Lab_BudgetaryName" X="1" Y="1">
                                <DataBinding ColumnKey="Budgetary_Name" TableKey="Annual_Budget"/>
                            </TextEditor>
                            <Label Key="Lab_BudgetaryName" Caption="预算名称" X="0" Y="1"/>
                            <TextEditor Key="BudgetDescription" Caption="预算描述" BuddyKey="Lab_BudgetDescription" X="4" Y="1">
                                <DataBinding ColumnKey="Budget_Description" TableKey="Annual_Budget"/>
                            </TextEditor>
                            <Label Key="Lab_BudgetDescription" Caption="预算描述" X="3" Y="1"/>
                            <ComboBox Key="BudgetaryClassiy" Caption="预算分类" BuddyKey="Lab_BudgetaryClassiy" X="7" Y="1">
                                <DataBinding ColumnKey="Budgetary_Classiy" TableKey="Annual_Budget"/>
                            </ComboBox>
                            <Label Key="Lab_BudgetaryClassiy" Caption="预算分类" X="6" Y="1"/>
                            <Dict Key="BudgetDepartment" Caption="预算部门" BuddyKey="Lab_BudgetDepartment" X="1" Y="2" ItemKey="CHCS_Department">
                                <DataBinding ColumnKey="Budget_Department" TableKey="Annual_Budget"/>
                            </Dict>
                            <Label Key="Lab_BudgetDepartment" Caption="预算部门" X="0" Y="2"/>
                            <DatePicker Key="TimeFrameStart" Caption="时间范围（起）" BuddyKey="Lab_TimeFrameStart" X="4" Y="2">
                                <DataBinding ColumnKey="Time_Frame_Start" TableKey="Annual_Budget"/>
                            </DatePicker>
                            <Label Key="Lab_TimeFrameStart" Caption="时间范围（起）" X="3" Y="2"/>
                            <DatePicker Key="TimeFrameStop" Caption="时间范围（终）" BuddyKey="Lab_TimeFrameStop" X="7" Y="2">
                                <DataBinding ColumnKey="Time_Frame_Stop" TableKey="Annual_Budget"/>
                            </DatePicker>
                            <Label Key="Lab_TimeFrameStop" Caption="时间范围（终）" X="6" Y="2"/>
                            <NumberEditor Key="TotalBudgetAmountTaxes" Caption="预算总金额（含税）" BuddyKey="Lab_TotalBudgetAmountTaxes" X="1" Y="3" Enable="false">
                                <DataBinding ColumnKey="Total_Budget_Amount_Taxes" TableKey="Annual_Budget"/>
                            </NumberEditor>
                            <Label Key="Lab_TotalBudgetAmountTaxes" Caption="预算总金额（含税）" X="0" Y="3"/>
                            <NumberEditor Key="TotalBudgetAmount" Caption="费用总金额（无税）" BuddyKey="Lab_TotalBudgetAmount" X="4" Y="3" Enable="false">
                                <DataBinding ColumnKey="Total_Budget_Amount" TableKey="Annual_Budget"/>
                            </NumberEditor>
                            <Label Key="Lab_TotalBudgetAmount" Caption="费用总金额（无税）" X="3" Y="3"/>
                            <NumberEditor Key="TotalAmount" Caption="总税额" BuddyKey="Lab_TotalAmount" X="7" Y="3" Enable="false">
                                <DataBinding ColumnKey="Total_Amount" TableKey="Annual_Budget"/>
                            </NumberEditor>
                            <Label Key="Lab_TotalAmount" Caption="总税额" X="6" Y="3"/>
                            <TextEditor Key="Remark" Caption="备注" BuddyKey="Lab_Remark" X="4" Y="4" XSpan="4">
                                <DataBinding ColumnKey="Remark" TableKey="Annual_Budget"/>
                            </TextEditor>
                            <Label Key="Lab_Remark" Caption="备注" X="3" Y="4"/>
                            <Dict Key="Staffing" Caption="编制人员" BuddyKey="Lab_Staffing" X="1" Y="4" ItemKey="LRP_Lookup_L">
                                <DataBinding ColumnKey="Staffing" TableKey="Annual_Budget"/>
                            </Dict>
                            <Label Key="Lab_Staffing" Caption="编制人员" X="0" Y="4"/>
                            <RowDefCollection RowGap="6" RowHeight="30">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="75px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="113px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="94px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="55px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="SystemInfo" Caption="系统信息">
                            <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator" XSpan="2">
                                <DataBinding ColumnKey="CREATOR" DefaultFormulaValue="GetOperator()" TableKey="Annual_Budget"/>
                            </Dict>
                            <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                            <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="5" Y="0" Enable="false" XSpan="2">
                                <DataBinding ColumnKey="CREATETIME" DefaultFormulaValue="ServerDate()" TableKey="Annual_Budget"/>
                            </DatePicker>
                            <Label Key="Lab_CREATETIME" Caption="创建时间" X="4" Y="0"/>
                            <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator" XSpan="2">
                                <DataBinding ColumnKey="MODIFIER" DefaultFormulaValue="GetOperator()" TableKey="Annual_Budget"/>
                            </Dict>
                            <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                            <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="5" Y="1" Enable="false" XSpan="2">
                                <DataBinding ColumnKey="MODIFYTIME" DefaultFormulaValue="ServerDate()" TableKey="Annual_Budget"/>
                            </DatePicker>
                            <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="4" Y="1"/>
                            <RowDefCollection RowGap="8">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="8">
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%" MinWidth="50px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <TabPanel Caption="TabPanel1" Key="TabPanel1">
                        <Grid Key="Grid1" Caption="年度费用预算明细表">
                            <GridColumnCollection>
                                <GridColumn Key="BudgetItems" Caption="预算项目" Width="80px"/>
                                <GridColumn Key="CostItem" Caption="费用项目" Width="80px"/>
                                <GridColumn Key="RouteTypeCode" Caption="航线分类" Width="80px"/>
                                <GridColumn Key="Route" Caption="航线" Width="80px"/>
                                <GridColumn Key="TaxRate" Caption="税率" Width="80px"/>
                                <GridColumn Key="CostBudgetTaxes" Caption="费用预算（含税）" Width="80px"/>
                                <GridColumn Key="CostBudget" Caption="费用预算（无税）" Width="80px"/>
                                <GridColumn Key="TaxAmount" Caption="税额" Width="80px"/>
                                <GridColumn Key="PerCapitaCostBudgetTaxes" Caption="人均费用预算（含税）" Width="80px"/>
                                <GridColumn Key="PerCapitaCostBudget" Caption="人均费用预算（无税）" Width="80px"/>
                            </GridColumnCollection>
                            <GridRowCollection>
                                <GridRow Key="row1" TableKey="Annual_Budget_L">
                                    <GridCell Key="BudgetItems" Caption="预算项目" CellType="Dict" ItemKey="CHCS_Budget_Items">
                                        <DataBinding ColumnKey="Budget_Items"/>
                                    </GridCell>
                                    <GridCell Key="CostItem" Caption="费用项目" CellType="Dict" ItemKey="LRP_Lookup_L">
                                        <DataBinding ColumnKey="Cost_Item"/>
                                    </GridCell>
                                    <GridCell Key="RouteTypeCode" Caption="航线分类" CellType="TextEditor">
                                        <DataBinding ColumnKey="Route_Type_Code"/>
                                    </GridCell>
                                    <GridCell Key="Route" Caption="航线" CellType="TextEditor">
                                        <DataBinding ColumnKey="Route"/>
                                    </GridCell>
                                    <GridCell Key="TaxRate" Caption="税率" CellType="NumberEditor">
                                        <DataBinding ColumnKey="Tax_Rate"/>
                                    </GridCell>
                                    <GridCell Key="CostBudgetTaxes" Caption="费用预算（含税）" CellType="NumberEditor">
                                        <DataBinding ColumnKey="Cost_Budget_Taxes"/>
                                    </GridCell>
                                    <GridCell Key="CostBudget" Caption="费用预算（无税）" CellType="NumberEditor">
                                        <DataBinding ColumnKey="Cost_Budget"/>
                                    </GridCell>
                                    <GridCell Key="TaxAmount" Caption="税额" CellType="NumberEditor">
                                        <DataBinding ColumnKey="Tax_Amount"/>
                                    </GridCell>
                                    <GridCell Key="PerCapitaCostBudgetTaxes" Caption="人均费用预算（含税）" CellType="NumberEditor">
                                        <DataBinding ColumnKey="Per_Capita_Cost_Budget_Taxes"/>
                                    </GridCell>
                                    <GridCell Key="PerCapitaCostBudget" Caption="人均费用预算（无税）" CellType="NumberEditor">
                                        <DataBinding ColumnKey="Per_Capita_Cost_Budget"/>
                                    </GridCell>
                                </GridRow>
                            </GridRowCollection>
                        </Grid>
                    </TabPanel>
                    <SplitSize Size="50%"/>
                    <SplitSize Size="50%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
