
.label {
	font-family: "思源黑体";
	font-size: 14px;
	color: #333;
	text-indent: 12px;
}
.button {
	font-family: "思源黑体";
	font-size: 13px; 
	color: #333;
	text-align: center;
	background-color:#ffffff;
	border-radius: 3px;
}
.button:active {
	background-color:#ffffff;
}
.button:disabled {
	background-color:#a7b8d1;
} 
.text-editor {
	font-family: "思源黑体";
	font-size: 13px;
	color:#333;
	
}

.text-area{
	padding:0 5px;
	font-family: "思源黑体";
	font-size: 13px;
	line-height: 18px;
	color:#999;
	border-style: solid;
	border-width: 1px;
	border-color:#78849a;
	border-radius: 3px;
}
.text-area:fcous{
	color:#333;
}
.text-area:disabled{
	color:#333;
	background-color: #f2f2f2;
	border-color:#abb7cd;
}
.text-area:required {
	background-image: textEditor_required.png;
	background-position: right top;
	border-color:#cf4925;
}
.password-editor{
	padding:0 5px;
	font-family: "思源黑体";
	font-size: 13px;
	color:#999;
	border-style: solid;
	border-width: 1px;
	border-color:#78849a;
	border-radius: 3px;
}
.password-editor:disabled{
	color:#999;
	background-color:#f2f2f2;
	border-color: #abb7cd;
}
.password-editor:focus{
	color: #333;
}

.password-editor-button{
	background-image:none;
	right: 0;
	top: 0;
}
.password-editor-msg{
	font-family: "思源黑体";
	font-size: 9px;
	color:#999;
}

.number-editor {
	font-family: "思源黑体";
	font-size: 13px;
	color:#333;
}
.check-box{
	button-image:image/checkbox.png;
}
.check-box:checked{
	button-image:image/checkbox_check.png;
}
.check-box:disabled{
	button-image:image/checkbox_disabled.png;
}
.check-box:checked:disabled{
	button-image:image/checkbox_checked_disabled.png;
}
.radio{
	button-image:image/radio.png;
}
.radio:checked{
	button-image:image/radio_checked.png;
}
.radio:disabled{
	button-image:image/radio_disabled.png;
}
.radio:checked:disabled{
	button-image:image/radio_checked_disabled.png;
}
.date-picker{
	font-family: "思源黑体";
	font-size: 13px;
	color:#333;
}
.date-picker:focus{
	border-color:#bbc1cc;
}
.date-picker:disabled{
	background-color: #ffffff;
}
.combo-box{
	font-family: "思源黑体";
	font-size: 14px;
	color: #333;
}
.combo-box-toggle{
	background-image:image/comboBox_up.png;
}
.combo-box:active{
	border-color: #bbc1cc;
}
.combo-box-list{
	border-style: solid;
	border-width: 1px;
	border-color:#78849a;
	border-radius: 3px;
}
.combo-box-option{
	padding: 0 10px;
	font-family: "思源黑体";
	font-size: 13px;
	color: #333;
	background-color:#fff;
}
.combo-box-option:active{
	background-color:#e6eaf2;
}

.tag-combo-item {
	border-style: solid;
	border-width: 1px;
	border-color: #999999;
	border-radius: 3px;
	color: #333;
	background-color: #fff;
	font-size: 14px;
}

.tag-combo-item:checked {
	border-color: #cc2a1e;
	color: #cc2a1e;
}

.image{
	background-color:#fff;
	text-align: center;
}
.container{

}
.tab-group{
	background-color:#ffffff;
}
.tab-group-item {
	font-family: "思源黑体";
	font-size: 15px;
	color: #4e595e;
	text-align:center;
}

.tab-group-item:checked{
    color: #f14c52;
}
.check-list-box{
	padding: 0 10px;
	font-family: "思源黑体";
	font-size: 13px;
	color: #333;
	background-image:image/comboBox_down.png;
	background-color:#fff;
	border-style: solid;
	border-width: 1px;
	border-color:#78849a;
	border-radius: 3px;
}
.check-list-box-toggle{
	background-image:image/comboBox_up.png;
}
.check-list-box:active{
	border-color: #bbc1cc;
}
.check-list-box:disabled{
	background-color:#ffffff;
}
.check-list-box-list{
	border-style: solid;
	border-width: 1px;
	border-color:#78849a;
	border-radius: 3px;
}
.check-list-box-option{
	padding: 0 10px;
	font-family: "思源黑体";
	font-size: 13px;
	color: #333;
	text-indent: 12px;
	background-color:#fff;
	button-image:image/checkbox.png; 
	background-position: 8px;
}
.check-list-box-option:active{
	background-color:#e6eaf2;
}
.check-list-box-option:checked{
	button-image:image/checkbox_checked.png; 
}
.tiled-list{
	background-color: #fff;
}
.tiled-list-cell{
	width: 25%;
	border-style: solid;
	border-width: 1px;
	border-color:#e6eaf2;
}
.listview{
	background-color: #fff;
}
.listview:active{
	background-color:#99b3ff;
}
.listview-splitline{
	bottom: 0;
	width: 100%;
	height: 1px;
	background-color: #abb7cd;
}
.rotater{
	background-color: #d9f2fd;
}
.rotater-indicator{
	left: 50%;
	width: 4px;
	height: 4px;
	border-radius: 50%;
	background-color: #e6eaf2;
}
.rotator-indicator-highlight{
	background-color: #008bff;
}
.water-fall{
	background-color: #e6eaf2;	
}
.rotator-list{
	background-color:#fff;
}
.rotator-list-cell{
	width: 25%;
	border-style: solid;
	border-width:1px;
	border-color:#e6eaf2;
}
.rotator-list-indicator{
	left: 50%;
	margin-left: 5px;
	width: 4px;
	height: 4px;
	border-radius: 50%;
	background-color: #e6eaf2;
}
.rotator-list-indicator-highlight{
	background-color: #008bff;
}
.table-view{
	font-family: "思源黑体";
	font-size: 15px;
	color: #666;
	text-indent: 12px;
	background-color:#fff;
}
.table-view:active{
	background-color:#dde0e7;
}
.table-view-title{
	font-family: "思源黑体";
	font-size: 15px;
	color: #666;
	text-align : left;
	text-indent: 12px;
	border-style: solid none;
	border-width: 1px;
	border-color:#b1bcd0;
	background-color:#f0f2f7;
}
.table-view-splitline{
	width: 100%;
	height: 1px;
	background-color: #b6c1d4;
}
.table-view-indicator{
	right: 22px;
	font-family: "思源黑体";
	font-size: 12px;
	font-style: normal;
	font-weight: normal;
	color: #03a9f4;
	line-height: 16px;
}
.gallery{
	font-family: "思源黑体";
	font-size: 13px;
	color: #333;
	text-align : left;
	background-color:#fff;
}
.dict{
	background-color: #fff;
}
.dict-button{
	font-family: "思源黑体";
	font-size: 13px;
	color: #03a9f4;
}
.dict-search-input{
	padding: 0 16px;
	font-family: "思源黑体";
	font-size: 13px;
	color: #333;
	text-indent: 4px;
	background-image: image/search_icon.png;
	background-position: 16px center;
	border-style: solid;
	border-width: 1px;
	border-color: #78849a;
	border-radius: 3px;
}
.dict-search-button{
	background-image: image/search_del_gray.png;
	right: 5px;
}
.dict-group-header{
	font-family: "思源黑体";
	font-size: 13px;
	color: #666;
	text-indent: 12px;
	border-style: solid none;
	border-width: 1px;
	border-color:#abb7cd;
}
.dict-group-rows{
	padding: 0 12px;
	font-family: "思源黑体";
	font-size: 16px;
	color: #070707;
}
.attachment{
	background-color: #fff;
	background-image:image/add_icon02.png; 
	background-position: center;
}
.attachment-pop{
	left: 50%;
	top: 50%;
	background-color: #fff;
}
.attachment-pop-button-upload{
	padding: 64px 0 0 0;
	left: 30%
	top: 20%;
	font-family: "思源黑体";
	font-size: 15px;
	color: #4e595e;
	text-align : center;
	background-image: image/attachment_upload.png;
	background-position: center top;
}
.attachment-pop-button-upload{
	padding: 64px 0 0 0;
	right: 30%
	top: 20%;
	font-family: "思源黑体";
	font-size: 15px;
	color: #4e595e;
	text-align : center;
	background-image: image/attachment_upload.png;
	background-position: center top;
}
.attachment-pop-button-close{
	left: 50%
	bottom: 10%;
	background-image: image/close_icon.png;
	background-position: center;
}
.stepper{
	background-color:#fff;
}
.stepper-input{
	padding:0 4px;
	font-family: "思源黑体";
	font-size: 13px;
	color: #333;
	text-align : center;
	border-style:solid;
	border-width:1px;
	border-color:#78849a;
}
.stepper-button-up{
	background-color:#fff;
	background-image:image/stepper_button_up.png;
	background-position: center;
	barder-radius: 0 3px 3px 0;
}
.stepper-button-down{
	background-color:#fff;
	background-image:image/stepper_button_down.png;
	background-position: center;
	barder-radius: 3px 0 0 3px;
}