<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Verdor_Contract" Caption="供应商合同" FormType="Entity">
    <DataSource RefObjectKey="CHCS_Verdor_Contract"/>
    <OperationCollection>
        <Operation Key="WORKITEM" Caption="WORKITEM" Tag="WORKITEM"/>
        <Operation Key="BPM" Caption="BPM" Tag="BPM"/>
        <Operation Key="Save" Caption="保存" Visible="!ReadOnly()">
            <Action>
                <![CDATA[SaveData(); UpdateView();]]>
            </Action>
        </Operation>
        <Operation Key="New" Caption="新增" Visible="ReadOnly()">
            <Action>
                <![CDATA[New('CHCS_Verdor_Contract', 'self');]]>
            </Action>
        </Operation>
        <Operation Key="Cancel" Caption="取消" Visible="!ReadOnly()">
            <Action>
                <![CDATA[Cancel();]]>
            </Action>
        </Operation>
        <Operation Key="Edit" Caption="编辑" Visible="ReadOnly()">
            <Action>
                <![CDATA[Edit();]]>
            </Action>
        </Operation>
        <Operation Key="Delete" Caption="删除" Visible="ReadOnly()">
            <Action>
                <![CDATA[DeleteData();UpdateView();Close();]]>
            </Action>
        </Operation>
    </OperationCollection>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板">
                    <TabPanel Caption="TabPanel1" Height="pref" Key="TabPanel1">
                        <GridLayoutPanel Key="Basic_Info" Height="pref" Padding="5px" OverflowY="Scroll" Caption="基本信息">
                            <TextEditor Key="Business_Unit" Caption="业务单元" BuddyKey="Lab_Business_Unit" X="1" Y="0" AsQuery="true">
                                <DataBinding ColumnKey="Business_Unit" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Business_Unit" Caption="业务单元" X="0" Y="0"/>
                            <TextEditor Key="Contract_Name" Caption="合同名称" BuddyKey="Lab_Contract_Name" X="4" Y="0" AsQuery="true">
                                <DataBinding ColumnKey="Contract_Name" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Name" Caption="合同名称" X="3" Y="0"/>
                            <TextEditor Key="Contract_Classify" Caption="合同分类" BuddyKey="Lab_Contract_Classify" X="7" Y="0" AsQuery="true">
                                <DataBinding ColumnKey="Contract_Classify" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Classify" Caption="合同分类" X="6" Y="0"/>
                            <TextEditor Key="Contract_Unit_Type_Select" Caption="签约单位类型选择" BuddyKey="Lab_Contract_Unit_Type_Select" X="1" Y="1">
                                <DataBinding ColumnKey="Contract_Unit_Type_Select" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Unit_Type_Select" Caption="签约单位类型选择" X="0" Y="1"/>
                            <TextEditor Key="Contract_Unit" Caption="签约单位" BuddyKey="Lab_Contract_Unit" X="4" Y="1" AsQuery="true">
                                <DataBinding ColumnKey="Contract_Unit" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Unit" Caption="签约单位" X="3" Y="1"/>
                            <Dict Key="Declaration_Department" Caption="申报部门" BuddyKey="Lab_Declaration_Department" X="7" Y="1" ItemKey="CHCS_Department">
                                <DataBinding ColumnKey="Declaration_Department" TableKey="CHCS_Verdor_Contract"/>
                            </Dict>
                            <Label Key="Lab_Declaration_Department" Caption="申报部门" X="6" Y="1"/>
                            <Dict Key="Currency" Caption="币种" BuddyKey="Lab_Currency" X="1" Y="2" ItemKey="Currency">
                                <DataBinding ColumnKey="Currency" TableKey="CHCS_Verdor_Contract"/>
                            </Dict>
                            <Label Key="Lab_Currency" Caption="币种" X="0" Y="2"/>
                            <ComboBox Key="Is_Money" Caption="是否涉及金额" BuddyKey="Lab_Is_Money" X="4" Y="2">
                                <DataBinding ColumnKey="Is_Money" TableKey="CHCS_Verdor_Contract"/>
                                <Item Caption="是" Key="0" Value="0"/>
                                <Item Caption="否" Key="1" Value="1"/>
                            </ComboBox>
                            <Label Key="Lab_Is_Money" Caption="是否涉及金额" X="3" Y="2"/>
                            <ComboBox Key="Is_Contract_Renewal" Caption="是否到期续签合同" BuddyKey="Lab_Is_Contract_Renewal" X="7" Y="2">
                                <DataBinding ColumnKey="Is_Contract_Renewal" TableKey="CHCS_Verdor_Contract"/>
                                <Item Caption="是" Key="0" Value="0"/>
                                <Item Caption="否" Key="1" Value="1"/>
                            </ComboBox>
                            <Label Key="Lab_Is_Contract_Renewal" Caption="是否到期续签合同" X="6" Y="2"/>
                            <TextEditor Key="Declaration_Reason" Caption="申报理由" BuddyKey="Lab_Declaration_Reason" X="1" Y="3" XSpan="7">
                                <DataBinding ColumnKey="Declaration_Reason" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Declaration_Reason" Caption="申报理由" X="0" Y="3"/>
                            <ComboBox Key="Contract_Type" Caption="合同类型" BuddyKey="Lab_Is_Money" X="1" Y="4" AsQuery="true">
                                <DataBinding ColumnKey="Contract_Type" TableKey="CHCS_Verdor_Contract"/>
                                <Item Caption="国内" Key="0" Value="国内"/>
                                <Item Caption="国际" Key="1" Value="国际"/>
                            </ComboBox>
                            <Label Key="Lab_Contract_Type" Caption="合同类型" X="0" Y="4"/>
                            <TextEditor Key="Paper_Countersign" Caption="纸质会签表会签" BuddyKey="Lab_Paper_Countersign" X="4" Y="4">
                                <DataBinding ColumnKey="Paper_Countersign" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Paper_Countersign" Caption="纸质会签表会签" X="3" Y="4"/>
                            <TextEditor Key="Is_Bidding" Caption="是否招投标" BuddyKey="Lab_Is_Bidding" X="7" Y="4">
                                <DataBinding ColumnKey="Is_Bidding" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Is_Bidding" Caption="是否招投标" X="6" Y="4"/>
                            <TextEditor Key="Applicant" Caption="申报人" BuddyKey="Lab_Applicant" X="1" Y="5">
                                <DataBinding ColumnKey="Applicant" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Applicant" Caption="申报人" X="0" Y="5"/>
                            <TextEditor Key="Applicant_Phone_Number" Caption="申报人电话" BuddyKey="Lab_Applicant_Phone_Number" X="4" Y="5">
                                <DataBinding ColumnKey="Applicant_Phone_Number" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Applicant_Phone_Number" Caption="申报人电话" X="3" Y="5"/>
                            <TextEditor Key="Contract_Manager" Caption="合同经办人" BuddyKey="Lab_Contract_Manager" X="1" Y="6">
                                <DataBinding ColumnKey="Contract_Manager" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Manager" Caption="合同经办人" X="0" Y="6"/>
                            <TextEditor Key="Contract_Manager_Phone_Number" Caption="合同经办人电话" BuddyKey="Lab_Contract_Manager_Phone_Number" X="4" Y="6">
                                <DataBinding ColumnKey="Contract_Manager_Phone_Number" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Manager_Phone_Number" Caption="合同经办人电话" X="3" Y="6"/>
                            <TextEditor Key="Principal_Attachment_Contract" Caption="主附合同" BuddyKey="Lab_Principal_Attachment_Contract" X="1" Y="7">
                                <DataBinding ColumnKey="Principal_Attachment_Contract" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Principal_Attachment_Contract" Caption="主附合同" X="0" Y="7"/>
                            <TextEditor Key="Principal_Contract" Caption="主合同" BuddyKey="Lab_Principal_Contract" X="4" Y="7">
                                <DataBinding ColumnKey="Principal_Contract" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Principal_Contract" Caption="主合同" X="3" Y="7"/>
                            <TextEditor Key="Principal_Contract_Code" Caption="主合同编号" BuddyKey="Lab_Principal_Contract_Code" X="7" Y="7" AsQuery="true">
                                <DataBinding ColumnKey="Principal_Contract_Code" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Principal_Contract_Code" Caption="主合同编号" X="6" Y="7"/>
                            <TextEditor Key="Principal_Contract_Name" Caption="主合同名称" BuddyKey="Lab_Principal_Contract_Name" X="1" Y="9">
                                <DataBinding ColumnKey="Principal_Contract_Name" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Principal_Contract_Name" Caption="主合同名称" X="0" Y="9"/>
                            <TextEditor Key="Related_Contract_Name" Caption="关联合同名称" BuddyKey="Lab_Related_Contract_Name" X="1" Y="10" AsQuery="true">
                                <DataBinding ColumnKey="Related_Contract_Name" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Related_Contract_Name" Caption="关联合同名称" X="0" Y="10"/>
                            <TextEditor Key="Contract_Code" Caption="合同编号" BuddyKey="Lab_Contract_Code" X="4" Y="10" AsQuery="true">
                                <DataBinding ColumnKey="Contract_Code" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Code" Caption="合同编号" X="3" Y="10"/>
                            <TextEditor Key="Contract_Code_Number" Caption="合同编号分号" BuddyKey="Lab_Contract_Code_Number" X="7" Y="10" AsQuery="true">
                                <DataBinding ColumnKey="Contract_Code_Number" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Contract_Code_Number" Caption="合同编号分号" X="6" Y="10"/>
                            <TextEditor Key="Signing_Contacts" Caption="签约联系人" BuddyKey="Lab_Signing_Contacts" X="1" Y="11">
                                <DataBinding ColumnKey="Signing_Contacts" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Signing_Contacts" Caption="签约联系人" X="0" Y="11"/>
                            <TextEditor Key="Signing_Telephone_Number" Caption="签约方电话" BuddyKey="Lab_Signing_Telephone_Number" X="4" Y="11">
                                <DataBinding ColumnKey="Signing_Telephone_Number" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Signing_Telephone_Number" Caption="签约方电话" X="3" Y="11"/>
                            <DatePicker Key="Effective_Date" Caption="生效日期" BuddyKey="Lab_Effective_Date" X="7" Y="11" AsQuery="true">
                                <DataBinding ColumnKey="Effective_Date" TableKey="CHCS_Verdor_Contract"/>
                            </DatePicker>
                            <Label Key="Lab_Effective_Date" Caption="生效日期" X="6" Y="11"/>
                            <DatePicker Key="Expiration_Date" Caption="到期日期" BuddyKey="Lab_Expiration_Date" X="1" Y="12" AsQuery="true">
                                <DataBinding ColumnKey="Expiration_Date" TableKey="CHCS_Verdor_Contract"/>
                            </DatePicker>
                            <Label Key="Lab_Expiration_Date" Caption="到期日期" X="0" Y="12"/>
                            <ComboBox Key="Is_Postpone_Clause" Caption="有无到期顺延条款" BuddyKey="Lab_Is_Postpone_Clause" X="4" Y="12">
                                <DataBinding ColumnKey="Is_Postpone_Clause" TableKey="CHCS_Verdor_Contract"/>
                                <Item Caption="有" Key="0" Value="0"/>
                                <Item Caption="无" Key="1" Value="1"/>
                            </ComboBox>
                            <Label Key="Lab_Is_Postpone_Clause" Caption="有无到期顺延条款" X="3" Y="12"/>
                            <TextEditor Key="Fixed_Contract_Period" Caption="固定合同期限" BuddyKey="Lab_Fixed_Contract_Period" X="7" Y="12">
                                <DataBinding ColumnKey="Fixed_Contract_Period" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Fixed_Contract_Period" Caption="固定合同期限" X="6" Y="12"/>
                            <TextEditor Key="Description_Of_Contract_Term" Caption="合同期限描述" BuddyKey="Lab_Description_Of_Contract_Term" X="1" Y="13" XSpan="7">
                                <DataBinding ColumnKey="Description_Of_Contract_Term" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Description_Of_Contract_Term" Caption="合同期限描述" X="0" Y="13"/>
                            <TextEditor Key="Supplier_NO" Caption="供应商编号" BuddyKey="Lab_Supplier_NO" X="1" Y="8">
                                <DataBinding ColumnKey="Supplier_NO" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Supplier_NO" Caption="供应商编号" X="0" Y="8"/>
                            <TextEditor Key="Supplier_Abbr" Caption="供应商简称" BuddyKey="Lab_Supplier_Abbr" X="7" Y="8">
                                <DataBinding ColumnKey="Supplier_Abbr" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Supplier_Abbr" Caption="供应商简称" X="6" Y="8"/>
                            <TextEditor Key="Supplier_Name" Caption="供应商名称" BuddyKey="Lab_Supplier_Name" X="4" Y="8" EmbedTextColor="">
                                <DataBinding ColumnKey="Supplier_Name" TableKey="CHCS_Verdor_Contract"/>
                            </TextEditor>
                            <Label Key="Lab_Supplier_Name" Caption="供应商名称" X="3" Y="8"/>
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="110px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="110px"/>
                                <ColumnDef Width="33%"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="110px"/>
                                <ColumnDef Width="34%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="Sys_Info" Height="pref" Padding="5px" Caption="系统信息">
                            <RowDefCollection RowHeight="30" RowGap="6">
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="20">
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="60px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <TabPanel Caption="TabPanel2" Key="TabPanel2">
                        <Grid Key="CHCS_Item_Management" Caption="条款管理">
                            <GridColumnCollection>
                                <GridColumn Key="column1" Caption="选择" Width="80px"/>
                                <GridColumn Key="Standard_Clause" Caption="标准条款" Width="80px"/>
                                <GridColumn Key="Deviation_Description" Caption="偏离描述" Width="80px"/>
                                <GridColumn Key="Lnfluence_Link" Caption="影响环节" Width="80px"/>
                            </GridColumnCollection>
                            <GridRowCollection>
                                <GridRow Key="row1" RowHeight="36" TableKey="CHCS_Item_Management">
                                    <GridCell Key="select1" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                    <GridCell Key="Standard_Clause" Caption="标准条款" CellType="TextEditor">
                                        <DataBinding ColumnKey="Standard_Clause"/>
                                    </GridCell>
                                    <GridCell Key="Deviation_Description" Caption="偏离描述" CellType="TextEditor">
                                        <DataBinding ColumnKey="Deviation_Description"/>
                                    </GridCell>
                                    <GridCell Key="Lnfluence_Link" Caption="影响环节" CellType="TextEditor">
                                        <DataBinding ColumnKey="Lnfluence_Link"/>
                                    </GridCell>
                                </GridRow>
                            </GridRowCollection>
                        </Grid>
                        <Grid Key="CHCS_Contract_Price" Caption="合同价格">
                            <GridColumnCollection>
                                <GridColumn Key="select2" Caption="选择" Width="80px"/>
                                <GridColumn Key="Product" Caption="餐种" Width="100px"/>
                                <GridColumn Key="Unit_Price" Caption="单价" Width="100px"/>
                                <GridColumn Key="Valid_Time_Start" Caption="有效期起" Width="100px"/>
                                <GridColumn Key="Valid_Time_End" Caption="有效期止" Width="100px"/>
                            </GridColumnCollection>
                            <GridRowCollection>
                                <GridRow Key="row1" RowHeight="36" TableKey="CHCS_Contract_Price">
                                    <GridCell Key="select2" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                    <GridCell Key="Product" Caption="餐种" CellType="Dict" ItemKey="CHCS_Product">
                                        <DataBinding ColumnKey="Product"/>
                                    </GridCell>
                                    <GridCell Key="Unit_Price" Caption="单价" CellType="NumberEditor" Format="">
                                        <DataBinding ColumnKey="Unit_Price"/>
                                    </GridCell>
                                    <GridCell Key="Valid_Time_Start" Caption="有效期起" CellType="DatePicker" OnlyDate="true" Format="yyyy-MM-dd">
                                        <DataBinding ColumnKey="Valid_Time_Start"/>
                                    </GridCell>
                                    <GridCell Key="Valid_Time_End" Caption="有效期止" CellType="DatePicker" OnlyDate="true" Format="yyyy-MM-dd">
                                        <DataBinding ColumnKey="Valid_Time_End"/>
                                    </GridCell>
                                </GridRow>
                            </GridRowCollection>
                        </Grid>
                    </TabPanel>
                    <SplitSize Size="65%"/>
                    <SplitSize Size="35%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
