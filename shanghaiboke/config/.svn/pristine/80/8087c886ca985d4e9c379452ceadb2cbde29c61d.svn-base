<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Flight" Caption="航班" FormType="ChainDict">
    <DataSource RefObjectKey="CHCS_Flight"/>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Description="载入事件" Range="Action" Verb="Load">
            <![CDATA[LoadData()]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main" Height="100%">
                <SplitPanel Key="SplitPanel1" Orientation="Vertical" Height="100%" Caption="SplitPanel1">
                    <TabPanel Caption="TabPanel0" Key="TabPanel0" Height="pref">
                        <GridLayoutPanel Caption="基本信息" Key="BasicTab" Padding="10px">
                            <TextEditor BuddyKey="L_Code" Caption="代码" Enable="!ReadOnly()" Key="Code" Tip="「代码」不可为空" X="1" XSpan="3" Y="0">
                                <DataBinding ColumnKey="Code" TableKey="CHCS_Flight" Required="true"/>
                            </TextEditor>
                            <TextEditor Key="FLIGHT_NO" Caption="航班号" BuddyKey="Lab_FLIGHT_NO" X="6" Y="0" Enable="!ReadOnly()" Tip="「机号」不可为空" XSpan="3">
                                <DataBinding ColumnKey="FLIGHT_NO" TableKey="CHCS_Flight" Required="true"/>
                            </TextEditor>
                            <Label Caption="代码" Key="L_Code" X="0" Y="0"/>
                            <Label Key="Lab_FLIGHT_NO" Caption="航班号" X="5" Y="0"/>
                            <Dict Key="Aircraft_Type_Number" Caption="机型" BuddyKey="Lab_Aircraft_Type_Number" X="1" Y="1" ItemKey="CHCS_Aircraft_Type" XSpan="3">
                                <DataBinding ColumnKey="Aircraft_Type_Number" TableKey="CHCS_Flight"/>
                            </Dict>
                            <Label Key="Lab_Aircraft_Type_Number" Caption="机型" X="0" Y="1"/>
                            <TextEditor Key="Remark" Caption="备注" BuddyKey="Lab_Remark" X="6" Y="1" XSpan="3">
                                <DataBinding ColumnKey="Remark" TableKey="CHCS_Flight"/>
                            </TextEditor>
                            <Label Key="Lab_Remark" Caption="备注" X="5" Y="1"/>
                            <RowDefCollection RowGap="8">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="8">
                                <ColumnDef Width="78px"/>
                                <ColumnDef Width="25%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="25%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="94px"/>
                                <ColumnDef Width="25%"/>
                                <ColumnDef Width="55px"/>
                                <ColumnDef Width="25%"/>
                                <ColumnDef Width="55px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="SysInfo" Padding="10px" Caption="系统信息">
                            <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="CREATOR" TableKey="CHCS_Flight"/>
                            </Dict>
                            <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                            <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                                <DataBinding ColumnKey="CREATETIME" TableKey="CHCS_Flight"/>
                            </DatePicker>
                            <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                            <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="MODIFIER" TableKey="CHCS_Flight"/>
                            </Dict>
                            <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                            <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                                <DataBinding ColumnKey="MODIFYTIME" TableKey="CHCS_Flight"/>
                            </DatePicker>
                            <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                            <RowDefCollection RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <TabPanel Caption="航段定义" Key="TabPanel1" Height="100%">
                        <Grid Key="Grid1" Caption="航段定义" Height="100%">
                            <GridColumnCollection>
                                <GridColumn Key="Route_Departure_Airport_Code" Caption="起飞站" Width="80px"/>
                                <GridColumn Key="Route_Arrival_Airport_Code" Caption="到达站" Width="80px"/>
                                <GridColumn Key="Flight_Service_Standard" Caption="航班服务标准" Width="96px"/>
                                <GridColumn Key="Departure" Caption="偏离飞行时长(min)" Width="120px"/>
                            </GridColumnCollection>
                            <GridRowCollection>
                                <GridRow Key="row1" RowHeight="36" TableKey="CHCS_Flight_L">
                                    <GridCell Key="Route_Departure_Airport_Code" Caption="起飞站" CellType="Dict" ItemKey="CHCS_Airport">
                                        <DataBinding ColumnKey="Route_Departure_Airport_Code"/>
                                    </GridCell>
                                    <GridCell Key="Route_Arrival_Airport_Code" Caption="到达站" CellType="Dict" ItemKey="CHCS_Airport">
                                        <DataBinding ColumnKey="Route_Arrival_Airport_Code"/>
                                    </GridCell>
                                    <GridCell Key="Flight_Service_Standard" Caption="航班服务标准" CellType="Dict" ItemKey="LRP_Lookup_L">
                                        <DataBinding ColumnKey="Flight_Service_Standard"/>
                                        <ItemFilter>
                                            <Filter Query="select oid  from LRP_Lookup_L where HeadCode='HBFWBZ'" Type="DataSet"/>
                                        </ItemFilter>
                                    </GridCell>
                                    <GridCell Key="Departure" Caption="偏离" CellType="NumberEditor" Format="">
                                        <DataBinding ColumnKey="Departure"/>
                                    </GridCell>
                                </GridRow>
                            </GridRowCollection>
                        </Grid>
                    </TabPanel>
                    <SplitSize Size="40%"/>
                    <SplitSize Size="60%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
