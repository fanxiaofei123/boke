<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form AbbrCaption="退厂订单序事簿" Caption="退厂订单序事簿" FormType="View" Key="V_BackNoticeView">
    <DataSource>
        <DataObject Caption="退厂订单序事簿" Key="V_BackNoticeView" PrimaryTableKey="LRP_BackNotice_H">
            <TableCollection>
                <Table Key="LRP_BackNotice_H" Persist="false" SourceType="Query" TableMode="Detail">
                    <Column Caption="对象标识" DBColumnName="OID" DataType="Long" Key="OID"/>
                    <Column Caption="父对象标识" DBColumnName="POID" DataType="Long" Key="POID"/>
                    <Column Caption="主对象标识" DBColumnName="SOID" DataType="Long" Key="SOID"/>
                    <Column Caption="对象版本" DBColumnName="VERID" DataType="Integer" Key="VERID"/>
                    <Column Caption="对象明细版本" DBColumnName="DVERID" DataType="Integer" Key="DVERID"/>
                    <Column Caption="状态" DataType="Integer" Key="Status"/>
                    <Column Caption="创建人" DataType="Long" Key="CREATOR"/>
                    <Column Caption="创建时间" DataType="DateTime" Key="CREATETIME"/>
                    <Column Caption="修改人" DataType="Long" Key="MODIFIER"/>
                    <Column Caption="修改时间" DataType="DateTime" Key="MODIFYTIME"/>
                    <Column Caption="通知单号" DataType="Varchar" Key="No" Length="100" SortType="Desc"/>
                    <Column Caption="货主单号" DataType="Varchar" Key="OwnerNo" Length="100"/>
                    <Column Caption="仓储中心id" DataType="Long" Key="WarehouseCenterId"/>
                    <Column Caption="货主id" DataType="Long" Key="OwnerId"/>
                    <Column Caption="预计发货日期时间起" DataType="DateTime" Key="ExpectDateTimeFrom"/>
                    <Column Caption="预计发货日期时间至" DataType="DateTime" Key="ExpectDateTimeTo"/>
                    <Column Caption="收货方" DataType="Long" Key="RecvPartyId"/>
                    <Column Caption="收货方名称" DataType="Varchar" Key="RecvPartyName" Length="100"/>
                    <Column Caption="省" DataType="Long" Key="ProvinceId"/>
                    <Column Caption="市" DataType="Long" Key="CityId"/>
                    <Column Caption="区" DataType="Long" Key="DistrictId"/>
                    <Column Caption="地址" DataType="Varchar" Key="Address" Length="100"/>
                    <Column Caption="联系人" DataType="Varchar" Key="Contactor" Length="100"/>
                    <Column Caption="联系人电话" DataType="Varchar" Key="ContactTel" Length="100"/>
                    <Column Caption="联系人电邮" DataType="Varchar" Key="ContactEmail" Length="100"/>
                    <Column Caption="承运方" DataType="Long" Key="CarrierId"/>
                    <Column Caption="承运方联系人" DataType="Varchar" Key="CarrierContactor" Length="100"/>
                    <Column Caption="承运方联系电话" DataType="Varchar" Key="CarrierTel" Length="100"/>
                    <Column Caption="运单号" DataType="Varchar" Key="WaybillNo" Length="100"/>
                    <Column Caption="备注" DataType="Varchar" Key="Remark" Length="1000"/>
                </Table>
            </TableCollection>
            <OIDFilter/>
        </DataObject>
    </DataSource>
    <OperationCollection>
        <Operation Caption="新增" Icon="new.png" Key="New">
            <Action>
                <![CDATA[
				New('V_BackNotice')	
			]]>
            </Action>
        </Operation>
        <Operation Caption="确认" Key="BatchConfirm" RefKey="BatchConfirm"/>
        <Operation Caption="撤销确认" Key="BatchConfirm-R" RefKey="BatchConfirm-R"/>
        <Operation Caption="作废" Key="BatchCancel" RefKey="BatchCancel"/>
        <Operation Caption="撤销作废" Key="BatchCancel-R" RefKey="BatchCancel-R"/>
        <Operation Caption="完成" Key="BatchFinish" RefKey="BatchFinish"/>
        <Operation Caption="撤销完成" Key="BatchFinish-R" RefKey="BatchFinish-R"/>
        <Operation Caption="删除" Key="BatchDelete" RefKey="BatchDelete"/>
        <Operation Caption="下推拣货单" Key="BatchMapPick">
            <Action>
                <![CDATA[
					if(CheckListNoSelect()){
						Confirm(LocaleString('OwnerNull','key108'),'OK','OK:{}');
					}
					else{
						if(CheckListSelectStatus(',200,500,')){
							var msg = InvokeService("LRP_BatchMap", false, false,"BackNotice-Tx205","LRP_BackNotice",GetListViewSelectID());
							if(Length(msg))
								{Confirm(msg,'OK','OK:{}');}
							else
								{Confirm(LocaleString('OwnerNull','key109'),'OK','OK:{}');}
						}
						else
						{ Confirm(LocaleString('OUTshow','outkey3'),'OK','OK:{}');}
					}
				]]>
            </Action>
        </Operation>
        <Operation Caption="关闭界面" Key="Close" RefKey="Close"/>
    </OperationCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="main">
                <ToolBar Height="pref" Key="main_toolbar">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Height="100%" Key="main_split" Orientation="Vertical">
                    <GridLayoutPanel Caption="GridLayoutPanel0" Key="GridLayoutPanel0" Padding="10px">
                        <TextEditor Caption="DataObjectKey" Enable="false" Key="DataObjectKey" Visible="false" X="0" Y="0">
                            <DataBinding DefaultValue="LRP_BackNotice"/>
                        </TextEditor>
                        <TextEditor Caption="退厂订单号" Key="No_C" X="1" XSpan="2" Y="0">
                            <Condition CondSign="like">
                                <ConditionTarget TableKey="LRP_BackNotice_H" ColumnKey="No"/>
                            </Condition>
                        </TextEditor>
                        <Label Caption="退厂订单号" Key="L_No" X="0" Y="0"/>
                        <Dict AllowMultiSelection="true" Caption="货主" ItemKey="LRP_Owner" Key="OwnerId_C" X="5" XSpan="2" Y="0">
                            <Condition CondSign="in">
                                <ConditionTarget TableKey="LRP_BackNotice_H" ColumnKey="OwnerId"/>
                            </Condition>
                        </Dict>
                        <Label Caption="货主" Key="L_OwnerId" X="4" Y="0"/>
                        <TextEditor Caption="货主单号" Key="OwnerNo_C" X="1" XSpan="2" Y="1">
                            <Condition CondSign="like">
                                <ConditionTarget TableKey="LRP_BackNotice_H" ColumnKey="OwnerNo"/>
                            </Condition>
                        </TextEditor>
                        <Label Caption="货主单号" Key="L_OwnerNo" X="0" Y="1"/>
                        <CheckListBox Caption="状态" Key="Status_C" X="5" XSpan="2" Y="1">
                            <Condition CondSign="in">
                                <ConditionTarget TableKey="LRP_BackNotice_H" ColumnKey="Status"/>
                            </Condition>
                            <Item Caption="已输入" Key="prepared" Value="100"/>
                            <Item Caption="已确认" Key="confirmed" Value="200"/>
                            <Item Caption="处理中" Key="handled" Value="500"/>
                            <Item Caption="已完成" Key="finished" Value="900"/>
                        </CheckListBox>
                        <Label Caption="状态" Key="L_Status" X="4" Y="1"/>
                        <Button Caption="查询" Key="Query" X="8" Y="0">
                            <OnClick>
                                <![CDATA[DealCondition();LoadData();ShowData();]]>
                            </OnClick>
                        </Button>
                        <Button Caption="重置" Key="Reset" X="8" Y="1">
                            <OnClick>
                                <![CDATA[ResetCondition();]]>
                            </OnClick>
                        </Button>
                        <DatePicker Caption="创建日期时间" Key="CREATETIME_C" X="1" XSpan="2" Y="2">
                            <DataBinding DefaultFormulaValue="Format(ServerDate(),'yyyy-MM-dd')&amp;' 00:00:00'"/>
                            <Condition CondSign="between" Group="CREATETIME" GroupHead="true">
                                <ConditionTarget TableKey="LRP_BackNotice_H" ColumnKey="CREATETIME"/>
                            </Condition>
                        </DatePicker>
                        <Label Caption="创建日期时间" Key="L_CREATETIME" X="0" Y="2"/>
                        <DatePicker Caption="至" Key="CREATETIME_C_1" X="5" XSpan="2" Y="2">
                            <DataBinding DefaultFormulaValue="Format(ServerDate(),'yyyy-MM-dd')&amp;' 23:59:59'"/>
                            <Condition CondSign="none" Group="CREATETIME" GroupTail="true">
                                <ConditionTarget TableKey="LRP_BackNotice_H" ColumnKey="CREATETIME"/>
                            </Condition>
                        </DatePicker>
                        <Label Caption="至" Key="L_CREATETIME_1" X="4" Y="2"/>
                        <NumberEditor Caption="仓储中心" Key="WarehouseCenterId_C" X="1" XSpan="2" Y="3" Visible="false">
                            <DataBinding DefaultFormulaValue="getCurOrgID()"/>
                            <Condition CondSign="=">
                                <ConditionTarget TableKey="LRP_BackNotice_H" ColumnKey="WarehouseCenterId"/>
                            </Condition>
                        </NumberEditor>
                        <Label Caption="仓储中心" Key="L_WarehouseCenterId" X="0" Y="3" Visible="false"/>
                        <RowDefCollection RowGap="8">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="8">
                            <ColumnDef Width="77px"/>
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%" MinWidth="50px"/>
                            <ColumnDef Width="30px"/>
                            <ColumnDef Width="55px"/>
                            <ColumnDef Width="100px"/>
                            <ColumnDef Width="50%" MinWidth="50px"/>
                            <ColumnDef Width="30px"/>
                            <ColumnDef Width="80px"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <ListView Caption="ListView0" Key="ListView0" TableKey="LRP_BackNotice_H" PageLoadType="DB" PageRowCount="10">
                        <ListViewColumnCollection>
                            <ListViewColumn Caption="对象标识" ColumnType="TextEditor" DataColumnKey="OID" Key="OID" Visible="false"/>
                            <ListViewColumn Caption="选择" ColumnType="CheckBox" Enable="true" IsSelect="true" Key="Select" ValueChanged="RefreshOperation();"/>
                            <ListViewColumn Caption="状态" ColumnType="ComboBox" DataColumnKey="Status" Key="Status" SourceType="Status"/>
                            <ListViewColumn Caption="退厂订单号" ColumnType="HyperLink" DataColumnKey="No" Key="No">
                                <OnClick>
                                    <![CDATA[
									
									Open('V_BackNotice', OID);
									]]>
                                </OnClick>
                            </ListViewColumn>
                            <ListViewColumn Caption="货主" ColumnType="Dict" DataColumnKey="OwnerId" ItemKey="LRP_Owner" Key="OwnerId"/>
                            <ListViewColumn Caption="货主单号" ColumnType="TextEditor" DataColumnKey="OwnerNo" Key="OwnerNo"/>
                            <ListViewColumn Caption="创建日期时间" ColumnType="DatePicker" DataColumnKey="CREATETIME" Key="CREATETIME"/>
                            <ListViewColumn Caption="预计发货日期时间起" ColumnType="DatePicker" DataColumnKey="ExpectDateTimeFrom" Key="ExpectDateTimeFrom" OnlyDate="true"/>
                            <ListViewColumn Caption="预计发货日期时间至" ColumnType="DatePicker" DataColumnKey="ExpectDateTimeTo" Key="ExpectDateTimeTo" OnlyDate="true"/>
                            <ListViewColumn Caption="收货方名称" ColumnType="TextEditor" DataColumnKey="RecvPartyName" Key="RecvPartyName"/>
                            <ListViewColumn Caption="省" ColumnType="Dict" DataColumnKey="ProvinceId" ItemKey="LRP_Province" Key="ProvinceId"/>
                            <ListViewColumn Caption="市" ColumnType="Dict" DataColumnKey="CityId" ItemKey="LRP_City" Key="CityId"/>
                            <ListViewColumn Caption="区" ColumnType="Dict" DataColumnKey="DistrictId" ItemKey="LRP_District" Key="DistrictId"/>
                            <ListViewColumn Caption="货承运方" ColumnType="Dict" DataColumnKey="CarrierId" ItemKey="LRP_Carrier" Key="CarrierId"/>
                            <ListViewColumn Caption="备注" ColumnType="TextEditor" DataColumnKey="Remark" Key="Remark"/>
                        </ListViewColumnCollection>
                        <RowDblClick>
                            <![CDATA[
								Open('V_BackNotice', OID);
						]]>
                        </RowDblClick>
                    </ListView>
                    <SplitSize Size="130px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[]]>
    </OnLoad>
</Form>
