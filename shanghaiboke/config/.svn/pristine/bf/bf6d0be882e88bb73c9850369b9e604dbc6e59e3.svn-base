<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Aircrew_Meal_Order" Caption="机组点餐单" FormType="Entity" ViewKey="CHCS_Aircrew_Meal_OrderView">
    <DataSource RefObjectKey="CHCS_Aircrew_Meal_Order"/>
    <OperationCollection>
        <Operation Key="WORKITEM" Caption="WORKITEM" Tag="WORKITEM"/>
        <Operation Key="BPM" Caption="BPM" Tag="BPM"/>
        <Operation Key="Save" Caption="保存" Visible="!ReadOnly()">
            <Action>
                <![CDATA[SaveData(); UpdateView();]]>
            </Action>
        </Operation>
        <Operation Key="New" Caption="新增" Visible="ReadOnly()">
            <Action>
                <![CDATA[New('CHCS_Aircrew_Meal_Order', 'self');]]>
            </Action>
        </Operation>
        <Operation Key="Edit" Caption="编辑" Visible="ReadOnly()">
            <Action>
                <![CDATA[Edit();]]>
            </Action>
        </Operation>
        <Operation Key="Cancel" Caption="取消" Visible="!ReadOnly()">
            <Action>
                <![CDATA[Cancel();]]>
            </Action>
        </Operation>
        <Operation Key="Close" Caption="关闭" Visible="ReadOnly()">
            <Action>
                <![CDATA[Close()]]>
            </Action>
        </Operation>
        <Operation Key="confirm" Caption="确认"/>
    </OperationCollection>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板" Padding="10px">
                    <TabPanel Caption="TabPanel1" Height="pref" Key="TabPanel1">
                        <GridLayoutPanel Key="first_head" Height="pref" Padding="5px" OverflowY="Hidden" Caption="基本信息">
                            <TextEditor Key="NO" Caption="单据编号" BuddyKey="Lab_NO" X="1" Y="0" Enable="false" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="NO"/>
                            </TextEditor>
                            <Label Key="Lab_NO" Caption="单据编号" X="0" Y="0"/>
                            <DatePicker Key="BillDate" Caption="单据日期" BuddyKey="Lab_BillDate" X="5" Y="0" Enable="false" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="BillDate"/>
                                <Format/>
                            </DatePicker>
                            <Label Key="Lab_BillDate" Caption="单据日期" X="4" Y="0"/>
                            <ComboBox Key="Status" Caption="状态" BuddyKey="Lab_Status" X="9" Y="0" Enable="false" SourceType="Status" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Status"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="状态" X="8" Y="0"/>
                            <Dict Key="Flight_Number" Caption="航班号" BuddyKey="Lab_Flight_Number" X="1" Y="1" ItemKey="CHCS_Flight" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Flight_Number"/>
                            </Dict>
                            <Label Key="Lab_Flight_Number" Caption="航班号" X="0" Y="1"/>
                            <DatePicker Key="FlightDate" Caption="航班日期" BuddyKey="Lab_FlightDate" X="5" Y="1" OnlyDate="true" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="FlightDate"/>
                                <Format>
                                    <Font Italic="true"/>
                                </Format>
                            </DatePicker>
                            <Label Key="Lab_FlightDate" Caption="航班日期" X="4" Y="1"/>
                            <Dict Key="Aircraft_Number" Caption="机号" BuddyKey="Lab_Aircraft_Number" X="9" Y="1" ItemKey="CHCS_Aircraft" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Aircraft_Number"/>
                            </Dict>
                            <Label Key="Lab_Aircraft_Number" Caption="机号" X="8" Y="1"/>
                            <Dict Key="Aircraft_Type_Number" Caption="机型" BuddyKey="Lab_Aircraft_Type_Number" X="1" Y="2" ItemKey="CHCS_Aircraft_Type" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Aircraft_Type_Number"/>
                            </Dict>
                            <Label Key="Lab_Aircraft_Type_Number" Caption="机型" X="0" Y="2"/>
                            <Dict Key="Department" Caption="部门" BuddyKey="Lab_Department" X="5" Y="3" ItemKey="CHCS_Department" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Department" DefaultFormulaValue="GetDictOID(&quot;CHCS_Department&quot;, Name, &quot;Department&quot;)"/>
                            </Dict>
                            <Label Key="Lab_Department" Caption="部门" X="4" Y="3"/>
                            <TextEditor Key="Manpower_System_Number" Caption="人力系统编号" BuddyKey="Lab_Manpower_System_Number" X="9" Y="4" XSpan="2">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Manpower_System_Number"/>
                            </TextEditor>
                            <Label Key="Lab_Manpower_System_Number" Caption="人力系统编号" X="8" Y="4"/>
                            <Dict Key="Name" Caption="姓名" BuddyKey="Lab_Name" X="1" Y="3" ItemKey="CHCS_Employee" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Name"/>
                            </Dict>
                            <Label Key="Lab_Name" Caption="姓名" X="0" Y="3"/>
                            <TextEditor Key="Phone_Nmber" Caption="电话" BuddyKey="Lab_Phone_Nmber" X="1" Y="4" XSpan="2">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Phone_Nmber"/>
                            </TextEditor>
                            <Label Key="Lab_Phone_Nmber" Caption="电话" X="0" Y="4"/>
                            <Dict Key="Point_Food" Caption="所点餐种" BuddyKey="Lab_Point_Food" X="5" Y="2" ItemKey="CHCS_Product" XSpan="2">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Point_Food"/>
                                <ItemFilter>
                                    <Filter Query="select p.OID from CHCS_Product p left join lrp_lookup_l l on p.Type=l.OID where l.Code=upper(?)" Type="DataSet">
                                        <FilterValue DataType="Varchar" ParaValue="CPLX-CZ" Type="Const"/>
                                    </Filter>
                                </ItemFilter>
                            </Dict>
                            <Label Key="Lab_Point_Food" Caption="所点餐种" X="4" Y="2"/>
                            <Dict Key="Point_Dish" Caption="所点菜品" BuddyKey="Lab_Point_Dish" X="9" Y="2" ItemKey="CHCS_Product" XSpan="2">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Point_Dish"/>
                                <ItemFilter>
                                    <Filter Query="select p.OID from CHCS_Product p left join lrp_lookup_l l on p.Type=l.OID where l.Code=upper(?)" Type="DataSet">
                                        <FilterValue DataType="Varchar" ParaValue="CPLX-DP" Type="Const"/>
                                    </Filter>
                                </ItemFilter>
                            </Dict>
                            <Label Key="Lab_Point_Dish" Caption="所点菜品" X="8" Y="2"/>
                            <Dict Key="Waste_Cause" Caption="作废原因" BuddyKey="Lab_Waste_Cause" X="1" Y="5" ItemKey="LRP_Lookup_L" XSpan="2">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Waste_Cause"/>
                                <ItemFilter>
                                    <Filter Query="select OID from lrp_lookup_l where upper(HeadCode)=upper(?)" Type="DataSet">
                                        <FilterValue DataType="Varchar" ParaValue="DCQXYY" Type="Const"/>
                                    </Filter>
                                </ItemFilter>
                            </Dict>
                            <Label Key="Lab_Waste_Cause" Caption="作废原因" X="0" Y="5"/>
                            <DatePicker Key="Waste_Time" Caption="作废时间" BuddyKey="Lab_Waste_Time" X="5" Y="5" XSpan="2">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Waste_Time"/>
                            </DatePicker>
                            <Label Key="Lab_Waste_Time" Caption="作废时间" X="4" Y="5"/>
                            <TextEditor Key="PCODE" Caption="PCODE " BuddyKey="Lab_PCODE" X="9" Y="5" XSpan="2">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="PCODE"/>
                            </TextEditor>
                            <Label Key="Lab_PCODE" Caption="PCODE " X="8" Y="5"/>
                            <TextEditor Key="Remark" Caption="备注" BuddyKey="Lab_Remark" X="1" Y="6" XSpan="10">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Remark"/>
                            </TextEditor>
                            <Label Key="Lab_Remark" Caption="备注" X="0" Y="6"/>
                            <Dict Key="Personnel_Sort" Caption="人员类别" BuddyKey="Lab_Personnel_Sort" X="9" Y="3" ItemKey="LRP_Lookup_L" XSpan="2" AsQuery="true">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Personnel_Sort" DefaultFormulaValue="GetDictValue(&quot;CHCS_Employee&quot;, Name, &quot;Post&quot;)"/>
                                <ItemFilter>
                                    <Filter Query="select OID from lrp_lookup_l where upper(HeadCode)=upper(?)" Type="DataSet">
                                        <FilterValue DataType="Varchar" ParaValue="RYZW" Type="Const"/>
                                    </Filter>
                                </ItemFilter>
                            </Dict>
                            <Label Key="Lab_Personnel_Sort" Caption="人员类别" X="8" Y="3"/>
                            <TextEditor Key="Staff_Positions" Caption="人员机上岗位" BuddyKey="Lab_Staff_Positions" X="5" Y="4" XSpan="2">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="Staff_Positions"/>
                            </TextEditor>
                            <Label Key="Lab_Staff_Positions" Caption="人员机上岗位" X="4" Y="4"/>
                            <RowDefCollection RowGap="5" RowHeight="30">
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="SystemInfo" OverflowY="Hidden" Caption="系统信息">
                            <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="CREATOR"/>
                            </Dict>
                            <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                            <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="MODIFIER"/>
                            </Dict>
                            <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                            <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="CREATETIME"/>
                            </DatePicker>
                            <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                            <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                                <DataBinding TableKey="CHCS_Aircrew_Meal_Order" ColumnKey="MODIFYTIME"/>
                            </DatePicker>
                            <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                            <RowDefCollection RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <SplitSize Size="100%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
