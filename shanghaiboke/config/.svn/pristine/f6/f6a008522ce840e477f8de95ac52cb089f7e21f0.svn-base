                            </DatePicker>
                            <Label Key="Lab_BillDate" Caption="单据日期" X="2" Y="0"/>
                            <ComboBox Key="Status" Caption="状态" BuddyKey="Lab_Status" X="5" Y="0" Enable="false" AsQuery="true">
