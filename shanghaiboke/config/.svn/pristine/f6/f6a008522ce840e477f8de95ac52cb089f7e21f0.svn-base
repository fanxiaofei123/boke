                            <Label Key="Lab_No" Caption="单据编号" X="0" Y="0"/>
                            <DatePicker Key="BillDate" Caption="单据日期" BuddyKey="Lab_BillDate" X="3" Y="0" Enable="false" AsQuery="true">
                                <DataBinding ColumnKey="BillDate" TableKey="CHCS_VIP_Replace_Rule_Set_H"/>
