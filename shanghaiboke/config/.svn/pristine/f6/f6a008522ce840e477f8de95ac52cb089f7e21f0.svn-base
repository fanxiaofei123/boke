                                <GridCell Key="Common_Meal" Caption="常规餐种" CellType="Dict" ItemKey="CHCS_Product">
                                    <DataBinding ColumnKey="Common_Meal"/>
                                </GridCell>
                                <GridCell Key="VIP_Meal" Caption="vip餐种" CellType="Dict" ItemKey="CHCS_Product">
                                    <DataBinding ColumnKey="VIP_Meal"/>
                                </GridCell>
                            </GridRow>
