            <Column Key="OwnerNo" Caption="货主单号" DataType="Varchar" Length="100" DBColumnName="OwnerNo"/>
            <Statement>
                <![CDATA[select * from (select q.*,s.IsRecvArea,s.IsShipArea,s.IsSampleTestArea,s.IsKittingArea,m.BasicUnit,OnhandQty-ExpectOutQty as AddQty,OwnerNoTable.OwnerNo  from LRP_Quant q join LRP_Location l on q.LocationId=l.oid join LRP_Storearea s on q.StoreareaId=s.oid join LRP_Material m on q.MaterialId=m.oid left join (select No, OwnerNo from  LRP_InboundNotice_H union select No, OwnerNo from LRP_ReturnNotice_H union select  No,OwnerNo from LRP_OutboundNotice_H union select  No,OwnerNo from LRP_BackNotice_H union select No,OwnerNo from LRP_KittingNotice_H)OwnerNoTable on q.RefOrderNo=OwnerNoTable.No where q.OnhandQty>0 and l.IsLocked=0 )a]]>
