                                        <DataBinding ColumnKey="DictUserDef2"/>
                                    </GridCell>
                                    <GridCell Caption="字典型3" CellType="Dict" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" ItemKeySourceType="Para" ItemKeySource="DictUserItemKey.DictUser1ItemKey" Key="DictUserDef3">
                                        <DataBinding ColumnKey="DictUserDef3"/>
                                    </GridCell>
                                    <GridCell Caption="字典型4" CellType="Dict" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" ItemKeySourceType="Para" ItemKeySource="DictUserItemKey.DictUser1ItemKey" Key="DictUserDef4">
                                        <DataBinding ColumnKey="DictUserDef4"/>
                                    </GridCell>
                                    <GridCell Caption="字典型5" CellType="Dict" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" ItemKeySourceType="Para" ItemKeySource="DictUserItemKey.DictUser1ItemKey" Key="DictUserDef5">
                                        <DataBinding ColumnKey="DictUserDef5"/>
                                    </GridCell>
                                    <GridCell Caption="日期型1" CellType="DatePicker" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" OnlyDate="true" Format="yyyy-MM-dd" Key="DateUserDef1">
                                        <DataBinding ColumnKey="DateUserDef1"/>
                                    </GridCell>
                                    <GridCell Caption="日期型2" CellType="DatePicker" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" OnlyDate="true" Format="yyyy-MM-dd" Key="DateUserDef2">
                                        <DataBinding ColumnKey="DateUserDef2"/>
                                    </GridCell>
                                    <GridCell Caption="数值型1" CellType="NumberEditor" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" Key="NumUserDef1">
                                        <DataBinding ColumnKey="NumUserDef1"/>
                                    </GridCell>
                                    <GridCell Caption="数值型2" CellType="NumberEditor" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" Key="NumUserDef2">
                                        <DataBinding ColumnKey="NumUserDef2"/>
                                    </GridCell>
                                    <GridCell Caption="数值型3" CellType="NumberEditor" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" Key="NumUserDef3">
                                        <DataBinding ColumnKey="NumUserDef3"/>
                                    </GridCell>
                                    <GridCell Caption="数量" CellType="NumberEditor" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" Key="Qty">
                                        <DataBinding ColumnKey="Qty" CheckRule="Qty&gt;0" ErrorInfo="数量必须大于0"/>
                                    </GridCell>
                                    <GridCell Caption="单位" CellType="ComboBox" Enable="!ReadOnly() &amp;&amp; MaterialId&gt;0 &amp;&amp; LogisticsOrderNo==''" Key="MaterialUnit" SourceType="Query" DynamicItems="true" ItemsDependency="MaterialId">
                                        <DataBinding ColumnKey="MaterialUnit" Required="true"/>
                                        <QueryDef>
                                            <Statement>
                                                <![CDATA[select BasicUnit as k,BasicUnit as n from LRP_Material where OID=? union select PackingUnit as p,PackingUnit as u from LRP_Material where OID=?]]>
                                            </Statement>
                                            <ParameterCollection>
                                                <Parameter DataType="Long" FieldKey="MaterialId" SourceType="Field"/>
                                                <Parameter DataType="Long" FieldKey="MaterialId" SourceType="Field"/>
                                            </ParameterCollection>
                                        </QueryDef>
                                    </GridCell>
                                    <GridCell Caption="基本单位量" CellType="NumberEditor" Enable="false" Key="BasicQty">
                                        <DataBinding ColumnKey="BasicQty" DefaultFormulaValue="if(MaterialUnit !=''){if(MaterialUnit==GetDictValue('LRP_Material',MaterialId,'BasicUnit')){Qty;}else{Qty * GetDictValue('LRP_Material',MaterialId,'PackingQty');}}"/>
                                    </GridCell>
                                    <GridCell Caption="物料状态" CellType="Dict" ItemKey="LRP_MaterialStatus" Enable="!ReadOnly() &amp;&amp; LogisticsOrderNo==''" Key="MaterialStatusId">
                                        <DataBinding ColumnKey="MaterialStatusId" Required="true"/>
                                    </GridCell>
                                    <GridCell Caption="指定仓间" CellType="Dict" Enable="!ReadOnly()" ItemKey="LRP_Storeroom" Key="StoreroomId">
                                        <DataBinding ColumnKey="StoreroomId"/>
                                        <ItemFilter ItemKey="LRP_Storeroom">
                                            <Filter Type="FieldValue">
                                                <FilterValue FieldKey="WarehouseCenterId" RefValue="WarehouseCenterId" Type="Field"/>
                                            </Filter>
                                        </ItemFilter>
                                    </GridCell>
                                    <GridCell Caption="指定库区" CellType="Dict" Enable="!ReadOnly()&amp;&amp; StoreroomId&gt;0" ItemKey="LRP_Storearea" Key="StoreareaId">
                                        <DataBinding ColumnKey="StoreareaId"/>
                                        <ItemFilter ItemKey="LRP_Storearea">
                                            <Filter Type="FieldValue">
                                                <FilterValue FieldKey="StoreroomId" RefValue="StoreroomId" Type="Field"/>
                                                <FilterValue FieldKey="IsRecvArea" RefValue="1" Type="Const"/>
                                                <FilterValue FieldKey="IsShipArea" RefValue="0" Type="Const"/>
                                                <FilterValue FieldKey="IsSampleTestArea" RefValue="0" Type="Const"/>
                                                <FilterValue FieldKey="IsKittingArea" RefValue="0" Type="Const"/>
                                            </Filter>
                                        </ItemFilter>
                                    </GridCell>
                                    <GridCell Caption="指定储位" CellType="Dict" Enable="!ReadOnly()&amp;&amp; StoreareaId&gt;0" ItemKey="LRP_Location" Key="LocationId">
                                        <DataBinding ColumnKey="LocationId"/>
                                        <ItemFilter ItemKey="LRP_Location">
                                            <Filter Type="FieldValue">
                                                <FilterValue FieldKey="StoreareaId" RefValue="StoreareaId" Type="Field"/>
                                            </Filter>
                                        </ItemFilter>
                                    </GridCell>
                                    <GridCell Caption="收货量" CellType="NumberEditor" Enable="false" Key="RecvQty">
                                        <DataBinding ColumnKey="RecvQty"/>
                                    </GridCell>
                                    <GridCell Caption="发货量" CellType="NumberEditor" Enable="false" Key="ShipQty">
                                        <DataBinding ColumnKey="ShipQty"/>
                                    </GridCell>
                                </GridRow>
                            </GridRowCollection>
                        </Grid>
                    </TabPanel>
                    <SplitSize Size="175px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
