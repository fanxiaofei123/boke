<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Caption="货权转移单" FormType="Entity" Key="V_Tx403" ViewKey="V_Tx403View">
    <DataSource RefObjectKey="LRP_WMTx"/>
    <OperationCollection>
        <Operation Caption="保存" Key="Save" Visible="!ReadOnly()">
            <Action>
                <![CDATA[var flag=false;
if(GetRowCount('Grid1', false)<=0){ 
	Confirm(LocaleString("OwnerNull", "key103"), 'OK','OK:{}');
}
var rs = DBNamedQuery("SelectMaSta", TxTypeCode);
var index = 0;
loop 'Grid1' (true){
        index = index + 1;
        var mtlStatusId = GetCellValue('Grid1', -1, 'DestMaterialStatusId');
	rs.beforefirst();
	while(rs.next()){
		if(mtlStatusId > 0 && rs.MaterialStatusId == mtlStatusId){
			flag=true;
			break;
		}
                flag = false;
	}
	if(flag == false){
		  Confirm(LocaleParaFormat("CommonDef", "key50", index) + GetDictValue('LRP_MaterialStatus', mtlStatusId, 'Name'), 'OK', 'OK:{}');
                  break;	
	}   
}
if(flag==true){
	SaveData();
	UpdateView();
}]]>
            </Action>
        </Operation>
        <Operation Caption="引入库存" Enable="!ReadOnly()&amp;&amp;Status==100" Key="QuoteQuant">
            <Action>
                <![CDATA[if(OwnerId<=0){
	Confirm(LocaleString("OwnerNull", "key146"), 'OK',{OK:{}});
	return true;
}
ShowModal('V_QuoteQuantView',
	{WarehouseCenterId_para:{WarehouseCenterId},
	OwnerId_para:{OwnerId},
	pFormKey:{GetFormKey()},
	IsRecvArea_para:{0},
	IsShipArea_para:{0},
	IsSampleTestArea_para:{0},
	IsKittingArea_para:{0}
	});]]>
            </Action>
        </Operation>
        <Operation Caption="编辑" Key="Edit" RefKey="Edit"/>
        <Operation Caption="删除" Key="Delete" RefKey="Delete"/>
        <Operation Caption="取消" Key="Cancel" RefKey="Cancel">
            <Action>
                <![CDATA[Cancel();]]>
            </Action>
        </Operation>
        <Operation Caption="行复制新增" Key="CopyGridRow" Visible="!ReadOnly()  &amp;&amp; Status==100">
            <Action>
                <![CDATA[CopyGridRow("Grid1",-1,"Qty","0");]]>
            </Action>
        </Operation>
        <Operation Caption="完成" Key="Finish" Visible="ReadOnly() &amp;&amp; Status==100">
            <Action>
                <![CDATA[Confirm(LocaleString("CommonDef", "key3"),'YES_NO',{YES:{
    Status = 900;
    if(ToString(FinishDateTime) == ''){
        SetValue('FinishDateTime', ServerDate());
    }
    loop "Grid1" (true){			
        WMTx_Status = 90;
    }
    SaveData();
    UpdateView();
    },NO:{}});]]>
            </Action>
            <ExceptionHandler>
                <![CDATA[	
				loop"Grid1"(true)
				{			
					WMTx_Status = 10;
				}			
				Status=StatusValue('prepared');
			]]>
            </ExceptionHandler>
        </Operation>
        <Operation Caption="撤销完成" Key="Finish-R" Visible="ReadOnly() &amp;&amp; Status==900">
            <Action>
                <![CDATA[Status=100;
if(ToString(FinishDateTime) != ''){
      SetValue('FinishDateTime', '');
}
loop "Grid1" (true){			
    WMTx_Status = 10;
}
SaveData();
UpdateView();]]>
            </Action>
            <ExceptionHandler>
                <![CDATA[	
				loop"Grid1"(true)
				{			
					WMTx_Status = 90;
				}			
				Status=StatusValue('finished');
			]]>
            </ExceptionHandler>
        </Operation>
        <Operation Caption="关闭界面" Key="Close" RefKey="Close"/>
    </OperationCollection>
