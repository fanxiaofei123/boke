<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Caption="退厂订单" FormType="Entity" Key="V_BackNotice">
    <DataSource RefObjectKey="LRP_BackNotice"/>
    <OperationCollection>
        <Operation Caption="新增" Key="New" Visible="ReadOnly()">
            <Action>
                <![CDATA[
				New('V_BackNotice');
			]]>
            </Action>
        </Operation>
        <Operation Caption="复制新增" Key="CopyNew" RefKey="CopyNew" ShortCuts="Ctrl+E"/>
        <Operation Caption="保存" Key="Save" ShortCuts="Ctrl+S" Visible="!ReadOnly()&amp;&amp;Status==100">
            <Action>
                <![CDATA[
				if(OwnerId<=0){		
					Confirm(LocaleString('OwnerNull','keyo'),'OK','OK:{}');
					return true;
				}
					
				if(GetRowCount('Grid0',false)<=0){
					Confirm(LocaleString('CommonDef','key1'),'OK','OK:{}');
					return true;
				}
				
				SaveData();
				UpdateView();
			]]>
            </Action>
        </Operation>
        <Operation Caption="编辑" Key="Edit" Visible="ReadOnly()&amp;&amp;Status==StatusValue('prepared')&amp;&amp;LogisticsOrderNo==''">
            <Action>
                <![CDATA[Edit();]]>
            </Action>
        </Operation>
        <Operation Caption="删除" Key="Delete" Visible="ReadOnly()&amp;&amp;Status==StatusValue('prepared')&amp;&amp;LogisticsOrderNo==''">
            <Action>
                <![CDATA[
			Confirm(LocaleString('CommonDef','key2'),'YES_NO',{YES:{DeleteData();UpdateView();Close();},NO:{}});
			
			]]>
            </Action>
        </Operation>
        <Operation Caption="取消" Key="Cancel" RefKey="Cancel"/>
        <Operation Caption="确认" Key="Confirm" RefKey="Confirm"/>
        <Operation Caption="撤销确认" Key="Confirm-R" Visible="ReadOnly()&amp;&amp;Status==StatusValue('confirmed')">
            <Action>
                <![CDATA[InvokeService("LRP_PlatformReservationDelete", true, false,GetOID());
Status=StatusValue('prepared');SaveData();UpdateView();
]]>
            </Action>
            <ExceptionHandler>
                <![CDATA[Status=StatusValue('confirmed');]]>
            </ExceptionHandler>
        </Operation>
        <Operation Caption="完成" Key="Finish" RefKey="Finish"/>
        <Operation Caption="撤销完成" Key="Finish-R" RefKey="Finish-R"/>
        <Operation Caption="作废" Key="ToCancel" RefKey="ToCancel"/>
        <Operation Caption="撤销作废" Key="ToCancel-R" RefKey="ToCancel-R"/>
        <Operation Caption="拣货" Key="Pick" Visible="ReadOnly()&amp;&amp;(Status==StatusValue('confirmed')||Status==StatusValue('handled'))">
            <Action>
                <![CDATA[
					//GridSelectAll("Grid0");
					MidMap('BackNotice-Tx205','V_Tx205')
			]]>
            </Action>
        </Operation>
        <Operation Caption="快速发货" Key="Ship" Visible="ReadOnly()&amp;&amp;Status==StatusValue('confirmed')">
            <Action>
                <![CDATA[
					//GridSelectAll("Grid0");
					MidMap('BackNotice-Tx212','V_Tx212')
			]]>
            </Action>
        </Operation>
        <Operation Caption="引入库存" Enable="!ReadOnly()&amp;&amp;Status==100" Key="QuoteQuant">
            <Action>
                <![CDATA[                    
			            if(OwnerId>0){
                            //SetPara('OwnerId_para',OwnerId);
							//SetPara('WarehouseCenterId_para',getCurOrgID());
							//SetPara('pFormKey',GetFormKey());
							ShowModal('V_QuoteQuantView',{OwnerId_para:{OwnerId},WarehouseCenterId_para:{getCurOrgID()},pFormKey:{GetFormKey()},IsRecvArea_para:{0},IsShipArea_para:{0},IsSampleTestArea_para:{0},IsKittingArea_para:{0}});
						}else{
                            Confirm(LocaleString('OwnerNull','key146'), 'OK',{OK:{}});
                        }]]>
            </Action>
        </Operation>
        <Operation Key="Railway" Caption="月台预约" Enable="Status==200">
            <Action>
                <![CDATA[PlatformOrder(4,WarehouseCenterId,GetOID(),No,ExpectDateTimeFrom,ExpectDateTimeTo,CarrierId,0);





]]>
            </Action>
        </Operation>
        <Operation Caption="关闭界面" Key="Close" RefKey="Close"/>
    </OperationCollection>
