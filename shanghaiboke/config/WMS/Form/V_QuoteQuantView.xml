                    <GridLayoutPanel Caption="GridLayoutPanel1" Key="GridLayoutPanel1">
                        <Button Caption="确定" Key="OK" X="1" Y="0">
                            <OnClick>
                                <![CDATA[var count = GetRowCount('Grid0',false);
											var flag=0;
											var i=0;
											  
											//遍历表格,是否有被勾选的明细行,有:flag=1
											while ( i < count ) {
												if(ToInt(GetCellValue('Grid0',i,'Select'))==1){
												  flag=1;
												  break;
												}
												i=i+1;
											  }
											  
											//没有被勾选的明细行,弹出警告
											if(flag==0){
												Confirm(LocaleString("CommonDef", "key48"),'OK','OK:{}');
											  }
											  
											//有被勾选的明细行,上引下推明细行
											if(flag==1){											

												//字符串需要加Parent
												IIFS(
													 GetPara("pFormKey")=="V_BackNotice",ViewMap('Quant-BackNotice',false),
												         GetPara("pFormKey")=="V_OutboundNotice",ViewMap('Quant-OutboundNotice',false),
												         GetPara("pFormKey")=="V_Tx113",ViewMap('RecvQuant-Tx113',false),
													 GetPara("pFormKey")=="V_Tx114",ViewMap('KittingQuant-Tx114',false),
													 GetPara("pFormKey")=="V_Tx203",ViewMap('PickingQuant-Tx203',false),
													 GetPara("pFormKey")=="V_Tx213",ViewMap('ShippingQuant-Tx213',false),
													 GetPara("pFormKey")=="V_Tx301",ViewMap('Quant-Tx301',false),
													 GetPara("pFormKey")=="V_Tx302",ViewMap('Quant-Tx302',false),
													 GetPara("pFormKey")=="V_Tx303",ViewMap('Quant-Tx303',false),
													 GetPara("pFormKey")=="V_Tx304",ViewMap('Quant-Tx304',false),
													 GetPara("pFormKey")=="V_Tx305",ViewMap('Quant-Tx305',false),
													 GetPara("pFormKey")=="V_Tx306",ViewMap('Quant-Tx306',false),
													 GetPara("pFormKey")=="V_Tx307",ViewMap('Quant-Tx307',false),
													 GetPara("pFormKey")=="V_Tx308",ViewMap('Quant-Tx308',false),
													 GetPara("pFormKey")=="V_Tx309",ViewMap('ShippingQuant-Tx309',false),
													 GetPara("pFormKey")=="V_Tx401",ViewMap('Quant-Tx401',false),
													 GetPara("pFormKey")=="V_Tx403",ViewMap('Quant-Tx403',false),
													 GetPara("pFormKey")=="V_Tx404",ViewMap('KittingQuant-Tx404',false),
													 GetPara("pFormKey")=="V_Tx405",ViewMap('Quant-Tx405',false),
													 GetPara("pFormKey")=="V_TakeStock",ViewMap('Quant-TakeStock',false),
                                                                                                         GetPara("pFormKey")=="V_KittingNotice",ViewMap('Quant-KittingNotice',false)
												);
								
											Close();
			       }]]>
                            </OnClick>
