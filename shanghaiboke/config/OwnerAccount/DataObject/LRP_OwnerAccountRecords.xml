<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<DataObject Caption="货主账户流水记录" Key="LRP_OwnerAccountRecords" NoPrefix="OC" ZeroPrefix="9" PrimaryTableKey="LRP_OwnerAccountRecords" PrimaryType="Entity">
    <TableCollection>
        <Table Caption="货主账户流水记录" Key="LRP_OwnerAccountRecords">
            <Column Caption="对象标识" DataType="Long" Key="OID"/>
            <Column Caption="主对象标识" DataType="Long" Key="SOID"/>
            <Column Caption="父对象标识" DataType="Long" Key="POID"/>
            <Column Caption="对象版本" DataType="Integer" Key="VERID"/>
            <Column Caption="对象明细版本" DataType="Integer" Key="DVERID"/>
            <Column Caption="状态" DataType="Integer" Key="Status"/>
            <Column Caption="创建人" DataType="Long" Key="CREATOR"/>
            <Column Caption="创建时间" DataType="DateTime" Key="CREATETIME"/>
            <Column Caption="修改人" DataType="Long" Key="MODIFIER"/>
            <Column Caption="修改时间" DataType="DateTime" Key="MODIFYTIME"/>
            <Column Caption="流水号" DataType="Varchar" Key="No" Length="100"/>
            <Column Caption="货主id" DataType="Long" Key="OwnerId"/>
            <Column Caption="货主用户" DataType="Long" Key="OwnerUserId"/>
            <Column Caption="币种" DataType="Varchar" Key="Currency" Length="10" DefaultValue="CNY"/>
            <Column Caption="之前金额" DataType="Numeric" Key="BeforeMoney" Precision="18" Scale="6" DefaultValue="0"/>
            <Column Caption="之后金额" DataType="Numeric" Key="AfterMoney" Precision="18" Scale="6" DefaultValue="0"/>
            <Column Key="TxMoney" Caption="本次调整金额" DataType="Numeric" Precision="18" Scale="6"/>
            <Column Key="FreezeMoney" Caption="冻结金额" DataType="Numeric" Precision="18" Scale="6"/>
            <Column Key="TxType" Caption="收支类型" DataType="Integer"/>
            <Column Key="Action" Caption="操作" DataType="Varchar" Length="40"/>
            <Column Key="ExtraMessage" Caption="附加信息" DataType="Varchar" Length="100"/>
        </Table>
    </TableCollection>
    <PostSaveProcess>
        <Process Description="反填余额">
            <![CDATA[LRP_UpdateOwnerAccountRecords();]]>
        </Process>
    </PostSaveProcess>
    <OIDFilter/>
</DataObject>
