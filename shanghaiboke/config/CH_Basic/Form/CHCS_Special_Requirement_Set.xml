<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_Special_Requirement_Set" Caption="配餐特殊要求设置" FormType="Dict">
    <DataSource RefObjectKey="CHCS_Special_Requirement_Set"/>
    <ScriptCollection>
        <Script Key="Load" Caption="载入" Description="载入事件" Range="Action" Verb="Load">
            <![CDATA[LoadData()]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <SplitPanel Key="SplitPanel1" Orientation="Vertical" Height="100%" Caption="SplitPanel1">
                <TabPanel Caption="TabPanel1" Key="TabPanel1">
                    <GridLayoutPanel Key="BasicInfo" Padding="10px" Caption="基础信息">
                        <TextEditor Key="Name" Caption="名称" BuddyKey="L_Name" X="4" Y="0">
                            <DataBinding ColumnKey="Name" TableKey="CHCS_Special_Requirement_Set" Required="true"/>
                        </TextEditor>
                        <Label Key="L_Name" Caption="名称" X="3" Y="0"/>
                        <TextEditor Key="Code" Caption="代码" BuddyKey="Lab_Code" X="1" Y="0">
                            <DataBinding ColumnKey="Code" TableKey="CHCS_Special_Requirement_Set" Required="true"/>
                        </TextEditor>
                        <Label Key="Lab_Code" Caption="代码" X="0" Y="0"/>
                        <TextEditor Key="Meal_Requirement" Caption="餐谱要求" BuddyKey="Lab_Meal_Requirement" X="1" Y="1">
                            <DataBinding ColumnKey="Meal_Requirement" TableKey="CHCS_Special_Requirement_Set"/>
                        </TextEditor>
                        <Label Key="Lab_Meal_Requirement" Caption="餐谱要求" X="0" Y="1"/>
                        <TextEditor Key="Meals_Provide_Description" Caption="配餐说明" BuddyKey="Lab_Meals_Provide_Description" X="4" Y="1">
                            <DataBinding ColumnKey="Meals_Provide_Description" TableKey="CHCS_Special_Requirement_Set"/>
                        </TextEditor>
                        <Label Key="Lab_Meals_Provide_Description" Caption="配餐说明" X="3" Y="1"/>
                        <NumberEditor Key="Price" Caption="价格" BuddyKey="Lab_Price" X="1" Y="2">
                            <DataBinding ColumnKey="Price" TableKey="CHCS_Special_Requirement_Set"/>
                        </NumberEditor>
                        <Label Key="Lab_Price" Caption="价格" X="0" Y="2"/>
                        <TextEditor Key="Remark" Caption="备注" BuddyKey="Lab_Remark" X="1" Y="3" XSpan="4">
                            <DataBinding ColumnKey="Remark" TableKey="CHCS_Special_Requirement_Set"/>
                        </TextEditor>
                        <Label Key="Lab_Remark" Caption="备注" X="0" Y="3"/>
                        <RowDefCollection RowGap="6" RowHeight="30">
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                            <RowDef/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="20">
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="30px"/>
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                    <GridLayoutPanel Key="SysInfo" Padding="10px" Caption="系统信息">
                        <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                            <DataBinding ColumnKey="CREATOR" TableKey="CHCS_Special_Requirement_Set"/>
                        </Dict>
                        <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                        <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                            <DataBinding ColumnKey="CREATETIME" TableKey="CHCS_Special_Requirement_Set"/>
                        </DatePicker>
                        <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                        <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                            <DataBinding ColumnKey="MODIFIER" TableKey="CHCS_Special_Requirement_Set"/>
                        </Dict>
                        <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                        <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                            <DataBinding ColumnKey="MODIFYTIME" TableKey="CHCS_Special_Requirement_Set"/>
                        </DatePicker>
                        <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                        <RowDefCollection RowGap="5">
                            <RowDef Height="30px"/>
                            <RowDef Height="30px"/>
                        </RowDefCollection>
                        <ColumnDefCollection ColumnGap="5">
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                            <ColumnDef Width="70px"/>
                            <ColumnDef Width="50%"/>
                        </ColumnDefCollection>
                    </GridLayoutPanel>
                </TabPanel>
                <TabPanel Caption="TabPanel2" Key="TabPanel2">
                    <Grid Key="CHCS_Terminal_Element" Caption="航站要素">
                        <GridColumnCollection>
                            <GridColumn Key="column1" Caption="选择" Width="80px" Sortable="true"/>
                            <GridColumn Key="Airport" Caption="机场" Width="80px" Sortable="true"/>
                        </GridColumnCollection>
                        <GridRowCollection>
                            <GridRow Key="row1" RowHeight="35" TableKey="CHCS_Terminal_Element">
                                <GridCell Key="select1" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                <GridCell Key="Airport" Caption="机场" CellType="Dict" ItemKey="CHCS_Airport">
                                    <DataBinding ColumnKey="Airport"/>
                                </GridCell>
                            </GridRow>
                        </GridRowCollection>
                    </Grid>
                    <Grid Key="CHCS_Verdor" Caption="航食公司">
                        <GridColumnCollection>
                            <GridColumn Key="column1" Caption="选择" Width="80px" Sortable="true"/>
                            <GridColumn Key="Verdor" Caption="航食公司" Width="80px" Sortable="true"/>
                        </GridColumnCollection>
                        <GridRowCollection>
                            <GridRow Key="row1" RowHeight="35" TableKey="CHCS_Verdor">
                                <GridCell Key="select2" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                <GridCell Key="Verdor" Caption="航食公司" CellType="Dict" ItemKey="Vendor">
                                    <DataBinding ColumnKey="Verdor"/>
                                </GridCell>
                            </GridRow>
                        </GridRowCollection>
                    </Grid>
                </TabPanel>
                <SplitSize Size="100%"/>
                <SplitSize Size="100px"/>
            </SplitPanel>
        </Block>
    </Body>
</Form>
