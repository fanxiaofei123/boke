<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="CHCS_MealCoupon_Provide" Caption="餐券配备单" FormType="Entity" ViewKey="CHCS_MealCoupon_ProvideView">
    <DataSource RefObjectKey="CHCS_MealCoupon_Provide"/>
    <OperationCollection>
        <Operation Key="WORKITEM" Caption="WORKITEM" Tag="WORKITEM"/>
        <Operation Key="BPM" Caption="BPM" Tag="BPM"/>
