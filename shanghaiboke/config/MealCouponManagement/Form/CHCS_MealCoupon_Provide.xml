    <ScriptCollection>
        <Script Key="Load" Caption="载入" Range="Action" Verb="Load">
            <![CDATA[LoadData();]]>
        </Script>
    </ScriptCollection>
    <Body>
        <Block>
            <FlexFlowLayoutPanel Key="root" Caption="根面板">
                <ToolBar Key="ToolBar1" Height="pref" Caption="ToolBar1">
                    <ToolBarItemCollection/>
                </ToolBar>
                <SplitPanel Key="main" Orientation="Vertical" Height="100%" Caption="主面板">
                    <TabPanel Caption="TabPanel1" Height="pref" Key="TabPanel1">
                        <GridLayoutPanel Key="first_head" Height="pref" Padding="5px" Caption="基本信息">
                            <TextEditor Key="NO" Caption="单据编号" BuddyKey="Lab_NO" X="1" Y="0" Enable="false" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="NO" TableKey="CHCS_MealCoupon_Provide_H"/>
                            </TextEditor>
                            <Label Key="Lab_NO" Caption="单据编号" X="0" Y="0"/>
                            <DatePicker Key="BillDate" Caption="飞行日期" BuddyKey="Lab_BillDate" X="5" Y="0" OnlyDate="true" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="BillDate" TableKey="CHCS_MealCoupon_Provide_H"/>
                                <Format/>
                            </DatePicker>
                            <Label Key="Lab_BillDate" Caption="飞行日期" X="4" Y="0"/>
                            <ComboBox Key="Status" Caption="状态" BuddyKey="Lab_Status" X="9" Y="0" Enable="false" SourceType="Status" XSpan="2" AsQuery="true">
                                <DataBinding ColumnKey="Status" TableKey="CHCS_MealCoupon_Provide_H"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="状态" X="8" Y="0"/>
                            <TextEditor Key="Remark" Caption="备注" BuddyKey="Lab_Remark" X="1" Y="1" XSpan="10">
                                <DataBinding ColumnKey="Remark" TableKey="CHCS_MealCoupon_Provide_H"/>
                            </TextEditor>
                            <Label Key="Lab_Remark" Caption="备注" X="0" Y="1"/>
                            <RowDefCollection RowGap="5" RowHeight="30">
                                <RowDef/>
                                <RowDef/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="33%" MinWidth="50px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                        <GridLayoutPanel Key="SystemInfo" Caption="系统信息">
                            <DatePicker Key="CREATETIME" Caption="创建时间" BuddyKey="Lab_CREATETIME" X="3" Y="0" Enable="false">
                                <DataBinding ColumnKey="CREATETIME" TableKey="CHCS_MealCoupon_Provide_H"/>
                            </DatePicker>
                            <Label Key="Lab_CREATETIME" Caption="创建时间" X="2" Y="0"/>
                            <DatePicker Key="MODIFYTIME" Caption="修改时间" BuddyKey="Lab_MODIFYTIME" X="3" Y="1" Enable="false">
                                <DataBinding ColumnKey="MODIFYTIME" TableKey="CHCS_MealCoupon_Provide_H"/>
                            </DatePicker>
                            <Label Key="Lab_MODIFYTIME" Caption="修改时间" X="2" Y="1"/>
                            <Dict Key="CREATOR" Caption="创建人" BuddyKey="Lab_CREATOR" X="1" Y="0" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="CREATOR" TableKey="CHCS_MealCoupon_Provide_H"/>
                            </Dict>
                            <Label Key="Lab_CREATOR" Caption="创建人" X="0" Y="0"/>
                            <Dict Key="MODIFIER" Caption="修改人" BuddyKey="Lab_MODIFIER" X="1" Y="1" Enable="false" ItemKey="Operator">
                                <DataBinding ColumnKey="MODIFIER" TableKey="CHCS_MealCoupon_Provide_H"/>
                            </Dict>
                            <Label Key="Lab_MODIFIER" Caption="修改人" X="0" Y="1"/>
                            <RowDefCollection RowGap="5">
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection ColumnGap="5">
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                                <ColumnDef Width="100px"/>
                                <ColumnDef Width="50%"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TabPanel>
                    <Grid Key="Grid1" Caption="Grid1">
                        <GridColumnCollection>
                            <GridColumn Key="column1" Caption="选择" Width="80px"/>
                            <GridColumn Key="Name" Caption="姓名" Width="100px"/>
                            <GridColumn Key="Department" Caption="部门" Width="130px"/>
                            <GridColumn Key="Flight_Time" Caption="飞行时长" Width="80px"/>
                            <GridColumn Key="Planned_Quantity" Caption="计划餐券" Width="80px"/>
                            <GridColumn Key="Actual_Quantity" Caption="实配餐数" Width="80px"/>
                            <GridColumn Key="Control_Quantity" Caption="调整数量" Width="80px"/>
                            <GridColumn Key="Deserve_Quantity" Caption="应得餐券" Width="80px"/>
                            <GridColumn Key="Deserve_Man" Caption="调整人" Width="100px"/>
                            <GridColumn Key="Deserve_Time" Caption="调整时间" Width="130px"/>
                            <GridColumn Key="Remark_l" Caption="备注" Width="80px"/>
                            <GridColumn Key="Flight_No" Caption="航班" Width="80px"/>
                            <GridColumn Key="Aircraft_Type_Number" Caption="机型" Width="80px"/>
                            <GridColumn Key="Route" Caption="航线" Width="100px"/>
                            <GridColumn Key="Vendor_Type" Caption="供应商类型" Width="100px"/>
                        </GridColumnCollection>
                        <GridRowCollection>
                            <GridRow Key="row1" RowHeight="36" TableKey="CHCS_MealCoupon_Provide_L">
                                <GridCell Key="select" Caption="选择" CellType="CheckBox" IsSelect="true"/>
                                <GridCell Key="Name" Caption="姓名" CellType="Dict" ItemKey="CHCS_Employee">
                                    <DataBinding ColumnKey="Name"/>
                                </GridCell>
                                <GridCell Key="Department" Caption="部门" CellType="Dict" Enable="false" ItemKey="CHCS_Department">
                                    <DataBinding ColumnKey="Department" DefaultFormulaValue="GetDictOID(&quot;CHCS_Department&quot;, Name, &quot;Department&quot;)"/>
                                </GridCell>
                                <GridCell Key="Flight_Time" Caption="飞行时长" CellType="NumberEditor" Format="" IntegerValue="true">
                                    <DataBinding ColumnKey="Flight_Time"/>
                                </GridCell>
                                <GridCell Key="Planned_Quantity" Caption="计划餐券" CellType="NumberEditor" Format="" IntegerValue="true">
                                    <DataBinding ColumnKey="Planned_Quantity"/>
                                </GridCell>
                                <GridCell Key="Actual_Quantity" Caption="实配餐数" CellType="NumberEditor" Format="" IntegerValue="true">
                                    <DataBinding ColumnKey="Actual_Quantity"/>
                                </GridCell>
                                <GridCell Key="Control_Quantity" Caption="调整数量" CellType="NumberEditor" Format="" IntegerValue="true">
                                    <DataBinding ColumnKey="Control_Quantity"/>
                                </GridCell>
                                <GridCell Key="Deserve_Quantity" Caption="应得餐券" CellType="NumberEditor" Enable="false" Format="" IntegerValue="true">
                                    <DataBinding ColumnKey="Deserve_Quantity" DefaultFormulaValue="Planned_Quantity-Actual_Quantity+Control_Quantity"/>
                                </GridCell>
                                <GridCell Key="Control_Man" Caption="调整人" CellType="Dict" ItemKey="CHCS_Employee">
                                    <DataBinding ColumnKey="Control_Man"/>
                                </GridCell>
                                <GridCell Key="Control_Time" Caption="调整时间" CellType="DatePicker">
                                    <DataBinding ColumnKey="Control_Time"/>
                                </GridCell>
                                <GridCell Key="Remark_l" Caption="备注" CellType="TextEditor">
                                    <DataBinding ColumnKey="Remark"/>
                                </GridCell>
                                <GridCell Key="Flight_No" Caption="航班" CellType="Dict" ItemKey="CHCS_Flight">
                                    <DataBinding ColumnKey="Flight_No"/>
                                </GridCell>
                                <GridCell Key="Aircraft_Type_Number" Caption="机型" CellType="Dict" ItemKey="CHCS_Aircraft_Type">
                                    <DataBinding ColumnKey="Aircraft_Type_Number"/>
                                </GridCell>
                                <GridCell Key="Route" Caption="航线" CellType="Dict" ItemKey="CHCS_Route">
                                    <DataBinding ColumnKey="Route"/>
                                </GridCell>
                                <GridCell Key="Vendor_Type" Caption="供应商类型" CellType="ComboBox">
                                    <DataBinding ColumnKey="Vendor_Type"/>
                                    <Item Caption="配餐" Key="0" Value="0"/>
                                    <Item Caption="配机供品" Key="1" Value="1"/>
                                </GridCell>
                            </GridRow>
                        </GridRowCollection>
                    </Grid>
                    <SplitSize Size="126px"/>
                    <SplitSize Size="100%"/>
                </SplitPanel>
            </FlexFlowLayoutPanel>
        </Block>
    </Body>
</Form>
