<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Form Key="LogisticsAppointment" Authenticate="false">
    <DataSource>
        <DataObject Key="LogisticsAppointment" PrimaryTableKey="LRP_LogisticsAppointment_H">
            <TableCollection>
                <Table Caption="业务预约头" Key="LRP_LogisticsAppointment_H" SourceType="Query">
                    <Column Caption="对象标识" DataType="Long" Key="OID"/>
                    <Column Caption="主对象标识" DataType="Long" Key="SOID"/>
                    <Column Caption="父对象标识" DataType="Long" Key="POID"/>
                    <Column Caption="对象版本" DataType="Integer" Key="VERID"/>
                    <Column Caption="对象明细版本" DataType="Integer" Key="DVERID"/>
                    <Column Caption="状态" DataType="Integer" Key="Status"/>
                    <Column Caption="创建人" DataType="Long" Key="CREATOR"/>
                    <Column Caption="创建时间" DataType="DateTime" Key="CREATETIME"/>
                    <Column Caption="修改人" DataType="Long" Key="MODIFIER"/>
                    <Column Caption="修改时间" DataType="DateTime" Key="MODIFYTIME"/>
                    <Column Caption="货主用户" DataType="Long" Key="OwnerUserId"/>
                    <Column Caption="单号" DataType="Varchar" Key="No" Length="100"/>
                    <Column Caption="联系人" Key="Contactor" DataType="Varchar" Length="100"/>
                    <Column Caption="联系人电话" Key="ContactTel" DataType="Varchar" Length="100"/>
                    <Column Caption="需求描述" Key="Requirement" DataType="Varchar" Length="200"/>
                    <Column Caption="处理情况" Key="Treatment" DataType="Varchar" Length="200"/>
                    <TableFilter>
                        <![CDATA[oid = ?]]>
                    </TableFilter>
                    <ParameterCollection>
                        <Parameter Formula="Para('LogisticsAppointmentOID');" DataType="Long"/>
                    </ParameterCollection>
                    <Statement>
                        <![CDATA[select 
    OID,
    NO,
	Status,
    CREATETIME,
    MODIFYTIME,
    OwnerUserId,
    Contactor,
    ContactTel,
    Requirement,
    Treatment
from LRP_LogisticsAppointment_H]]>
                    </Statement>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
    <Body>
        <Block>
            <TableView Key="info_table" X="0" Y="0">
                <TableRowCollection>
                    <TableRow Key="r1" TopMargin="5px">
                        <GridLayoutPanel Key="detail_panel1" Height="pref" LeftMargin="5px">
                            <Label Key="Lab_No" Caption="预约号" X="3" Y="3"/>
                            <Label Key="No" X="4" Y="3" XSpan="4">
                                <DataBinding ColumnKey="No" TableKey="LRP_LogisticsAppointment_H"/>
                            </Label>
                            <Label Key="OrderMessage" Caption="基本信息" X="3" Y="2" XSpan="5">
                                <Format ForeColor="#cc3333"/>
                            </Label>
                            <DatePicker Key="OrderDataTime" X="4" Y="4" Enable="false" XSpan="4" Format="MM月dd日 HH:mm">
                                <DataBinding ColumnKey="CREATETIME" TableKey="LRP_LogisticsAppointment_H"/>
                                <Format HAlign="Left" BackColor="#ffffff"/>
                            </DatePicker>
                            <Label Key="Lab_OrderDataTime" Caption="预约时间" X="3" Y="4"/>
                            <ComboBox Key="Status" Caption="预约状态" BuddyKey="Lab_Status" OnlyShow="true" X="4" Y="5" SourceType="Status" Enable="false" XSpan="4">
                                <DataBinding ColumnKey="Status" TableKey="LRP_LogisticsAppointment_H"/>
                                <Format HAlign="Left"/>
                            </ComboBox>
                            <Label Key="Lab_Status" Caption="单据状态" X="3" Y="5"/>
                            <Image Key="Image1" X="1" Y="2" SourceType="Resource" Image="3-1.png" ImageScaleType="Fit_XY" Stretch="true" Padding="5px"/>
                            <Label Key="Label1" X="0" Y="0" XSpan="8">
                                <Format BackColor="#e6e6e6"/>
                            </Label>
                            <RowDefCollection>
                                <RowDef Height="10px"/>
                                <RowDef Height="10px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="70px"/>
                                <ColumnDef Width="90px"/>
                                <ColumnDef Width="100%"/>
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="5px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TableRow>
                    <TableRow Key="row6">
                        <GridLayoutPanel Key="detail_panel16" Height="pref" LeftMargin="5px">
                            <Label Key="Contactor" X="4" Y="4" Enable="false" XSpan="4">
                                <DataBinding ColumnKey="Contactor" TableKey="LRP_LogisticsAppointment_H"/>
                            </Label>
                            <Label Key="Lab_Contactor" Caption="联系人" X="3" Y="4"/>
                            <Label Key="Blank2" X="0" Y="0" XSpan="8">
                                <Format BackColor="#e6e6e6">
                                    <Font Size="15" Bold="true"/>
                                </Format>
                            </Label>
                            <Label Key="ContactorMessage" Caption="联系人信息" X="3" Y="2" XSpan="5">
                                <Format ForeColor="#cc3333"/>
                            </Label>
                            <Label Key="ContactTel" BuddyKey="Lab_ContactTel" X="4" Y="5" XSpan="4">
                                <DataBinding ColumnKey="ContactTel" TableKey="LRP_LogisticsAppointment_H"/>
                            </Label>
                            <Label Key="Lab_ContactTel" Caption="电话" X="3" Y="5"/>
                            <Dict Key="OwnerUserId" X="4" Y="3" Visible="false" ItemKey="LRP_OwnerUser">
                                <DataBinding ColumnKey="OwnerUserId" TableKey="LRP_LogisticsAppointment_H"/>
                            </Dict>
                            <Label Key="Lab_Dict1" Caption="货主用户" X="3" Y="3"/>
                            <Label Key="OwnerUser" X="4" Y="3" XSpan="3">
                                <DataBinding DefaultFormulaValue="GetDictValue('LRP_OwnerUser',OwnerUserId, 'LRP_OwnerUser.Name');"/>
                            </Label>
                            <Image Key="Image2" X="1" Y="2" Image="3-1.png" ImageScaleType="Fit_XY" SourceType="Resource" Stretch="true" Padding="5px"/>
                            <RowDefCollection>
                                <RowDef Height="10px"/>
                                <RowDef Height="10px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                            </RowDefCollection>
                            <ColumnDefCollection>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="70px"/>
                                <ColumnDef Width="90px"/>
                                <ColumnDef Width="100%"/>
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="5px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TableRow>
                    <TableRow Key="r2" TopMargin="10px">
                        <GridLayoutPanel Key="detail_panel2" Height="pref" LeftMargin="5px">
                            <Label Key="Blank3" X="0" Y="0" XSpan="8">
                                <Format BackColor="#e6e6e6">
                                    <Font Size="15" Bold="true"/>
                                </Format>
                            </Label>
                            <Label Key="RequireMessage" Caption="业务描述" X="3" Y="2" XSpan="5">
                                <Format ForeColor="#cc3333"/>
                            </Label>
                            <TextEditor Key="Treatment" BuddyKey="Lab_Treatment" X="4" Y="6" Enable="false" XSpan="3" YSpan="2" BorderColor="#cccccc" HasBorder="true">
                                <DataBinding ColumnKey="Treatment" TableKey="LRP_LogisticsAppointment_H"/>
                                <Format HAlign="Left" VAlign="Top">
                                    <Font Size="13"/>
                                </Format>
                            </TextEditor>
                            <Label Key="Lab_Treatment" Caption="处理情况" X="3" Y="6"/>
                            <TextEditor Key="Requirement" BuddyKey="Lab_Requirement" X="4" Y="3" Enable="false" XSpan="3" YSpan="2" BorderColor="#cccccc" HasBorder="true">
                                <DataBinding ColumnKey="Requirement" TableKey="LRP_LogisticsAppointment_H"/>
                                <Format HAlign="Left" VAlign="Top">
                                    <Font Size="13"/>
                                </Format>
                            </TextEditor>
                            <Label Key="Lab_Requirement" Caption="需求描述" X="3" Y="3"/>
                            <Image Key="Image3" X="1" Y="2" Image="3-1.png" ImageScaleType="Fit_XY" SourceType="Resource" Stretch="true" Padding="5px"/>
                            <RowDefCollection>
                                <RowDef Height="10px"/>
                                <RowDef Height="10px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="50px"/>
                                <RowDef Height="10px"/>
                                <RowDef Height="30px"/>
                                <RowDef Height="50px"/>
                            </RowDefCollection>
                            <ColumnDefCollection>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="30px"/>
                                <ColumnDef Width="10px"/>
                                <ColumnDef Width="70px"/>
                                <ColumnDef Width="90px"/>
                                <ColumnDef Width="100%"/>
                                <ColumnDef Width="120px"/>
                                <ColumnDef Width="10px"/>
                            </ColumnDefCollection>
                        </GridLayoutPanel>
                    </TableRow>
                </TableRowCollection>
            </TableView>
        </Block>
    </Body>
    <OnLoad>
        <![CDATA[LoadData()]]>
    </OnLoad>
    <NavigationBar BackColor="#f14c52" Style="Custom">
        <BarItem Key="Centertitle" Caption="预约订单信息" Location="Center">
            <Label Key="TitleCenter" Caption="预约订单信息" Width="pref">
                <Format ForeColor="#ffffff" HAlign="Center">
                    <Font Size="20"/>
                </Format>
            </Label>
        </BarItem>
        <LeftButton Icon="back_icon.png">
            <![CDATA[BackTo("MRoot");SendSignal('SessionChanged');
		]]>
        </LeftButton>
    </NavigationBar>
</Form>
